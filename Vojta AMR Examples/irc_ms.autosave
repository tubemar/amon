      <Object Name="Process1" Class="Amit.Ngs.Core.NgsProcessPeriod" Def="Process">
        <Id>0</Id>
        <Body Class="Amit.Ngs.Core.NgsCodeBodyText"><![CDATA[	//Globální režim objektu nastavitelný na x dní
	if DateTime.Day != BackupRam.DayOld then
		BackupRam.DayOld = DateTime.Day;
		BackupRam.DayCnt = BackupRam.DayCnt-1;
		BackupRam.GenMode = BackupRam.DayCnt < 1 ? 0 : BackupRam.GenMode;
		BackupRam.DayCnt = BackupRam.GenMode == 0 ? 1 : BackupRam.DayCnt;
	endif;

	//************************
	// Místnost 1 - šatna
	//************************

	//Čtení hodnot ovladače
	Ram.AM_ComStat[0,0].0 = not Regmet_AMx1.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,0].0 then
		Ram.Tm_M[0,0] = Regmet_AMx1.TempMeasured;
		Ram.Tm_Corr[0,0] = BackUpRam.RoomCorrMeze[0,0]+((BackUpRam.RoomCorrMeze[0,1]-BackUpRam.RoomCorrMeze[0,0])*Regmet_AMx1.Correction)/100.0;
		//Zatím nelze ovládat vzdáleně režim (LED)
		//BackupRam.iTz_Mode[0,0] = Regmet_AMx1.Mode;
	else	
		//Při chybě komunikace vynuluvat měřenou hodnotu i korekci
		Ram.Tm_M[0,0] = 0.0;
		Ram.Tm_Corr[0,0] = 0.0;
	endif;
	
	//Test na globální režim a nastavelní lokálního režimu místnosti
	Ram.xTz_Mode[0,0] = BackupRam.GenMode == 0 ? BackupRam.iTz_Mode[0,0] : BackupRam.GenMode;
	if Ram.xTz_Mode[0,0]==0 then
		//Režim časový plán
		Ram.fx = DayPlan1.Output();
	else
		if Ram.xTz_Mode[0,0]==1 then
			//Režim útlumová teplota
			Ram.fx = BackupRam.Tz_Par[0,0];
		else
			//Režim komfortní teplota
			Ram.fx = BackupRam.Tz_Par[0,1];
		endif;
	endif;

	//Omezení mezí žádané prostorové teploty
	Limiter1.Exec(Ram.fx);
	Ram.Tz_M[0,0] = Limiter1.Out;
	

	//Vyhodnocení požadavku topit
	Hyst1.Limit = Ram.Tz_M[0,0];
	Hyst1.Hysteresis = EEprom.RoomHyst[0,0];
	Ram.TopBity_DO.0 = Hyst1.Output(Ram.Tm_M[0,0]);

	//************************
	// Místnost 2 - hala
	//************************

	//Čtení hodnot ovladače č.1, teplota + korekce
	Ram.AM_ComStat[0,1].1 = not Regmet_AMx2.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,1].1 then
		Ram.Tm_M[1,1] = Regmet_AMx2.TempMeasured;
		Ram.Tm_Corr[1,0] = BackUpRam.RoomCorrMeze[1,0]+((BackUpRam.RoomCorrMeze[1,1]-BackUpRam.RoomCorrMeze[1,0])*Regmet_AMx2.Correction)/100.0;
		//BackupRam.iTz_Mode[0,0] = Regmet_AMx2.Mode;
	else
		Ram.Tm_M[1,1] = 0.0;
		Ram.Tm_Corr[1,0] = 0.0;
	endif;
	
	//Čtení hodnot ovladače č.2, pouze teplota
	Ram.AM_ComStat[0,2].1 = not Regmet_AMx3.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,2].1 then
		Ram.Tm_M[1,2] = Regmet_AMx3.TempMeasured;
	else
		Ram.Tm_M[1,2] = 0.0;
	endif;
	
	//Stav komunikace ovladače pro místnost
	if (Ram.AM_ComStat[0,1].1) or (Ram.AM_ComStat[0,2].1) then
		//Některý nebo oba ovladače komunikují
		Ram.AM_ComStat[0,0].1 = true;
		if (Ram.AM_ComStat[0,1].1) and (Ram.AM_ComStat[0,2].1) then
			//Komunikují oba, průměr teplot
			Ram.Tm_M[1,0] = (Ram.Tm_M[1,1]+Ram.Tm_M[1,2])/2.0;
		else
			if Ram.AM_ComStat[0,1].1 then
				//Komunikuje jen první
				Ram.Tm_M[1,0] = Ram.Tm_M[1,1];
			else
				//Jinak komunikuje jen druhý
				Ram.Tm_M[1,0] = Ram.Tm_M[1,2];
			endif;
		endif;
	else
		//Nekomunikuje ani jeden
		Ram.AM_ComStat[0,0].1 = false;
		Ram.Tm_M[1,0] = 0.0;
	endif;

	//************************
	// Místnost 3 - jídelna
	//************************

	//Čtení hodnot ovladače
	Ram.AM_ComStat[0,0].2 = not Regmet_AMx4.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,0].2 then
		Ram.Tm_M[2,0] = Regmet_AMx4.TempMeasured;
		Ram.Tm_Corr[2,0] = BackUpRam.RoomCorrMeze[2,0]+((BackUpRam.RoomCorrMeze[2,1]-BackUpRam.RoomCorrMeze[2,0])*Regmet_AMx4.Correction)/100.0;
	else	
		//Při chybě komunikace vynuluvat měřenou hodnotu i korekci
		Ram.Tm_M[2,0] = 0.0;
		Ram.Tm_Corr[2,0] = 0.0;
	endif;

	//************************
	// Místnost 4 - denní místnost 1
	//************************

	//Čtení hodnot ovladače č.1, teplota + korekce
	Ram.AM_ComStat[0,1].3 = not Regmet_AMx5.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,1].3 then
		Ram.Tm_M[3,1] = Regmet_AMx5.TempMeasured;
		Ram.Tm_Corr[3,0] = BackUpRam.RoomCorrMeze[3,0]+((BackUpRam.RoomCorrMeze[3,1]-BackUpRam.RoomCorrMeze[3,0])*Regmet_AMx5.Correction)/100.0;
	else
		Ram.Tm_M[3,1] = 0.0;
		Ram.Tm_Corr[3,0] = 0.0;
	endif;
	
	//Čtení hodnot ovladače č.2, pouze teplota
	Ram.AM_ComStat[0,2].3 = not Regmet_AMx6.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,2].3 then
		Ram.Tm_M[3,2] = Regmet_AMx6.TempMeasured;
	else
		Ram.Tm_M[3,2] = 0.0;
	endif;
	
	//Stav komunikace ovladače pro místnost
	if (Ram.AM_ComStat[0,1].3) or (Ram.AM_ComStat[0,2].3) then
		//Některý nebo oba ovladače komunikují
		Ram.AM_ComStat[0,0].3 = true;
		if (Ram.AM_ComStat[0,1].3) and (Ram.AM_ComStat[0,2].3) then
			//Komunikují oba, průměr teplot
			Ram.Tm_M[3,0] = (Ram.Tm_M[3,1]+Ram.Tm_M[3,2])/2.0;
		else
			if Ram.AM_ComStat[0,1].3 then
				//Komunikuje jen první
				Ram.Tm_M[3,0] = Ram.Tm_M[3,1];
			else
				//Jinak komunikuje jen druhý
				Ram.Tm_M[3,0] = Ram.Tm_M[3,2];
			endif;
		endif;
	else
		//Nekomunikuje ani jeden
		Ram.AM_ComStat[0,0].3 = false;
		Ram.Tm_M[3,0] = 0.0;
	endif;

	//************************
	// Místnost 5 - umývárna 1
	//************************

	//Čtení hodnot ovladače
	Ram.AM_ComStat[0,0].4 = not Regmet_AMx7.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,0].4 then
		Ram.Tm_M[4,0] = Regmet_AMx7.TempMeasured;
		Ram.Tm_Corr[4,0] = BackUpRam.RoomCorrMeze[4,0]+((BackUpRam.RoomCorrMeze[4,1]-BackUpRam.RoomCorrMeze[4,0])*Regmet_AMx7.Correction)/100.0;
	else	
		//Při chybě komunikace vynuluvat měřenou hodnotu i korekci
		Ram.Tm_M[4,0] = 0.0;
		Ram.Tm_Corr[4,0] = 0.0;
	endif;

	//************************
	// Místnost 6 - denní místnost 2
	//************************

	//Čtení hodnot ovladače č.1, teplota + korekce
	Ram.AM_ComStat[0,1].5 = not Regmet_AMx8.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,1].5 then
		Ram.Tm_M[5,1] = Regmet_AMx8.TempMeasured;
		Ram.Tm_Corr[5,0] = BackUpRam.RoomCorrMeze[5,0]+((BackUpRam.RoomCorrMeze[5,1]-BackUpRam.RoomCorrMeze[5,0])*Regmet_AMx8.Correction)/100.0;
	else
		Ram.Tm_M[5,1] = 0.0;
		Ram.Tm_Corr[5,0] = 0.0;
	endif;
	
	//Čtení hodnot ovladače č.2, pouze teplota
	Ram.AM_ComStat[0,2].5 = not Regmet_AMx9.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,2].5 then
		Ram.Tm_M[5,2] = Regmet_AMx9.TempMeasured;
	else
		Ram.Tm_M[5,2] = 0.0;
	endif;
	
	//Stav komunikace ovladače pro místnost
	if (Ram.AM_ComStat[0,1].5) or (Ram.AM_ComStat[0,2].5) then
		//Některý nebo oba ovladače komunikují
		Ram.AM_ComStat[0,0].5 = true;
		if (Ram.AM_ComStat[0,1].5) and (Ram.AM_ComStat[0,2].5) then
			//Komunikují oba, průměr teplot
			Ram.Tm_M[5,0] = (Ram.Tm_M[5,1]+Ram.Tm_M[5,2])/2.0;
		else
			if Ram.AM_ComStat[0,1].5 then
				//Komunikuje jen první
				Ram.Tm_M[5,0] = Ram.Tm_M[5,1];
			else
				//Jinak komunikuje jen druhý
				Ram.Tm_M[5,0] = Ram.Tm_M[5,2];
			endif;
		endif;
	else
		//Nekomunikuje ani jeden
		Ram.AM_ComStat[0,0].5 = false;
		Ram.Tm_M[5,0] = 0.0;
	endif;

	//************************
	// Místnost 7 - denní místnost 3
	//************************

	//Čtení hodnot ovladače č.1, teplota + korekce
	Ram.AM_ComStat[0,1].6 = not Regmet_AMx10.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,1].6 then
		Ram.Tm_M[6,1] = Regmet_AMx10.TempMeasured;
		Ram.Tm_Corr[6,0] = BackUpRam.RoomCorrMeze[6,0]+((BackUpRam.RoomCorrMeze[6,1]-BackUpRam.RoomCorrMeze[6,0])*Regmet_AMx10.Correction)/100.0;
	else
		Ram.Tm_M[6,1] = 0.0;
		Ram.Tm_Corr[6,0] = 0.0;
	endif;
	
	//Čtení hodnot ovladače č.2, pouze teplota
	Ram.AM_ComStat[0,2].6 = not Regmet_AMx11.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,2].6 then
		Ram.Tm_M[6,2] = Regmet_AMx11.TempMeasured;
	else
		Ram.Tm_M[6,2] = 0.0;
	endif;
	
	//Stav komunikace ovladače pro místnost
	if (Ram.AM_ComStat[0,1].6) or (Ram.AM_ComStat[0,2].6) then
		//Některý nebo oba ovladače komunikují
		Ram.AM_ComStat[0,0].6 = true;
		if (Ram.AM_ComStat[0,1].6) and (Ram.AM_ComStat[0,2].6) then
			//Komunikují oba, průměr teplot
			Ram.Tm_M[6,0] = (Ram.Tm_M[6,1]+Ram.Tm_M[6,2])/2.0;
		else
			if Ram.AM_ComStat[0,1].6 then
				//Komunikuje jen první
				Ram.Tm_M[6,0] = Ram.Tm_M[6,1];
			else
				//Jinak komunikuje jen druhý
				Ram.Tm_M[6,0] = Ram.Tm_M[6,2];
			endif;
		endif;
	else
		//Nekomunikuje ani jeden
		Ram.AM_ComStat[0,0].6 = false;
		Ram.Tm_M[6,0] = 0.0;
	endif;

	//************************
	// Místnost 8 - umývárna 2
	//************************

	//Čtení hodnot ovladače
	Ram.AM_ComStat[0,0].7 = not Regmet_AMx12.Disconnected;
	//Stav komunikace
	if Ram.AM_ComStat[0,0].7 then
		Ram.Tm_M[7,0] = Regmet_AMx12.TempMeasured;
		Ram.Tm_Corr[7,0] = BackUpRam.RoomCorrMeze[7,0]+((BackUpRam.RoomCorrMeze[7,1]-BackUpRam.RoomCorrMeze[7,0])*Regmet_AMx12.Correction)/100.0;
	else	
		//Při chybě komunikace vynuluvat měřenou hodnotu i korekci
		Ram.Tm_M[7,0] = 0.0;
		Ram.Tm_Corr[7,0] = 0.0;
	endif;

	//Požadavky do hlavic
	DI2RDO2_1.RDO.0 = Ram.TopBity_DO.0;
	DI2RDO2_1.RDO.1 = not DI2RDO2_1.RDO.1;

]]><Regions /></Body>
