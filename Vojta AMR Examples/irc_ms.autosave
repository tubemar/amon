          <Script><![CDATA[event M1_OnOpen()
	SelectData();
end;

event M1_OnRefresh()
	SelectData();
end;

procedure SelectData()
	NumericView1.Row = Ram.Index;
	NumericView2.Row = Ram.Index;
	NumericView3.Row = Ram.Index;
	NumericView4.Row = Ram.Index;
	NumericView5.Row = Ram.Index;
	CaseImage2.Value = Ram.xTz_Mode[Ram.Index,0];
		if ((Ram.Index==1) or (Ram.Index==3) or (Ram.Index==5) or (Ram.Index==6)) then
		//Místnosti se dvěma teplotami
		NumericView4.Visible = true;
		NumericView5.Visible = true;
		Label6.Visible = true;
		Label7.Visible = true;
	else
		//Místnosti s jednou teplotou
		NumericView4.Visible = false;
		NumericView5.Visible = false;
		Label6.Visible = false;
		Label7.Visible = false;
	endif;
end;

event Button4_OnButtonDown()
	if (Ram.Index >= 7) then
		Ram.Index = 0;
	else
		Ram.Index = Ram.Index+1;
	endif;
end;

event Button3_OnButtonDown()
	if (Ram.Index <= 0) then
		Ram.Index = 7;
	else
		Ram.Index = Ram.Index-1;
	endif;
end;

event Blinker1_OnChangeOff()
	if Ram.AM_ComStat[0,0].(Ram.Index) then
		NumericView1.Visible = true;
	else
		NumericView1.Visible = false;
	endif;
end;

event Blinker1_OnChangeOn()
	NumericView1.Visible = true;
end;]]></Script>
