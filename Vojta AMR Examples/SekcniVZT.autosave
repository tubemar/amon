        <Screen Name="S_Start" Class="Amit.Lcw.Shared.LcwScreen">
          <DisplayType>4</DisplayType>
          <Refresh>1000</Refresh>
          <Comment>Startovní sekvekce</Comment>
          <BackColor>#CBD7E7</BackColor>
