event Button1_OnButtonDown()
	if (PinEnter1.Value == EEprom.UserPin) then
		if (PinEnter2.Value == PinEnter3.Value) then
			EEprom.UserPin = PinEnter2.Value;
			//Změna PINu OK
			S_NewPINstatus.Memory1.Value=0;
			S_NewPINstatus.Show();
		else
			//Špatný nový PIN
			S_NewPINstatus.Memory1.Value=2;
			S_NewPINstatus.Show();
		endif;
	else
		//Špatný starý PIN
		S_NewPINstatus.Memory1.Value=1;
		S_NewPINstatus.Show();
	endif;
end;

//cz// Návrat beze změny.
//en// Cancel changes.
event Button2_OnButtonDown()
	S_UserMenu.Show();
end;]]></Script>
