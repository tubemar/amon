          <Script><![CDATA[event S_ScreenSaver_OnOpen()
	Global.ScreenSaver1.Enabled = false;
	Label6.Visible = Ram.ComError;
end;
	
//cz// "Probuzení" displeje.
event ImageMap1_OnRegionDown0()
	Application.Brightness = EEprom.Brightness;
	if Ram.TwilightTime > 0 then
		Global.ScreenSaver1.Value = EEprom.ScreenSaverTime;
		Application.Brightness = EEprom.Brightness;
		S_pin.Show();
	endif;
	Ram.TwilightTime = 5;
end;

event S_ScreenSaver_OnRefresh()
	Label6.Visible = Ram.ComError;
	if (TimerPulse1.Out and not (Ram.Req.0 and Ram.Req.1)) then
		if (CaseImage1.Value < 2) then
			CaseImage1.Value = 2;
		else
			CaseImage1.Value = 1;
		endif;
	else
		CaseImage1.Value = 0;
	endif;

	if (TimerPulse2.Out and not (Ram.Req.2 and Ram.Req.3)) then
		if (CaseImage2.Value < 2) then
			CaseImage2.Value = 2;
		else
			CaseImage2.Value = 1;
		endif;
	else
		CaseImage2.Value = 0;
	endif;

	if Ram.TwilightTime > 0 then
  		Ram.TwilightTime = Ram.TwilightTime - 1;
		if Ram.TwilightTime == 0 then
	  		Application.Brightness = 0;
		endif;
	endif;
end;]]></Script>
