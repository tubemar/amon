          <Script><![CDATA[event S_Brightness_OnOpen()
	//cz// Aktuální hodnota jasu.
	//en// Actual value of brightness.
	ProgressBar1.Value = Application.Brightness;
	Ram.Brightness = Application.Brightness;
end;

//cz// Inkrementace jasu.
//en// Brightness increment.
event Button3_OnButtonPress()
	if ProgressBar1.Value < 100 then
		ProgressBar1.Value = ProgressBar1.Value + 10;
	else
		ProgressBar1.Value = 100;
	endif;
	Application.Brightness = ProgressBar1.Value;
end;

//cz// Dekrementace jasu.
//en// Brightness decrement.
event Button4_OnButtonPress()
	if ProgressBar1.Value > 0 then
		ProgressBar1.Value = ProgressBar1.Value - 10;
	else
		ProgressBar1.Value = 0;
	endif;
	Application.Brightness = ProgressBar1.Value;	
end;

//cz// Uložení hodnoty jasu.
//en// Storage brightness value.
event Button1_OnButtonDown()
	EEprom.Brightness = ProgressBar1.Value;
	S_UserMenu.Show();
end;

//cz// Návrat beze změny.
//en// Cancel changes.
event Button2_OnButtonDown()
	Application.Brightness = Ram.Brightness;
	S_UserMenu.Show();
end;]]></Script>
