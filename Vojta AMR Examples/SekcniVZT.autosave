          <Script><![CDATA[event S_Contrast_OnOpen()
	//cz// Aktuální hodnota kontrastu.
	//en// Actual value of contrast.
	ProgressBar1.Value = Application.Contrast;
	Ram.Contrast = Application.Contrast;
end;

//cz// Inkrementace kontrastu.
//en// Contrast increment.
event Button3_OnButtonPress()
	if ProgressBar1.Value < 100 then
		ProgressBar1.Value = ProgressBar1.Value + 10;
	else
		ProgressBar1.Value = 100;
	endif;
	Application.Contrast = ProgressBar1.Value;
end;

//cz// Dekrementace komtrastu.
//en// Contrast decrement.
event Button4_OnButtonPress()
	if ProgressBar1.Value > 0 then
		ProgressBar1.Value = ProgressBar1.Value - 10;
	else
		ProgressBar1.Value = 0;
	endif;
	Application.Contrast = ProgressBar1.Value;
end;

//cz// Uložení hodnoty kontrastu.
//en// Storage contrast value.
event Button1_OnButtonDown()
	S_UserMenu.Show();
end;

//cz// Návrat beze změny.
//en// Cancel changes.
event Button2_OnButtonDown()
	Application.Contrast = Ram.Contrast;	
	S_UserMenu.Show();
end;]]></Script>
