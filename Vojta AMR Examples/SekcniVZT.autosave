      <Object Name="Process1" Class="Amit.Ngs.Core.NgsProcessPeriod" Def="Process">
        <Id>0</Id>
        <Body Class="Amit.Ngs.Core.NgsCodeBodyText"><![CDATA[//zpoždění po restartu
TimerOnRestart.Exec(Ram.Restart.0);
if (TimerOnRestart.Out) then
	Ram.Restart.0 = false;
endif;

//načtení a filtrace teploty v potrubí
Ram.TeplPrivod_temp = Filtr1R1.Output(ModbusAmr1.AI0);
if (Ram.Restart.0) then
	Ram.TeplPrivod = ModbusAmr1.AI0 + EEprom.TempSenzorCorrection2;
else
	Ram.TeplPrivod = Ram.TeplPrivod_temp + EEprom.TempSenzorCorrection2;
endif;

Ram.TeplProstor = IO.DeviceTemperature + EEprom.TempSenzorCorrection;

//Místnost 3D měření
//horní mez
if (Ram.TeplPrivod > (EEprom.Setpoint1[1,0] + EEprom.Setpoint1[2,0]*0.5)) then
	Ram.Req.0 = false;
else
	if (Ram.TeplPrivod < (EEprom.Setpoint1[1,0] - EEprom.Setpoint1[2,0]*0.5)) then
		Ram.Req.0 = true;
	endif;
endif;
//dolní mez
if (Ram.TeplPrivod < (EEprom.Setpoint1[0,0] - EEprom.Setpoint1[2,0]*0.5)) then
	Ram.Req.1 = false;
else
	if (Ram.TeplPrivod > (EEprom.Setpoint1[0,0] + EEprom.Setpoint1[2,0]*0.5)) then
		Ram.Req.1 = true;
	endif;
endif;

//Místnost TPV
//horní mez
if (Ram.TeplPrivod > (EEprom.Setpoint2[1,0] + EEprom.Setpoint2[2,0]*0.5)) then
	Ram.Req.2 = false;
else
	if (Ram.TeplPrivod < (EEprom.Setpoint2[1,0] - EEprom.Setpoint2[2,0]*0.5)) then
		Ram.Req.2 = true;
	endif;
endif;
//dolní mez
if (Ram.TeplPrivod < (EEprom.Setpoint2[0,0] - EEprom.Setpoint2[2,0]*0.5)) then
	Ram.Req.3 = false;
else
	if (Ram.TeplPrivod > (EEprom.Setpoint2[0,0] + EEprom.Setpoint2[2,0]*0.5)) then
		Ram.Req.3 = true;
	endif;
endif;

//provětrávání 3DM
Ram.Req.4 = not Ram.Klapka.0 and (TimerPulse1.PulseLenght > 0);
TimerOn1.Exec(Ram.Req.4);
TimerPulse1.Exec(TimerOn1.Out);

//provětrávání TPV
Ram.Req.5 = not Ram.Klapka.1 and (TimerPulse2.PulseLenght > 0);
TimerOn2.Exec(Ram.Req.5);
TimerPulse2.Exec(TimerOn2.Out);

//klapka 3DM
Ram.Klapka.0 = not Ram.Restart.0 and ((Ram.Req.0 and Ram.Req.1) or (TimerPulse1.Out));
Ram.RDO.0 = Ram.Klapka.0;

//klapka TPV
Ram.Klapka.1 = not Ram.Restart.0 and ((Ram.Req.2 and Ram.Req.3) or (TimerPulse2.Out));
Ram.RDO.1 = Ram.Klapka.1;

//požadavek na VZT
Ram.RDO.2 = Ram.RDO.0 or Ram.RDO.1;

ModbusAmr1.RDO = Ram.RDO;

//stav komunikace
Ram.ComError = ModBusMaster.Disconnected;
]]><Regions /></Body>
