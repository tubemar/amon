          <Script><![CDATA[event S_DispSet_OnOpen()
	//cz// Aktuální doba spořiče obrazovky.
	//en// Actual screensaver delay.
	NumericView1.Value = EEprom.ScreenSaverTime;
end;

//cz// Inkrementace doby.
//en// Screensaver delay increment.
event Button3_OnButtonPress()
	if NumericView1.Value == -1 then
		NumericView1.Value = 10;
	else
		if NumericView1.Value < 120 then	
			NumericView1.Value = NumericView1.Value + 1;
		endif;
	endif;
end;

//cz// Dekrementace doby.
//en// Screensaver delay decrement.
event Button4_OnButtonPress()
	if NumericView1.Value == 10 then
		NumericView1.Value = -1;
	endif;
	if NumericView1.Value > 10 then
		NumericView1.Value = NumericView1.Value -1;
	endif;
end;

//cz// Uložení doby spořiče obrazovky.
//en// Storage screensaver delay.
event Button1_OnButtonDown()
	EEprom.ScreenSaverTime = NumericView1.Value; 
	Global.ScreenSaver1.Value = NumericView1.Value;
	S_UserMenu.Show();
end;

//cz// Návrat beze změny.
//en// Cancel changes.
event Button2_OnButtonDown()
	S_UserMenu.Show();	
end;]]></Script>
