          </OnRefresh>
          <Elements />
          <Script><![CDATA[event S_Start_OnOpen()
end;

event S_Start_OnRefresh()
	//cz// První spuštění po nahrání aplikace. Inicializace hodnot.
	//en// First start after upload aplication. Initialization of values.
	if Application.ColdStart then
		Application.Brightness = 100;
		EEprom.Brightness = 100;
		Application.Contrast = 50;
		EEprom.ScreenSaverTime = 60;
		Global.ScreenSaver1.Value = 60;
		EEprom.UserPIN = 1111;
		EEprom.Setpoint1[0,0] = 18.0;
		EEprom.Setpoint1[1,0] = 22.0;
		EEprom.Setpoint1[2,0] = 1.0;
		EEprom.Setpoint2[0,0] = 18.0;
		EEprom.Setpoint2[1,0] = 22.0;
		EEprom.Setpoint2[2,0] = 1.0;
		EEprom.AutoVent1[0,0] = 30;
		EEprom.AutoVent1[1,0] = 10;
		EEprom.AutoVent2[0,0] = 30;
		EEprom.AutoVent2[1,0] = 10;
		TimerOn1.Delay = EEprom.AutoVent1[0,0] * 60000;
		TimerPulse1.PulseLenght = EEprom.AutoVent1[1,0] * 60000;
		TimerOn2.Delay = EEprom.AutoVent2[0,0] * 60000;
		TimerPulse2.PulseLenght = EEprom.AutoVent2[1,0] * 60000;
		S_Calib.Show();
	else
	//cz// Po restartu. Načtení uložených hodnot z paměti.
	//en// After restart. Load values from memmory.
		Global.ScreenSaver1.Value = EEprom.ScreenSaverTime;
		Application.Brightness = EEprom.Brightness;
		TimerOn1.Delay = EEprom.AutoVent1[0,0] * 60000;
		TimerPulse1.PulseLenght = EEprom.AutoVent1[1,0] * 60000;
		TimerOn2.Delay = EEprom.AutoVent2[0,0] * 60000;
		TimerPulse2.PulseLenght = EEprom.AutoVent2[1,0] * 60000;
		S_ScreenSaver.Show();
	endif;   
end;]]></Script>
