        <CodeBlock Name="ProcInput" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Cteni vstupu</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[// Digitální vstupy
If @DI_Vstupy, :NONE
	DigIn #0, DI0_in, 0x0000
	Let DI0 = DI0_in ^ DI0_xneg

	DigIn #1, DI1_in, 0x0000
	Let DI1 = DI1_in ^ DI1_xneg

	DigIn #2, DI2_in, 0x0000
	Let DI2 = DI2_in ^ DI2_xneg
EndIf

//Čtení UI AMAP
Call libAI

//Čtení UI Arion
Call libDM_UI8_1
	
// Platnost rozesilane venkovni teploty
Let TVenkLoc1 = AI0[0,0]
VarWStat RemTVenk, ix.9, 0
Timer 0x0008, ix.9, RemVenkValid.0, 0, 1800
Let TVenk1 = if(RemVenkValid.0,RemTVenk,TVenkLoc1)

Let THVV  = AI0[1,0]
Let THVZ  = AI0[2,0]
Let TTUV_PPO1 = AI0[3,0]
Let TTUV_PPO2 = AI0[4,0]

Let TTUVC = AI1[0,0]
Let TTVV1 = AI1[1,0]
Let TTVZ1 = AI1[2,0]
Let TTVV2 = AI1[3,0]
Let TTVZ2 = AI1[4,0]

Let TTUV_Aku = AI2[0,0]
Let TTV_PPO1 = AI2[1,0]
Let TTV_PPO2 = AI2[2,0]
Let dPTV1 = AI2[3,0]
Let PTV1  = AI2[4,0]

Let PHVV  = AR_UI0[0,0]
Let PHVZ  = AR_UI0[1,0]
Let PHVVR = AR_UI0[2,0]
Let PSV   = AR_UI0[3,0]

Let dPHV1 = PHVVR - PHVZ
]]></Script>
          </Body>
          <Period>500</Period>
