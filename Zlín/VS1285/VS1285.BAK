        <CodeBlock Name="ProcDigOut" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Digitální výstupy</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[If @Servis, :NONE
Else :NONE
	Let AO_XR  = 0
	Let DO0_XR = 0
	Let DO1_XR = 0
	Let DO2_XR = 0
	Let DO3_XR = 0
	Let DO4_XR = 0
EndIf

//Digitální výstupy

// Režim automaticky / ručně
Let DO0 = (~DO0_XR & DO0_A) | (DO0_XR & DO0_R)
Let DO1 = (~DO1_XR & DO1_A) | (DO1_XR & DO1_R)
Let DO2 = (~DO2_XR & DO2_A) | (DO2_XR & DO2_R)
Let DO3 = (~DO3_XR & DO3_A) | (DO3_XR & DO3_R)
Let DO4 = (~DO4_XR & DO4_A) | (DO4_XR & DO4_R)
// Beznárazové přepnutí do ručního režimu
Let DO0_R = DO0; DO1_R = DO1; DO2_R = DO2; DO3_R = DO3; DO4_R = DO4
// Zápis na výstupy

If @DO_Vystupy, :NONE
	DigOut DO0, #0, 0x0000
	DigOut DO1, #1, 0x0000
	DigOut DO2, #2, 0x0000
	DigOut DO3, #3, 0x0000
	DigOut DO4, #4, 0x0000
EndIf
]]></Script>
          </Body>
          <Period>500</Period>
