        <CodeBlock Name="RegTUV1" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Regulace TUV</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Regulace TUV s akumulační nádrží

//Casovani TUV 
DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0000, Holidays, UtlTiTUV1, UtlTUV1, WTUV1

Let @ChodTUV1ti = bool(WTUV1 > 10)

//Rozliseni zadane kvuli chodu pro displej
Let WTUVuprdi1 = if(RePIDTUV1.1 and RePIDTUV2.1, 0, WTUV1)

Let ix.0 = @BlokVS or Alarm_E1.8
Let @CTUV_cir = @ChodTUV1di and @ChodTUV1ti and !ix.0			//Čerpadla TUV

Let ix.0 = @BlokVS or Alarm_E1.3 or Alarm_E1.18 or Alarm_E1.8
Let ix.1 = (@ChodTUV1di and @ChodTUV1ti)

 
// Regulace TUV PPO1
Let RePIDTUV1.1 = !(ix.1 and @ReqPPO3) or ix.0
If RePIDTUV1.1
	Let YTUV1 = 0
EndIf
PID WTUV1, TTUV_PPO1, YTUV1, RePIDTUV1, PaPIDTUV1

// Regulace TUV PPO2
Let RePIDTUV2.1 = !(ix.1 and @ReqPPO4) or ix.0
If RePIDTUV2.1
	Let YTUV2 = 0
EndIf
PID WTUV1, TTUV_PPO2, YTUV2, RePIDTUV2, PaPIDTUV1
 ]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="RegUT1" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Regulace vetve UT</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//************************************************************
// ÚT pro byty
//************************************************************

// Určení výchozí hodnoty žádané
If @KonstPr1
	Let WTV1 = KonstPrTV1
Else :NONE
	//Interpolace ekvitermni krivky
	//Najdeme 5. bod křivky - při jaké venkovce bude W=0
	Let EkvPoint1[4,0] = if((EkvPoint1[3,1]-EkvPoint1[2,1])!=0, EkvPoint1[2,0] - (EkvPoint1[2,1]*(EkvPoint1[3,0]-EkvPoint1[2,0])/(EkvPoint1[3,1]-EkvPoint1[2,1])), 30)
	Let EkvPoint1[4,1] = 0
	Interpol TVenk1, WTV1, EkvPoint1
EndIf

// Casovy plan UT
DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0000, Holidays, UtlTiTV1, UtlTV1, PrUtlTV
// Aplikace útlumu
Let WTV1 = WTV1-(WTV1/100)*PrUtlTV
Limiter WTV1,0,80

Hyst2 WTV1, @ChodTV1, TNajVyp, TNajZap, 0									//Hystereze najizdeci teploty

Let @CTV1_1 = (@ChodTV1di and @ChodTV1 and @ChodTV_TE) and !@BlokVS and @TlakTV_ok			//Cerpadlo UT1_1 + protaceni
Let @CTV1_2 = ((@ChodTV2di and @ChodTV1 and @ChodTV_TE and @SelCTV1 and Alarm_E1.5) or bool(ProTV)) and @TlakTV_ok and !@BlokVS 	//Cerpadlo UT1_2 + protaceni

Let ix.0 = Alarm_E1.0 or Alarm_E1.16 or @BlokVS
Let ix.1 = @ChodTV1di and @ChodTV1 and @ChodTV_TE
Let ix.2 = !ix.1 or ix.0

// Rampování žádané
fb_Rampa WTV1, TTVV1, ix.2, SlewRateUp, SlewRateDn, WTV1_reg


// Regulace UT1 PPO1
Let RePIDTV1.1 = !(ix.1 and @ReqPPO1) or ix.0								
If RePIDTV1.1										//Manuální režim při odstavení nebo poruše
	Let YTV1 = 0									//V manuálu zavřít ventil
EndIf

PID WTV1_reg, TTV_PPO1, YTV1, RePIDTV1, PaPIDTV1

// Regulace UT1 PPO2
Let RePIDTV2.1 = !(ix.1 and @ReqPPO2) or ix.0						
If RePIDTV2.1										//Manuální režim při odstavení nebo poruše
	Let YTV2 = 0									//V manuálu zavřít ventil
EndIf

PID WTV1_reg, TTV_PPO2, YTV2, RePIDTV2, PaPIDTV1


//Regulace dP UT1
Let RePIDdTV.1 = !(@ChodTV1di and @ChodTV1) or !@CTV1_1	
If RePIDdTV.1										//Manuální režim při odstavení nebo poruše
	Let YdPTV1 = 0								//V manuálu stop
EndIf 
TimerOff RePIDdTV.1, tNabeh_dPTV, ix.9, Lx
Let WdPTV1_reg = WdPTV1*(Lx/tNabeh_dPTV)						//rampa náběhu žádané
PID WdPTV1_reg, dPTV1, YdPTV1, RePIDdTV, PaPIDdTV



]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="RegUT2" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Regulace vetve UT</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//************************************************************
// ÚT pro MŠ
//************************************************************

// Určení výchozí hodnoty žádané
If @KonstPr2
	Let WTV2 = KonstPrTV2
Else :NONE
	//Interpolace ekvitermni krivky
	//Najdeme 5. bod křivky - při jaké venkovce bude W=0
	Let EkvPoint2[4,0] = if((EkvPoint2[3,1]-EkvPoint2[2,1])!=0, EkvPoint2[2,0] - (EkvPoint2[2,1]*(EkvPoint2[3,0]-EkvPoint2[2,0])/(EkvPoint2[3,1]-EkvPoint2[2,1])), 30)
	Let EkvPoint2[4,1] = 0
	Interpol TVenk1, WTV2, EkvPoint2
EndIf

// Casovy plan UT
DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0000, Holidays, UtlTiTV2, UtlTV2, PrUtlTV
// Aplikace útlumu
Let WTV2 = WTV2-(WTV2/100)*PrUtlTV
Limiter WTV2,0,80

Hyst2 WTV2, @ChodTV2, TNajVyp, TNajZap, 0								//Hystereze najizdeci teploty

Let @CTV2 = ((@ChodTV2di and @ChodTV2) or bool(ProTV)) and !@BlokVS and @TlakTV_ok 		//Cerpadlo UT2 + protaceni

Let ix.0 = Alarm_E1.2 or Alarm_E1.17 or @BlokVS
Let ix.1 = @ChodTV2di and @ChodTV2 and @ChodTV_TE
Let ix.2 = !ix.1 or ix.0

// Rampování žádané
fb_Rampa WTV2, TTVV2, ix.2, SlewRateUp, SlewRateDn, WTV2_reg

// Regulace UT2
Let RePIDTV3.1 = !ix.1 or ix.0				
If RePIDTV3.1										//Manuální režim při odstavení nebo poruše
	Let YTV3 = 0									//V manuálu zavřít ventil
EndIf 

PID WTV2_reg, TTVV2, YTV3, RePIDTV3, PaPIDTV3
]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="RTC" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Zimní/letní čas</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[sb_RTC RTC_time, RTC_parts]]></Script>
          </Body>
          <Period>1000</Period>
          <Offset>0</Offset>
          <Type>15</Type>
          <InterruptEdge>0</InterruptEdge>
        </CodeBlock>
      </CodeBlockList>
      <CodeFBList>
        <CodeBlock Name="fb_Rampa" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>Rampování žádané </Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	
	VarWStat iSRDn, @Chng, 0
	If @Chng
		//Velikost kroku rampy za 1 volání
		Let SR_dy_dn = iSRDn/60.0
	Endif
	VarWStat iSRUp, @Chng, 0
	If @Chng
		//Velikost kroku rampy za 1 volání
		Let SR_dy_up = iSRUp/60.0
	Endif
	If @Disable
		Let ioWSlide = If(iM < iW, iM, iW)
	Else
		Let @dXYup_OK = ((iW - ioWSlide) < SR_dy_up) or (SR_dy_up < 0.00001)
		Let @dXYdn_OK = ((ioWSlide - iW) < SR_dy_dn) or (SR_dy_dn < 0.00001)
		Let ioWSlide  = If(iW > ioWSlide, If(@dXYup_OK, iW, ioWSlide+SR_dy_up), If(@dXYdn_OK, iW, ioWSlide-SR_dy_dn))
	Endif]]></Script>
          </Body>
