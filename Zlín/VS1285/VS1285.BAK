        <CodeBlock Name="Kaskada1" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>kaskáda PPO</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[
If @KaskMan1
	Let Ka_PocPPO1 = Ka_PocPPOMan[0,0]
Else 
	Let YTV_Max = max(YTV1, YTV2)
	Let @RqChngPPO = (YTV_Max > Ka_MinMax[0,1]) and !@BlokChngK1 and (Ka_PocPPO1 < 2)	//Přidat PPO?
	TimerOn @RqChngPPO, tKa_MinMax[0,1], @AddPPO1, tKa_MinMax[1,1]					//Trvá požadavavek dost dlouho?

	Let @RqChngPPO = (YTV_Max < Ka_MinMax[0,0]) and !@BlokChngK1  and (Ka_PocPPO1 > 1)	//Ubrat PPO?
	TimerOn @RqChngPPO, tKa_MinMax[0,0], @RemPPO1, tKa_MinMax[1,0]					//Trvá požadavavek dost dlouho?

	If (@BlokChngK1)
	Else
		If (@AddPPO1 and (Ka_PocPPO1 < 2))
			Let Ka_PocPPO1 = Ka_PocPPO1+1
		Endif
		If (@RemPPO1 and (Ka_PocPPO1 > 1))
			Let Ka_PocPPO1 = Ka_PocPPO1-1
		Endif
	Endif
	Let ix.0 = @AddPPO1 or @RemPPO1
	TimerPuls ix.0, tKa_MinMax[0,2], @BlokChngK1, NONE						//Počkat po změně na ustálení
EndIf 

Limiter Ka_PocPPO1, 1, 2

Let ControlKask1[0,0].0 = !@EnTV_PO1di
Let ControlKask1[1,0].0 = !@EnTV_PO2di

Let ix = 0 	// if(Alarm_E1.1 or Alarm_E1.14 or Alarm_E1.15 or Alarm_E1.16,1,0)
Boilers Ka_PocPPO1, ix, OperTimePPO1, ControlKask1, 0x0004, 0

Let @ReqPPO1 = ControlKask1[0,1].1
Let @ReqPPO2 = ControlKask1[1,1].1
//Let @ReqPPO3 = ControlKask2[2,1].1
//Let @ReqPPO4 = ControlKask2[3,1].1]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="Kaskada2" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>kaskáda PPO</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[
If @KaskMan2
	Let Ka_PocPPO2 = Ka_PocPPOMan[1,0]
Else 
	Let YTV_Max = max(YTUV1, YTUV2)
	Let @RqChngPPO = (YTV_Max > Ka_MinMax[0,1]) and !@BlokChngK2 and (Ka_PocPPO2 < 2)		//Přidat PPO?
	TimerOn @RqChngPPO, tKa_MinMax[0,1], @AddPPO2, tKa_MinMax[1,1]					//Trvá požadavavek dost dlouho?

	Let @RqChngPPO = (YTV_Max < Ka_MinMax[0,0]) and !@BlokChngK2  and (Ka_PocPPO2 > 1)		//Ubrat PPO?
	TimerOn @RqChngPPO, tKa_MinMax[0,0], @RemPPO2, tKa_MinMax[1,0]					//Trvá požadavavek dost dlouho?

	If (@BlokChngK2)
	Else
		If (@AddPPO2 and (Ka_PocPPO2 < 2))
			Let Ka_PocPPO2 = Ka_PocPPO2+1
		Endif
		If (@RemPPO2 and (Ka_PocPPO2 > 1))
			Let Ka_PocPPO2 = Ka_PocPPO2-1
		Endif
	Endif
	Let ix.0 = @AddPPO2 or @RemPPO2
	TimerPuls ix.0, tKa_MinMax[0,2], @BlokChngK2, NONE						//Počkat po změně na ustálení
EndIf 

Limiter Ka_PocPPO2, 1, 2

Let ControlKask2[0,0].0 = !@EnTUV_PO1di
Let ControlKask2[1,0].0 = !@EnTUV_PO2di

Let ix = 0		//if(Alarm_E1.1 or Alarm_E1.14 or Alarm_E1.15 or Alarm_E1.18,1,0)
Boilers Ka_PocPPO2, ix, OperTimePPO2, ControlKask2, 0x0004, 0

Let @ReqPPO3 = ControlKask2[0,1].1
Let @ReqPPO4 = ControlKask2[1,1].1]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="Klapky" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Ovládání klapek PPO v kaskádách</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[// Řízení klapek výměníků

TimerOff @ReqPPO1, tDobKlap, ix.0, NONE
TimerOff @ReqPPO2, tDobKlap, ix.1, NONE
TimerOff @ReqPPO3, tDobKlap, ix.2, NONE
TimerOff @ReqPPO4, tDobKlap, ix.3, NONE

BinDiff ix, Kl_Zmena, 0x000F, 0x000F, 0x00000000

// UT
If (Kl_Zmena.0)
	Let @KlTV_PPO1z = false
	Let @KlTV_PPO1o = false
Else
	Let @KlTV_PPO1o = ix.0
	Let @KlTV_PPO1z = !@KlTV_PPO1o
EndIf

If (Kl_Zmena.1)
	Let @KlTV_PPO2z = false
	Let @KlTV_PPO2o = false
Else
	Let @KlTV_PPO2o = ix.1
	Let @KlTV_PPO2z = !@KlTV_PPO2o
EndIf


// TUV
If (Kl_Zmena.3)
	Let @KlTUV_PPO1z = false
	Let @KlTUV_PPO1o = false
Else
	Let @KlTUV_PPO1o = ix.2
	Let @KlTUV_PPO1z = !@KlTUV_PPO1o
EndIf

If (Kl_Zmena.4)
	Let @KlTUV_PPO2z = false
	Let @KlTUV_PPO2o = false
Else
	Let @KlTUV_PPO2o = ix.3
	Let @KlTUV_PPO2z = !@KlTUV_PPO2o
EndIf

]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="libAI" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Zpracování analogových vstupů</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[     	//Analogové vstupy
	//*************************************************
	//******************* KANÁL AI0 *******************
	//*************************************************
	
	//******************* ANALOGOVÝ VSTUP AI0.0 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AI0_typ.0, :NONE
	  //Pt1000
	  If TC0_typ.0, :NONE
	    //Měření napětí na čidle v rozsahu 0 .. 5 V
	    AnIn #Ni03_0, AI0_Pom[0,0], 5.000, 0.000, 5.000, 0.000, 5.000
	    //Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
	    Interpol AI0_Pom[0,0], AI0_okamzita[0,0], Pt1000_Kriv
	  //Ni1000
	  Else :NONE
           //5000ppm
           If Ni0_typ.0, :NONE
             Ni1000 #Ni03_0, AI0_okamzita[0,0], 5000
             //6180ppm
           Else :NONE
             Ni1000 #Ni03_0, AI0_okamzita[0,0], 6180
           EndIf 
	  EndIf
         //Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
       Else :NONE
         AnIn #AI00_0, AI0_okamzita[0,0], AI0_params[0,0], AI0_params[0,1], AI0_params[0,2], AI0_params[0,3], AI0_params[0,4]
       EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AI0_konst[0, 0]*AI0_okamzita[0, 0]+AI0_konst[0, 1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AI0[0,0], AI0_konst[0,2]
	
	
       //******************* ANALOGOVÝ VSTUP AI0.1 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AI0_typ.1, :NONE
	  //Pt1000
	  If TC0_typ.1, :NONE
	    //Měření napětí na čidle v rozsahu 0 .. 5 V
	    AnIn #Ni03_1, AI0_Pom[1,0], 5.000, 0.000, 5.000, 0.000, 5.000
	    //Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
	    Interpol AI0_Pom[1,0], AI0_okamzita[1,0], Pt1000_Kriv
	  //Ni1000
	  Else :NONE
           //5000ppm
           If Ni0_typ.1, :NONE
             Ni1000 #Ni03_1, AI0_okamzita[1,0], 5000
             //6180ppm
           Else :NONE
             Ni1000 #Ni03_1, AI0_okamzita[1,0], 6180
           EndIf 
	  EndIf
         //Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
       Else :NONE
         AnIn #AI00_1, AI0_okamzita[1,0], AI0_params[1,0], AI0_params[1,1], AI0_params[1,2], AI0_params[1,3], AI0_params[1,4]
       EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AI0_konst[1, 0]*AI0_okamzita[1, 0]+AI0_konst[1, 1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AI0[1,0], AI0_konst[1,2]
       
	//******************* ANALOGOVÝ VSTUP AI0.2 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AI0_typ.2, :NONE
	  //Pt1000
	  If TC0_typ.2, :NONE
