	//Přepnutí kanálu AI0 na AUTOMATICKÝ nebo MANUÁLNÍ režim
	For Ix, 0.000, 4.000, 1.000
	  Let  AI0[Ix,0] = IF(AI0_XR.(Ix),AI0_R[Ix,0],AI0[Ix,0]); AI0_R[Ix,0] = AI0[Ix,0]
	EndFor
     

