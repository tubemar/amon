	if LCD_col>=5 then
		LCD_col=0;
	else
		LCD_col=LCD_col+1;
	endif;
	UtlTiAct = UtlTiTUV1[LCD_row,LCD_col];
	NumericEdit1.Value = UtlTUV1[LCD_row,LCD_col];
	NumericView1.Value = LCD_col+1;
end;

event Key2_OnKeyPress()								//posun o polozku planu vzad
	if LCD_col<=0 then
		LCD_col=5;
	else
		LCD_col=LCD_col-1;
	endif;
	UtlTiAct = UtlTiTUV1[LCD_row,LCD_col];
	NumericEdit1.Value = UtlTUV1[LCD_row,LCD_col];
	NumericView1.Value = LCD_col+1;
end;

