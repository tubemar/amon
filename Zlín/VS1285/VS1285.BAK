TimerOn @Restart, tRestart[0,0], ix.0, NONE
Let @Restart = If(ix.0,false,@Restart)

Let @PSVF = AI1_okamzita[3,0] < -5				//Snímač SV - porucha
Hyst PTV1,@TlakTV_ok,50.000,5.000,0x0000			//Hranice tlaku pro povolení čerpadel

Let ix.0 = TTVV1>90
TimerOn ix.0, 120000, ix.1, NONE
Let Alarm_A1.0 = ix.1			       	//Prehrati ÚT byty

Let Alarm_A1.1 = @STOPBtn and !@OverVS			//Prehrati prostoru VS

Let ix.2 = (TTVV2>90)
TimerOn ix.2, 120000, ix.3, NONE
Let Alarm_A1.2 = ix.3 		       		//Přehřátí ÚT MŠ

Let ix.4 = TTUV_Aku>65
TimerOn ix.4, 60000, ix.5, NONE
Let Alarm_A1.3 = ix.5					//Přehřátí TUV

Let Alarm_A1.4 = (TTUV_PPO1<WTUV1-7) and !RePIDTUV1.1  	//Nízká tepl. TUV akumulace
Let Alarm_A1.5 = !@chCTV1_1 and @CTV1_1  			//Porucha měniče ÚT byty
Let Alarm_A1.6 = !@chCTV1_2 and @CTV1_2      		//Porucha cerp. ÚT byty 2
Let Alarm_A1.7 = !@chCTUV_cir and @CTUV_cir     		//Porucha TUV cirk.
Let Alarm_A1.8 = !@TlakSV or ((PSV<100) and !@PSVF)	//Tlak SV
Let Alarm_A1.9 = PTV1 < PTV1min				//Nízký tlak TV
Let Alarm_A1.10 = PTV1 > PTV1max				//Vysoký tlak TV
Let Alarm_A1.11 = @SolDop					//Dlouhé doplňování
Let Alarm_A1.12 = (TTUVC<WTUV1-12) and !RePIDTUV1.1	//Nízká tepl. TUV cirk.
Let Alarm_A1.13 = !@chCTV2 and @CTV2       		//Porucha cerp. ÚT MŠ
Let Alarm_A1.14 = @OverVS and !@LeakVS			//Zaplaveni VS
Let Alarm_A1.15 = !@STOPBtn	       			//STOP tlacitko
Let Alarm_A1.16 = !@Over_TV1	      			//Porucha přehřátí ÚT1 termostat
Let Alarm_A1.17 = !@Over_TV2	      			//Porucha přehřátí ÚT2 termostat
Let Alarm_A1.18 = !@Over_TUV1	      			//Porucha přehřátí TUV termostat

//Kvit pro vsechny chyby
Let Alarm_Q1 = If(KvitOut.0, ~Alarm_A1, Alarm_Q1)
Let KvitOut.0 = false

ErrSig32 Alarm_A1, 0x00001, Alarm_Q1, 0x00001, Alarm_E1.0, Alarm_Ig.0, 4, 4, 1010, 0, 0, 0			//Prehrati ÚT byty
ErrSig32 Alarm_A1, 0x00002, Alarm_Q1, 0x00002, Alarm_E1.1, Alarm_Ig.0, 4, 4, 1010, 1, 0, 0       		//Prehrati prostoru
ErrSig32 Alarm_A1, 0x00004, Alarm_Q1, 0x00004, Alarm_E1.2, Alarm_Ig.0, 4, 4, 1010, 2, 0, 0	       	//Přehřátí ÚT MŠ
ErrSig32 Alarm_A1, 0x00008, Alarm_Q1, 0x00008, Alarm_E1.3, Alarm_Ig.0, 5, 4, 1010, 3, 0, 0	       	//Prehrati TUV
ErrSig32 Alarm_A1, 0x00010, Alarm_Q1, 0x00010, Alarm_E1.4, Alarm_Ig.0, 1800, 4, 1010, 4, 0, 0       	//Nizka tepl. TUV
ErrSig32 Alarm_A1, 0x00020, Alarm_Q1, 0x00020, Alarm_E1.5, Alarm_Ig.0, 6, 4, 1010, 5, 0, 0	       	//Porucha měnič ÚT byty
ErrSig32 Alarm_A1, 0x00040, Alarm_Q1, 0x00040, Alarm_E1.6, Alarm_Ig.0, 6, 4, 1010, 6, 0, 0       		//Porucha cerp. ÚT byty 2
ErrSig32 Alarm_A1, 0x00080, Alarm_Q1, 0x00080, Alarm_E1.7, Alarm_Ig.0, 6, 4, 1010, 7, 0, 0       		//Porucha TUV cirk.
ErrSig32 Alarm_A1, 0x00100, Alarm_Q1, 0x00100, Alarm_E1.8, Alarm_Ig.0, 10, 4, 1010, 8, 0, 0      		//Tlak SV
ErrSig32 Alarm_A1, 0x00200, Alarm_Q1, 0x00200, Alarm_E1.9, Alarm_Ig.0, 10, 4, 1010, 9, 0, 0     		//Níz. tlak TV
ErrSig32 Alarm_A1, 0x00400, Alarm_Q1, 0x00400, Alarm_E1.10, Alarm_Ig.0, 10, 4, 1010, 10, 0, 0       	//Vys. tlak TV
ErrSig32 Alarm_A1, 0x00800, Alarm_Q1, 0x00800, Alarm_E1.11, Alarm_Ig.0, 900, 4, 1010, 11, 0, 0       	//Dlouhé doplň.
ErrSig32 Alarm_A1, 0x01000, Alarm_Q1, 0x01000, Alarm_E1.12, Alarm_Ig.0, 2400, 4, 1010, 12, 0, 0      	//Nízká tepl. TUV cirk.
ErrSig32 Alarm_A1, 0x02000, Alarm_Q1, 0x02000, Alarm_E1.13, Alarm_Ig.0, 6, 4, 1010, 13, 0, 0       	//Porucha cerp. MŠ
ErrSig32 Alarm_A1, 0x04000, Alarm_Q1, 0x04000, Alarm_E1.14, Alarm_Ig.0, 4, 4, 1010, 14, 0, 0       	//Zaplaveni VS
ErrSig32 Alarm_A1, 0x08000, Alarm_Q1, 0x08000, Alarm_E1.15, Alarm_Ig.0, 2, 1, 1010, 15, 0, 0       	//STOP tlacitko
ErrSig32 Alarm_A1, 0x10000, Alarm_Q1, 0x10000, Alarm_E1.16, Alarm_Ig.0, 2, 4, 1010, 16, 0, 0       	//Přehřátí ÚT1 termostat
ErrSig32 Alarm_A1, 0x20000, Alarm_Q1, 0x20000, Alarm_E1.17, Alarm_Ig.0, 2, 4, 1010, 17, 0, 0       	//Přehřátí ÚT2 termostat
ErrSig32 Alarm_A1, 0x40000, Alarm_Q1, 0x40000, Alarm_E1.18, Alarm_Ig.0, 2, 4, 1010, 18, 0, 0       	//Přehřátí TUV termostat
Let @ErrOr = bool(Alarm_E1)			//suma poruch pro LCD

//Autokvitace poruch
Let Alarm_AQ1.0  = Alarm_E1.0  and not Alarm_A1.0
Let Alarm_AQ1.1  = Alarm_E1.1  and not Alarm_A1.1
Let Alarm_AQ1.2  = Alarm_E1.2  and not Alarm_A1.2
Let Alarm_AQ1.3  = Alarm_E1.3  and not Alarm_A1.3
Let Alarm_AQ1.4  = Alarm_E1.4  and not Alarm_A1.4
Let Alarm_AQ1.5  = Alarm_E1.5  and not Alarm_A1.5
Let Alarm_AQ1.6  = Alarm_E1.6  and not Alarm_A1.6
Let Alarm_AQ1.7  = Alarm_E1.7  and not Alarm_A1.7
Let Alarm_AQ1.8  = Alarm_E1.8  and not Alarm_A1.8
Let Alarm_AQ1.9  = Alarm_E1.9  and not Alarm_A1.9
Let Alarm_AQ1.10 = Alarm_E1.10 and not Alarm_A1.10
Let Alarm_AQ1.11 = Alarm_E1.11 and not Alarm_A1.11
Let Alarm_AQ1.12 = Alarm_E1.12 and not Alarm_A1.12
Let Alarm_AQ1.13 = Alarm_E1.13 and not Alarm_A1.13
Let Alarm_AQ1.14 = Alarm_E1.14 and not Alarm_A1.14
//Let Alarm_AQ1.15 = Alarm_E1.15 and not Alarm_A1.15
Let Alarm_AQ1.16 = Alarm_E1.16 and not Alarm_A1.16
Let Alarm_AQ1.17 = Alarm_E1.17 and not Alarm_A1.17
Let Alarm_AQ1.18 = Alarm_E1.18 and not Alarm_A1.18


Timer 0x0008, Alarm_AQ1.0, Alarm_Q1.0, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.1, Alarm_Q1.1, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.2, Alarm_Q1.2, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.3, Alarm_Q1.3, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.4, Alarm_Q1.4, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.5, Alarm_Q1.5, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.6, Alarm_Q1.6, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.7, Alarm_Q1.7, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.8, Alarm_Q1.8, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.9, Alarm_Q1.9, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.10, Alarm_Q1.10, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.11, Alarm_Q1.11, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.12, Alarm_Q1.12, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.13, Alarm_Q1.13, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.14, Alarm_Q1.14, Alarm_AQTm, 0
//Timer 0x0008, Alarm_AQ1.15, Alarm_Q1.15, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.16, Alarm_Q1.16, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.17, Alarm_Q1.17, Alarm_AQTm, 0
Timer 0x0008, Alarm_AQ1.18, Alarm_Q1.18, Alarm_AQTm, 0


Let @BlokVS = Alarm_E1.15 or Alarm_A1.15 or Alarm_E1.14 or Alarm_E1.1
]]></Script>
          </Body>
          <Period>1000</Period>
          <Offset>100</Offset>
          <Type>1</Type>
          <InterruptEdge>1</InterruptEdge>
        </CodeBlock>
