Let RePIDTV2  = RePIDTV2  | 0x0010
Let RePIDTUV1  = RePIDTUV1  | 0x0010
Let RePIDdHV = RePIDdHV | 0x0010
Let RePIDdTV = RePIDdTV | 0x0010

// Inicializace výstupů
Let DO0 = 0
Let DO1 = 0
Let DO2 = 0
Let DO3 = 0
Let DO4 = 0

Let AO[0,0] = 0
Let AO[1,0] = 0
Let AO[2,0] = 0
Let AO[3,0] = 0]]></Script>
