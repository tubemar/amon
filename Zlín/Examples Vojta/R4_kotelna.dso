	Let tvx_Tm[7, 0] = AR_UI0[2, 0]		//UT5 HK6
	Let tvx_Tm[8, 0] = AR_UI0[3, 0]		//UT6 HK7
	Let tvx_Tm[9, 0] = AR_UI0[4, 0]		//UT7 HK8
	Let tvx_Tm[10,0] = AR_UI0[5, 0]		//UT8 HK9
	Let PTV1	= AR_UI0[6, 0]		//Tlak
	Let LTANK	= AR_UI0[7, 0]		//Hladina/hmotnost doplňovací nádrže
]]></Script>
