          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	// Řízení 3-bod. ventilů
	Valve tvx_akce[3,0], tvx_prebeh[3,0], tvx_poloha[3,0], @zavUT1, @otvUT1
	Valve tvx_akce[4,0], tvx_prebeh[4,0], tvx_poloha[4,0], @zavUT2, @otvUT2
	Valve tvx_akce[5,0], tvx_prebeh[5,0], tvx_poloha[5,0], @zavUT3, @otvUT3
	Valve tvx_akce[6,0], tvx_prebeh[6,0], tvx_poloha[6,0], @zavUT4, @otvUT4
	Valve tvx_akce[7,0], tvx_prebeh[7,0], tvx_poloha[7,0], @zavUT5, @otvUT5
	Valve tvx_akce[8,0], tvx_prebeh[8,0], tvx_poloha[8,0], @zavUT6, @otvUT6
	Valve tvx_akce[9,0], tvx_prebeh[9,0], tvx_poloha[9,0], @zavUT7, @otvUT7
	Valve tvx_akce[10,0], tvx_prebeh[10,0], tvx_poloha[10,0], @zavUT8, @otvUT8
	
	// Režim automaticky / ručně
       Let DO0 = (~DO0_XR & DO0_A) | (DO0_XR & DO0_R)
       // Beznárazové přepnutí do ručního režimu
       Let DO0_R = DO0
       // Zápis na výstupy
       DigOut DO0, #0, 0x0000

	Call libDM_RDO8_1
	Call libDM_RDO8_2
	Call libDM_DO18_1
	Call libDM_DO18_2

	
]]></Script>
