        <CodeBlock Name="libDM_RDO8_1" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Zápis na digitální výstupy xxxRDO8</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//Digitální výstupy
	
	//*************************************************
	//******************* KANÁL RDO0 *******************
	//*************************************************
	
	//Přepnutí AUTOMATICKÝ nebo MANUÁLNÍ režim
	Let AR_RL0 = ((~AR_RL0_XR | AR_RL0_Valve) & AR_RL0_A) | (AR_RL0_XR & ~AR_RL0_Valve & AR_RL0_R)
	//Beznárazové přepnuti do ručního režimu  
