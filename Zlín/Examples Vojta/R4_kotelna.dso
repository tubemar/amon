	Else :NONE
		Let AR_UI1_okam[5,0] = AR_UI1_pom[5,0]
	EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AR_UI1_konst[5, 0]*AR_UI1_okam[5, 0]+AR_UI1_konst[5, 1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AR_UI1[5,0], AR_UI1_konst[5,2]

	//******************* ANALOGOVÝ VSTUP UI6 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AR_UI1_typ.6, :NONE
		//Pro převod U->T používám rozsah 0-5V (není třeba měnit default params)
		Let fX = AR_UI1_pom[6,0]/20.0
		//Pt1000
		If AR_TC1_typ.6, :NONE
			//Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
			Interpol fX, AR_UI1_okam[6,0], Pt1000_Kriv
		//Ni1000
		Else :NONE
			//5000ppm
			If AR_Ni1_typ.6, :NONE
				Ni1000U2T fX, AR_UI1_okam[6,0], 5000, 15.000, 3920.000
			//6180ppm
			Else :NONE
				Ni1000U2T fX, AR_UI1_okam[6,0], 6180, 15.000, 3920.000
			EndIf 
		EndIf
	//Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
	Else :NONE
		Let AR_UI1_okam[6,0] = AR_UI1_pom[6,0]
	EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AR_UI1_konst[6, 0]*AR_UI1_okam[6, 0]+AR_UI1_konst[6, 1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AR_UI1[6,0], AR_UI1_konst[6,2]

	//******************* ANALOGOVÝ VSTUP UI7 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AR_UI1_typ.7, :NONE
		//Pro převod U->T používám rozsah 0-5V (není třeba měnit default params)
		Let fX = AR_UI1_pom[7,0]/20.0
		//Pt1000
		If AR_TC1_typ.7, :NONE
			//Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
			Interpol fX, AR_UI1_okam[7,0], Pt1000_Kriv
		//Ni1000
		Else :NONE
			//5000ppm
			If AR_Ni1_typ.7, :NONE
				Ni1000U2T fX, AR_UI1_okam[7,0], 5000, 15.000, 3920.000
			//6180ppm
			Else :NONE
				Ni1000U2T fX, AR_UI1_okam[7,0], 6180, 15.000, 3920.000
			EndIf 
		EndIf
	//Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
	Else :NONE
		Let AR_UI1_okam[7,0] = AR_UI1_pom[7,0]
	EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AR_UI1_konst[7, 0]*AR_UI1_okam[7, 0]+AR_UI1_konst[7, 1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AR_UI1[7,0], AR_UI1_konst[7,2]

	//Přepnutí kanálu UI na AUTOMATICKÝ nebo MANUÁLNÍ režim
	For Ix, 0.000, 7.000, 1.000
	  Let  AR_UI1[Ix,0] = IF(AR_UI1_XR.(Ix),AR_UI1_R[Ix,0],AR_UI1[Ix,0]); AR_UI1_R[Ix,0] = AR_UI1[Ix,0]
	EndFor]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="libDO" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Zápis na digitální výstupy</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//Digitální výstupy
	
	//*************************************************
	//******************* KANÁL DO0 *******************
	//*************************************************
	
	//Přepnutí AUTOMATICKÝ nebo MANUÁLNÍ režim
	Let DO0 = ((~DO0_XR | DO0_Valve) & DO0_A) | (DO0_XR & ~DO0_Valve & DO0_R)
	//Beznárazové přepnuti do ručního režimu  
	Let DO0_R = DO0
	
	//*************** Zápis na digitální výstupy kanálu DO0 ***************
	DigOut DO0, #0, 0x0000
	
]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="libDopln" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Řízení doplňování, odpouštění a odplynění</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	// řízení dopouštění a odpouštění
	Hyst2 PTV1, ix.0, P_dopl[0,0], P_dopl[1,0], 0x0001
	Hyst2 PTV1, ix.1, P_odp[0,0], P_odp[1,0], 0x0000
	
	// odplynění
	
	Let @ReqDeGas = ((RTC_time mod 86400 > DeGas_tod) and @EnAutDegas) or @ManStartD //každý den automaticky nebo ručně
	Let @ReqStartDeg = @ReqDeGas and !@ReqDeGasOld and !@KotStop
	Let @ReqDeGasOld = @ReqDeGas
	Let @ReqStopDeG = @ManStopD or @oDeGas or @Restart or @KotStop
	
	RS @ReqStartDeg, @ReqStopDeG, @xDegas
	
	Let @ManStartD = if(@ManStartD,false,@ManStartD)
	Let @ManStopD = if(@ManStopD,false,@ManStopD)
	
	TimerOn @xDegas, tDeGas, @oDeGas, NONE
	
	Let @CDOP   = !@KotStop and !Alarm_E1.10 and !Alarm_A1.8 and ((ix.0 and !ix.1) or @xDeGas)
	Let @ReqOdp = !@KotStop and ix.1
	
	Let RVODP_rezim.1 = @KotStop
	If RVODP_rezim.1
		Let Yodp_akce = 0
	Endif
	Let fx = if(@xDegas,(P_dopl[0,0] + P_odp[0,0])/2, P_odp[0,0])
	PID PTV1, fx, Yodp_akce, RVODP_rezim, RVODP_PID
	
	// řízení dopouštění nádrže
	Hyst2 LTANK, ix.13, LevelZ[1,0], LevelZ[2,0], 0x0001
	Let @DopNadrz = ix.13 and !@KotStop]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="libObecne" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Obecne</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[
// Prázdniny
Holiday Svatky[*,19], Prazdniny

// Léto podle venkovní teploty

// Při výpadku venkovky použijeme jinou, příp. náhradní teplotu
Let TE_reg = if(@TVenkF, TE_const, TVenkS)

Let TE_regJ= if(@TVenkJF or !@R2_CommOK, TE_reg, TVenkJ)

// Tlumená venkovní teplota, vypočtemě z průměru teplot
Let fx = 60 * TE_leto_par[0, 0]
Filtr1R TE_reg, Te_tlum, fx

// Meze regulační a tlumené venkovní teploty
Hyst TE_reg, ix.0, TE_leto_par[1, 0], 1.000, 0x0000
Hyst TE_tlum, ix.1, TE_leto_par[1, 0], 1.000, 0x0000

// Pokud obě vysoké -> léto, pokud obě nízké -> zima
Let @te_xLeto = if(ix.0 or ix.1, @te_xLeto, false)
Let @te_xLeto = if(ix.0 and ix.1, true, @te_xLeto)
]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="libZdroje" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Řízení a řazení zdrojů tepla</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//Požadavek TUV
	Let @ReqTUV = tvx_stav[0,0].8 or tvx_stav[1,0].8 or tvx_stav[2,0].8
	
	//Požadavky jednotlivých větví ÚT
	Let ix.0 = tvx_stav[3,0].9 and !tvx_stav[3,0].12
	Let ix.1 = tvx_stav[4,0].9 and !tvx_stav[4,0].12
	Let ix.2 = tvx_stav[5,0].9 and !tvx_stav[5,0].12
	Let ix.3 = tvx_stav[6,0].9 and !tvx_stav[6,0].12
	Let ix.4 = tvx_stav[7,0].9 and !tvx_stav[7,0].12
	Let ix.5 = tvx_stav[8,0].9 and !tvx_stav[8,0].12
	Let ix.6 = tvx_stav[9,0].9 and !tvx_stav[9,0].12
	Let ix.7 = tvx_stav[10,0].9 and !tvx_stav[10,0].12

	Let @ReqUT = ix.0 or ix.1 or ix.2 or ix.3 or ix.4 or ix.5 or ix.6 or ix.7
	
	Let @VypKotle = @KotStop or Alarm_A1.4

	Interpol TE_reg, fx, EkvPointVZT
	Let WTV_VZT = if(@ReqVZT, fx, 10.0)
	
	//Společná žádaná
	Let WTUV_Max = min(tvx_Tz[0,0] + OffsetTUV,95)
	Let WTV_Max = min(max(tvx_Tz[3,0],tvx_Tz[4,0],tvx_Tz[5,0],tvx_Tz[6,0],tvx_Tz[7,0],tvx_Tz[8,0],tvx_Tz[9,0],tvx_Tz[10,0]) + OffsetTV,95)
	Let WTVSpol = max(WTV_Max,WTUV_Max,WTV_VZT)

	//Kaskáda kotlů
	SyncMark 5, 1, 10, 0, 0, @Poradi, NONE

	Let Ka_PorKotlu[0,0] = @DisK1 or @Kot1_Err
	Let Ka_PorKotlu[1,0] = @DisK2 or @Kot2_Err
	Let Ka_PorKotlu[2,0] = @DisK3 or @Kot3_Err

	CasCon WTVSpol, TKotV, @VypKotle, @Poradi, Ka_Period, WKot_Meze[0,0], WKot_Meze[1,0], Ka_TypKotlu, Ka_PorKotlu, Ka_Rizeni, Ka_VykonKot, Ka_ProvozKot, Ka_Hladina, Ka_Zesileni, Ka_Integrace, Ka_SDK, Ka_PripojInt, Ka_OdpojInt, Ka_IngralAct

	//Přepočet minut na hodiny pro ViewDet
	Let Ka_ProHodiny[0,0] = long(Ka_ProvozKot[0,0]/60)
	Let Ka_ProHodiny[1,0] = long(Ka_ProvozKot[1,0]/60)
	Let Ka_ProHodiny[2,0] = long(Ka_ProvozKot[2,0]/60)
	
]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="ProcAlarm" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Poruchy</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//Příznak restartu
	TimerOn @Restart, tRestart[0,0], ix.0, NONE
	Let @Restart = If(ix.0,false,@Restart)

	//Test analogových snímačů VZT1
	Limits AI0_okamzita[0,0], @TVenkF, @TVenkF, -40.0, 130.0, 5.0, 0x0000
	Limits AI0_okamzita[1,0], @TKotV_F, @TKotV_F, -40.0, 130.0, 5.0, 0x0000
	Limits AI0_okamzita[2,0], @TKotZ_F, @TKotZ_F, -40.0, 130.0, 5.0, 0x0000

	// *****************************************************************************
	//                    PORUCHY Kotelny
	// *****************************************************************************
	Let Alarm_A1.0 = !@STOP_btnOK and !@Restart			//STOP tlačítko
	Let Alarm_A1.1 =  @STOP_btnOK and !@Gas_OK    and !@Restart	//Únik plynu
	Let Alarm_A1.2 =  @Gas_OK     and !@OverVS_OK and !@Restart	//Přehřátí prostoru
	Let Alarm_A1.3 =  @OverVS_OK  and !@LeakVS_OK and !@Restart	//Zaplavení prostoru

	Let ix.0 = TKotV>105.0
	TimerOn ix.0, 30000, Alarm_A1.4, NONE				//Přehřátí kotlů

	Let ix.0 = tvx_stav[0,0].6 or tvx_stav[1,0].6 or tvx_stav[2,0].6
	TimerOn ix.0, 30000, ix.1, NONE
	Let Alarm_A1.5 = !@TempTUV1_OK or ix.1				//Přehřátí TUV

	//Nízký a vysoký tlak ÚT
	Limits PTV1, Alarm_A1.7, Alarm_A1.6, PTV_Meze[0,0], PTV_Meze[1,0], 2.000, 0x0000
	//Nízká a vysoká hladina v nádrži
	Limits LTANK, Alarm_A1.9, Alarm_A1.8, LevelZ[0,0], LevelZ[3,0], 2.000, 0x0000

	Let ix.2 = !@Dopl_OK	  						//Porucha doplňovacího automatu
	TimerOn ix.2, tMaxDopl, Alarm_A1.10, NONE				//pak to bude dlouhé dopňování

	Let Alarm_A1.11 = !@SV_Press_OK					//Nízký tlak SV

	Let Alarm_A1.12 = @Kot1_Err						//Porucha kotle
	Let Alarm_A1.13 = @Kot2_Err						//Porucha kotle
	Let Alarm_A1.14 = @Kot3_Err						//Porucha kotle


	//Kvitace alarmů
	Let Alarm_Q1 = If(pQuit.0, ~(Alarm_I1 | Alarm_A1), Alarm_Q1)
	Let pQuit.0 = false
	
	ErrSig32 Alarm_A1, 0x00000001, Alarm_Q1, 0x00000001, Alarm_E1.0, Alarm_I1.0, 1, 0, 4096, 2000, 0, 0
	ErrSig32 Alarm_A1, 0x00000002, Alarm_Q1, 0x00000002, Alarm_E1.1, Alarm_I1.1, 1, 0, 4096, 2001, 0, 0
	ErrSig32 Alarm_A1, 0x00000004, Alarm_Q1, 0x00000004, Alarm_E1.2, Alarm_I1.2, 1, 0, 4096, 2002, 0, 0
	ErrSig32 Alarm_A1, 0x00000008, Alarm_Q1, 0x00000008, Alarm_E1.3, Alarm_I1.3, 1, 0, 4096, 2003, 0, 0
	ErrSig32 Alarm_A1, 0x00000010, Alarm_Q1, 0x00000010, Alarm_E1.4, Alarm_I1.4, 1, 0, 4096, 2004, 0, 0
	ErrSig32 Alarm_A1, 0x00000020, Alarm_Q1, 0x00000020, Alarm_E1.5, Alarm_I1.5, 1, 0, 4096, 2005, 0, 0
	ErrSig32 Alarm_A1, 0x00000040, Alarm_Q1, 0x00000040, Alarm_E1.6, Alarm_I1.6, 20, 0, 4096, 2006, 0, 0
	ErrSig32 Alarm_A1, 0x00000080, Alarm_Q1, 0x00000080, Alarm_E1.7, Alarm_I1.7, 20, 0, 4096, 2007, 0, 0
	ErrSig32 Alarm_A1, 0x00000100, Alarm_Q1, 0x00000100, Alarm_E1.8, Alarm_I1.8, 20, 0, 4096, 2008, 0, 0
	ErrSig32 Alarm_A1, 0x00000200, Alarm_Q1, 0x00000200, Alarm_E1.9, Alarm_I1.9, 20, 0, 4096, 2009, 0, 0
	ErrSig32 Alarm_A1, 0x00000400, Alarm_Q1, 0x00000400, Alarm_E1.10, Alarm_I1.10, 3, 0, 4096, 2010, 0, 0
	ErrSig32 Alarm_A1, 0x00000800, Alarm_Q1, 0x00000800, Alarm_E1.11, Alarm_I1.11, 5, 0, 4096, 2011, 0, 0
	ErrSig32 Alarm_A1, 0x00001000, Alarm_Q1, 0x00001000, Alarm_E1.12, Alarm_I1.12, 1, 0, 4096, 2012, 0, 0
	ErrSig32 Alarm_A1, 0x00002000, Alarm_Q1, 0x00002000, Alarm_E1.13, Alarm_I1.13, 1, 0, 4096, 2013, 0, 0
	ErrSig32 Alarm_A1, 0x00004000, Alarm_Q1, 0x00004000, Alarm_E1.14, Alarm_I1.14, 1, 0, 4096, 2014, 0, 0
	ErrSig32 Alarm_A1, 0x00008000, Alarm_Q1, 0x00008000, Alarm_E1.15, Alarm_I1.15, 1, 0, 4096, 2015, 0, 0
	ErrSig32 Alarm_A1, 0x00010000, Alarm_Q1, 0x00010000, Alarm_E1.16, Alarm_I1.16, 1, 0, 4096, 2016, 0, 0
	
	//odstav vše
	Hyst2 PTV1, @HavPTV, 50.000, 55.000, 0x0001
	
	// Blok kotelny
	Let @KotStop = Alarm_A1.0 or Alarm_E1.0 or Alarm_A1.1  or Alarm_E1.1 or Alarm_A1.2 or Alarm_A1.3 or @Restart
	Let @HUP = !(Alarm_A1.0 or Alarm_E1.0 or Alarm_A1.1  or Alarm_E1.1)
	
	Let @AlarmSign =  bool(Alarm_E1)
	Let @ErrSig = @AlarmSign]]></Script>
          </Body>
          <Period>1000</Period>
          <Offset>250</Offset>
          <Type>1</Type>
          <InterruptEdge>0</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="ProcArch" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Archivace hodnot</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[// Archivace žádaných a měřených hodnot, perioda 10 minut
SyncMark 2, 10, 0, 0, 0, @SyncArc, NONE

// Kotelna
SyncArch WTVSpol, 1, arc_Kot[0,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch TKotV, 1, arc_Kot[1,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch TKotZ, 1, arc_Kot[2,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch PTV1, 1, arc_Kot[3,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch TVenkS, 1, arc_Kot[4,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000

//TUV
SyncArch tvx_Tz[0,*], 1, arc_TUV[0,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tm[0,*], 3, arc_TUV[1,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000

//ÚT1
SyncArch tvx_Tp[3,*], 1, arc_UT1[0,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tz[3,*], 1, arc_UT1[1,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tm[3,*], 1, arc_UT1[2,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_akce[3,*], 1, arc_UT1[3,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000

//ÚT2
SyncArch tvx_Tp[4,*], 1, arc_UT2[0,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tz[4,*], 1, arc_UT2[1,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tm[4,*], 1, arc_UT2[2,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_akce[4,*], 1, arc_UT2[3,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000

//ÚT3
SyncArch tvx_Tp[5,*], 1, arc_UT3[0,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tz[5,*], 1, arc_UT3[1,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tm[5,*], 1, arc_UT3[2,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_akce[5,*], 1, arc_UT3[3,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000

//ÚT4
SyncArch tvx_Tp[6,*], 1, arc_UT4[0,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tz[6,*], 1, arc_UT4[1,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tm[6,*], 1, arc_UT4[2,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_akce[6,*], 1, arc_UT4[3,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000

//ÚT5
SyncArch tvx_Tp[7,*], 1, arc_UT5[0,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tz[7,*], 1, arc_UT5[1,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tm[7,*], 1, arc_UT5[2,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_akce[7,*], 1, arc_UT5[3,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000

//ÚT6
SyncArch tvx_Tp[8,*], 1, arc_UT6[0,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tz[8,*], 1, arc_UT6[1,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tm[8,*], 1, arc_UT6[2,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_akce[8,*], 1, arc_UT6[3,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000

//ÚT7
SyncArch tvx_Tp[9,*], 1, arc_UT7[0,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tz[9,*], 1, arc_UT7[1,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tm[9,*], 1, arc_UT7[2,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_akce[9,*], 1, arc_UT7[3,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000

//ÚT8
SyncArch tvx_Tp[10,*], 1, arc_UT8[0,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tz[10,*], 1, arc_UT8[1,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_Tm[10,*], 1, arc_UT8[2,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0000
SyncArch tvx_akce[10,*], 1, arc_UT8[3,*], arc_Casy, @SyncArc, @SyncArc, 0, arc_Index, 0x0001

// Archivace hodnot TUV, perioda 6 hodin
GetTime RTC_time, RTC_slozky, NONE
SyncMark 3, 1, 0, 0, 0, @SyncHour, NONE
Let @Sync_6h = if(@SyncHour and ((RTC_slozky[2,0] mod 6)==0), true, false)
SyncArch tvx_Tm[0,*], 3, arc_TUV2[1,*], arc_Casy2, @Sync_6h, @Sync_6h, 0, arc_Index2, 0x0001

// Archivace provozních hodin kotlů, perioda 1 měsíc
SyncMark 6, 1, 0, 0, 0, @SyncMonth, NONE
If @SyncMonth
	Let Ka_ProH_dif[0,0] = Ka_ProHodiny[0,0] - Ka_ProH_old[0,0]
	Let Ka_ProH_dif[1,0] = Ka_ProHodiny[1,0] - Ka_ProH_old[1,0]
	Let Ka_ProH_dif[2,0] = Ka_ProHodiny[2,0] - Ka_ProH_old[2,0]
	MtxCopy Ka_ProH_old, NONE, NONE, Ka_ProHodiny, NONE, NONE, NONE, NONE, NONE.0
Endif
SyncArch Ka_ProH_dif[0,*], 3, arc_HourRun[3,*], arc_Casy3, @SyncMonth, @SyncMonth, 0, arc_Index3, 0x0000
SyncArch Ka_ProHodiny[0,*], 3, arc_HourRun[0,*], arc_Casy3, @SyncMonth, @SyncMonth, 0, arc_Index3, 0x0001
]]></Script>
          </Body>
          <Period>5000</Period>
