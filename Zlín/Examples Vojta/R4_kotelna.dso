	//Přiřazení analogových vstupů
	Let TVenkS 	= AI0[0, 0]
	Let TKotZ  	= AI0[1, 0]
	Let TKotV  	= AI0[2, 0]
	
	Let tvx_Tm[0, 0] = AI0[3, 0]		//TUV1
	Let tvx_Tm[1, 0] = AI0[4, 0]		//TUV2
