        <CodeBlock Name="procCtrl" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Řízení vzduchotechniky</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//******************************************************************************
	//Volba režimu dle venkovní teploty
	//******************************************************************************
	Call libObecne
	
	//******************************************************************************
	//Řízení TUV
	//******************************************************************************
	Let @StopTUV = @KotStop or @HavPTV or !@TempTUV1_OK
	//Časování legionelly
	DayPlan 0x0000, 1, 0x0002, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, Svatky, LegTi, LegVal, Leg
	
	Let @Leg = bool(Leg)
	Let ix.0 = false
	fb_TUV_zas_leg 0, @StopTUV, @Leg, tvx_rezim, tvx_stav, tvx_Tpk, cp_timTUV1, cp_valTUV1, Svatky, tvx_Ameze, tvx_LegDelay, tvx_Tz, tvx_Tm, ix.0, @ReqCTUO1
	Let @TUV1_Bypass = tvx_stav[0, 0].15
	//Všchny ohřívače jedou společně
	Let tvx_rezim[1,0] = tvx_rezim[0,0]
	Let tvx_rezim[2,0] = tvx_rezim[0,0]
	Let tvx_Tpk[1,0] = tvx_Tpk[0,0]
	Let tvx_Tpk[2,0] = tvx_Tpk[0,0]
	fb_TUV_zas_leg 1, @StopTUV, @Leg, tvx_rezim, tvx_stav, tvx_Tpk, cp_timTUV1, cp_valTUV1, Svatky, tvx_Ameze, tvx_LegDelay, tvx_Tz, tvx_Tm, ix.0, @ReqCTUO2
	fb_TUV_zas_leg 2, @StopTUV, @Leg, tvx_rezim, tvx_stav, tvx_Tpk, cp_timTUV1, cp_valTUV1, Svatky, tvx_Ameze, tvx_LegDelay, tvx_Tz, tvx_Tm, ix.0, @ReqCTUO3

	//******************************************************************************
	//Podmínka spuštění čerpadel ohřevu TUV - na rozdělovači musí být dostatečná teplota
	//******************************************************************************
	Let TMinTUV[0, 0] = tvx_Tm[0, 0] + TUVMinOffset
	Let TMinTUV[1, 0] = tvx_Tm[1, 0] + TUVMinOffset
	Let TMinTUV[2, 0] = tvx_Tm[2, 0] + TUVMinOffset

	Hyst TKotV, @TOhrTUV1_OK, TMinTUV[0,0], 1.000, 0x0000
	Hyst TKotV, @TOhrTUV2_OK, TMinTUV[1,0], 1.000, 0x0000
	Hyst TKotV, @TOhrTUV3_OK, TMinTUV[2,0], 1.000, 0x0000

	Let @CTUVoh1 = @ReqCTUO1 and @TOhrTUV1_OK
	Let @CTUVoh2 = @ReqCTUO2 and @TOhrTUV2_OK
	Let @CTUVoh3 = @ReqCTUO3 and @TOhrTUV3_OK

	Let @KlapTUV1 = @CTUVoh1
	Let @KlapTUV2 = @CTUVoh2
	Let @KlapTUV3 = @CTUVoh3

	//******************************************************************************
	//Čerpadlo cirkuace
	//******************************************************************************
	SyncMark 2,1,0,0,0,@Tm_1min,NONE

	Let CirkCnt1 = if((CirkCnt1>=(Cirk1_Tm[0,0]+Cirk1_Tm[1,0])) or (plTUC1<1), 0, CirkCnt1 + int(@Tm_1min))

	Let iZapTUC1 = if(iZapTUC1 > 2,0,iZapTUC1)
	Switch iZapTUC1
		Case 0	//AUT
			DayPlan 0x0000, 8, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0080, Svatky, cp_timTUC1, cp_valTUC1, plTUC1
		EndCase
		Case 1	//VYP
			Let plTUC1 = 0
		EndCase
		Case 2	//ZAP
			Let plTUC1 = 2
		EndCase
	EndSwitch
	
	Let @CTUC = !(@KotStop or Alarm_A1.11 or (tvx_rezim[0,0]==1)) and ((tvx_Tz[0,0]>10.0) and (plTUC1 > 0) or @TUV1_Bypass)

	Let ix.11 = @KotStop or @HavPTV
	fb_TV_smes_konstPr 3, ix.11, @te_xLeto, tvx_KonstMod.3, tvx_rezim, tvx_stav, tvx_Tpk, cp_timTV1, cp_valTV1, Svatky, TE_reg, tvx_EqK, tvx_Ameze, tvx_Tz, tvx_Tp, tvx_Tm, Debug.0, @RVTV1XR, tvx_PIDrezim, tvx_akce, RVTV1_PID, @CTV1
	fb_TV_smes_konstPr 4, ix.11, @te_xLeto, tvx_KonstMod.4, tvx_rezim, tvx_stav, tvx_Tpk, cp_timTV2, cp_valTV2, Svatky, TE_regJ, tvx_EqK, tvx_Ameze, tvx_Tz, tvx_Tp, tvx_Tm, Debug.1, @RVTV2XR, tvx_PIDrezim, tvx_akce, RVTV2_PID, @CTV2
	fb_TV_smes_konstPr 5, ix.11, @te_xLeto, tvx_KonstMod.5, tvx_rezim, tvx_stav, tvx_Tpk, cp_timTV3, cp_valTV3, Svatky, TE_regJ, tvx_EqK, tvx_Ameze, tvx_Tz, tvx_Tp, tvx_Tm, Debug.2, @RVTV3XR, tvx_PIDrezim, tvx_akce, RVTV3_PID, @CTV3
	fb_TV_smes_konstPr 6, ix.11, @te_xLeto, tvx_KonstMod.6, tvx_rezim, tvx_stav, tvx_Tpk, cp_timTV4, cp_valTV4, Svatky, TE_regJ, tvx_EqK, tvx_Ameze, tvx_Tz, tvx_Tp, tvx_Tm, Debug.3, @RVTV4XR, tvx_PIDrezim, tvx_akce, RVTV4_PID, @CTV4
	fb_TV_smes_konstPr 7, ix.11, @te_xLeto, tvx_KonstMod.7, tvx_rezim, tvx_stav, tvx_Tpk, cp_timTV5, cp_valTV5, Svatky, TE_reg, tvx_EqK, tvx_Ameze, tvx_Tz, tvx_Tp, tvx_Tm, Debug.4, @RVTV5XR, tvx_PIDrezim, tvx_akce, RVTV5_PID, @CTV5
	fb_TV_smes_konstPr 8, ix.11, @te_xLeto, tvx_KonstMod.8, tvx_rezim, tvx_stav, tvx_Tpk, cp_timTV6, cp_valTV6, Svatky, TE_regJ, tvx_EqK, tvx_Ameze, tvx_Tz, tvx_Tp, tvx_Tm, Debug.5, @RVTV6XR, tvx_PIDrezim, tvx_akce, RVTV6_PID, @CTV6
	fb_TV_smes_konstPr 9, ix.11, @te_xLeto, tvx_KonstMod.9, tvx_rezim, tvx_stav, tvx_Tpk, cp_timTV7, cp_valTV7, Svatky, TE_reg, tvx_EqK, tvx_Ameze, tvx_Tz, tvx_Tp, tvx_Tm, Debug.6, @RVTV7XR, tvx_PIDrezim, tvx_akce, RVTV7_PID, @CTV7
	fb_TV_smes_konstPr 10,ix.11, @te_xLeto, tvx_KonstMod.10,tvx_rezim, tvx_stav, tvx_Tpk, cp_timTV8, cp_valTV8, Svatky, TE_reg, tvx_EqK, tvx_Ameze, tvx_Tz, tvx_Tp, tvx_Tm, Debug.7, @RVTV8XR, tvx_PIDrezim, tvx_akce, RVTV8_PID, @CTV8


	//******************************************************************************
	//Ovládání kotlů
	//******************************************************************************
	Let @K1_st1 = Ka_Rizeni[0,0].0 and !@KotStop
	Let @K1_st2 = Ka_Rizeni[0,0].1 and !@KotStop
	Let @K2_st1 = Ka_Rizeni[1,0].0 and !@KotStop
	Let @K2_st2 = Ka_Rizeni[1,0].1 and !@KotStop
	Let @K3_st1 = Ka_Rizeni[2,0].0 and !@KotStop
	Let @K3_st2 = Ka_Rizeni[2,0].1 and !@KotStop

	//Protáčení čerpadla K1
	Let ix.0 = not @CKot1
	TimerOn ix.0, tProtoc[0,0], ix.1, NONE
	TimerPuls ix.1, tProtoc[1,0], ix.2, NONE
	
	//Ovládání čerpadla K1 s doběhem
	Let ix.3 = (@K1_st1 or @K1_st2)
	TimerOff ix.3, tCerpDobeh, ix.4, NONE
	Let @CKot1 = ((ix.2 or ix.4) and !@KotStop) and (PTV1 > 50)
	
	//Protáčení čerpadla K2
	Let ix.0 = not @CKot2
	TimerOn ix.0, tProtoc[0,0], ix.1, NONE
	TimerPuls ix.1, tProtoc[1,0], ix.2, NONE
	
	//Ovládání čerpadla K2 s doběhem
	Let ix.3 = (@K2_st1 or @K2_st2)
	TimerOff ix.3, tCerpDobeh, ix.4, NONE
	Let @CKot2 = ((ix.2 or ix.4) and !@KotStop) and (PTV1 > 50)
	
	//Protáčení čerpadla K3
	Let ix.0 = not @CKot3
	TimerOn ix.0, tProtoc[0,0], ix.1, NONE
	TimerPuls ix.1, tProtoc[1,0], ix.2, NONE
	
	//Ovládání čerpadla K3 s doběhem
	Let ix.3 = (@K3_st1 or @K3_st2)
	TimerOff ix.3, tCerpDobeh, ix.4, NONE
	Let @CKot3 = ((ix.2 or ix.4) and !@KotStop) and (PTV1 > 50)
	
	//******************************************************************************
	//Požadavky na chod VZT
	//******************************************************************************
	VarWStat ReqR1Heat, @R1_Written, 0
	TimerOff @R1_Written, tRemTOut, @R1_CommOK, NONE

	VarWStat ReqR2Heat, @R2_Written, 0
	TimerOff @R2_Written, tRemTOut, @R2_CommOK, NONE
	
	//Test přijaté hodnoty venkovní teploty
	Let TvenkJ = ReqR2Heat[1,0]/10.0
	Limits TVenkJ, @TVenkJF, @TVenkJF, -40.0, 130.0, 5.0, 0x0000

	Hyst TE_reg, @TvenkLow, 10.000, 1.000, 0x0001

	// hrana požadavku nahazuje flag
	Let ix.0 = @R1_CommOK and @R2_CommOK
	If ix.0
		//Komunikace OK, sledujeme požadavky
		Let @ReqVZT = ((ReqR1Heat & 0x0007)!=0) or ((ReqR2Heat[0,0] & 0x0001)!=0)
	Else
		//Komunikace s R1 nejede, při nízké venkovce pojede CTVspol furt
		Let @ReqVZT = @TvenkLow
	EndIf

	Let ix.2 = !@CTV_VZT
	TimerOn ix.2, tProtoc[0,0], ix.3, NONE
	TimerPuls ix.3, tProtoc[1,0], ix.4, NONE		//Protáčení čerpadla
	Let @CTV_VZT = (@ReqVZT or ix.4) and !@KotStop

]]></Script>
          </Body>
          <Period>1000</Period>
          <Offset>300</Offset>
          <Type>4</Type>
          <InterruptEdge>1</InterruptEdge>
