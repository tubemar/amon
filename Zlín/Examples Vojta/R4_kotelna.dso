          <Script><![CDATA[event S_06_08_07_OnOpen()
  S_06_08_07.FocusFirstControl();
  //Přepsaní hodnot v obrazovce
  NumericView1.Value = Memory1.Value;
  NumericUpDn1.Row = Memory1.Value;
  NumericUpDn2.Row = Memory1.Value;
  NumericUpDn3.Row = Memory1.Value;
end;

event Key1_OnKeyPress()
// klávesa Down
  if (Memory1.Value < 7) then
    Memory1.Value = Memory1.Value + 1;
  else
    Memory1.Value = 0;
  endif;
  //Přepsaní hodnot v obrazovce
  NumericView1.Value = Memory1.Value;
  NumericUpDn1.Row = Memory1.Value;
  NumericUpDn2.Row = Memory1.Value;
  NumericUpDn3.Row = Memory1.Value;
end;

event Key2_OnKeyPress()
// klávesa Up
  if (Memory1.Value > 0) then
    Memory1.Value = Memory1.Value - 1;
  else
    Memory1.Value = 7;
  endif;
  //Přepsaní hodnot v obrazovce
  NumericView1.Value = Memory1.Value;
  NumericUpDn1.Row = Memory1.Value;
