        <CodeBlock Name="fb_TV_smes_konstPr" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>Topná větev směšovaná</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[// Verze 1.02
// 2009-10-13
// Požadavek na maximální teplotu se předává pouze PID regulátoru,
// do parametru oTtvz se zapisuje vypočítaná ekvitermní teplota.
// Verze 1.01
// 2009-07-09
// Místo požadavku na maximální otevření ventilu požadavek na maximální teplotu topné vody.
// 2009-07-08
// Úprava protáčení pohonů. Nejprve se otevře a zavře ventil, teprve pak se pouští čerpadlo.
// Úprava určení žádané hodnoty topné vody při vypnutí.
// 2009-07-02
// Meze teploty topné vody jako parametr.

// Vstupně výstupní parametry do vnitřních proměnných
Let tvStav = ioStav[Index, 0]
Let RV_rezim = ioRV_rezim[Index, 0]

// Režim topné větve
Let tvRezimBin = Int(Pow(2, iRezim[Index, 0]))
// V případě odstavení Vyp, jinak podle povelu
// Default režim Aut
Let @xVyp =   (@povVyp and !@povZap and !@povAut) or  @iStop
Let @xZap =   !@povVyp and  @povZap and !@povAut and !@iStop
Let @xAut = ((!@povVyp and !@povZap and  @povAut) or (!@xVyp and !@xZap)) and !@iStop

// Věrohodnost měření teploty topné vody
Let @Ttv_F = (iTtvm[Index, 0] < -30) or (iTtvm[Index, 0] > 130)
// Havarijní meze teploty topné vody
Let @Ttv_LL = ((iTtvm[Index, 0] < iTtv_meze[Index, 0]) or @Ttv_LL) and !((iTtvm[Index, 0] > (iTtv_meze[Index, 0] + Ttv_hyst)) or @Ttv_F)
Let @Ttv_HH = ((iTtvm[Index, 0] > iTtv_meze[Index, 3]) or @Ttv_HH) and !((iTtvm[Index, 0] < (iTtv_meze[Index, 3] - Ttv_hyst)) or @Ttv_F)

// Požadavek na maximální teplotu topné větve
Let @Ttv_max = (@iTtv_max or @Ttv_LL) and !@iStop

// Letní režim topné větve
Let @xLeto = @iLeto and @xAut and !@Ttv_max

// Žádaná teplota prostoru
If @xAut, :NONE
	DayPlan 0x0000, 8, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0080, iSvatky, iCP_casy, iCP_tepl, Tpz
Else :NONE
	Let Tpz = iTpz[Index, 0]
EndIf
Let oTpz[Index, 0] = Tpz

// Žádaná teplota topné vody
If @xVyp, :NONE
	// Při vypnutí regulace na nezámrznou teplotu
	Let oTtvz[Index, 0] = if(@iStop, oTtvz[Index, 0], iTtv_meze[Index, 0] + Ttv_hyst)
Else :NONE
	// V provozu regulace na ekvitermní teplotu
	// Korekce ekvitermní konstanty podle venkovní teploty
	Interpol iTE, EqK_korkoef, EqK_korekce
	Let fx = iEqK[Index, 0] + EqK_korkoef
	Let EqK = if(fx < 0.0, 0.0, fx)
	// Výpočet žádané teploty topné vody
	Let ix.10 = @iKonstTV and @xZap
	If ix.10
		Let fx = iTpz[Index, 1]
	Else
		Let fx = ((1.0 + EqK) * Tpz) - (EqK * iTE)
	EndIf
	Let oTtvz[Index, 0] = if(fx < iTtv_meze[Index, 1], iTtv_meze[Index, 1], if(fx > iTtv_meze[Index, 2], iTtv_meze[Index, 2], fx))
EndIf

// Protáčení pohonů
// Letní protáčení
SyncMark 5, 1, 32, 10, 0, ix.0, NONE
// 1. otevření ventilu
TimerOn @pLprotoc1, 180000, ix.1, NONE
Let @pLprotoc1 = (ix.0 or @pLprotoc1) and !(ix.1 or @iStop or @iRV_ruc or !@xLeto)
// 2. uzavření ventilu
TimerOn @pLprotoc2, 180000, ix.2, NONE
Let @pLprotoc2 = (ix.1 or @pLprotoc2) and !(ix.2 or @iStop or @iRV_ruc or !@xLeto)
// Protočení čerpadla
SyncMark 3, 1, 0, 0, 0, ix.12, NONE
TimerOn @pCprotoc, tCerpProtoc, ix.3, NONE
Let @pCprotoc = ((ix.2 and @xLeto) or (ix.12 and !@xLeto) or @pCprotoc) and !(ix.3 or @iStop)

// Ruční režim regulátoru
Let RV_rezim.1 = @iRV_ruc or @iStop or @xLeto or @Ttv_HH or @Ttv_F

// Výpočet akčního zásahu
Let Ttvz = if(@Ttv_max, iTtv_meze[Index, 2], oTtvz[Index, 0])
Let Ttvm = iTtvm[Index, 0]
Let fx = ioRV_akce[Index, 0]
PID Ttvz, Ttvm, fx, RV_rezim, iRV_PID

// Akční zásah v ručním režimu
If RV_rezim.1, :NONE
	If @iRV_ruc, :NONE
		// V ručním režimu externě zapsaný zásah
	Else :NONE
		Let ix.0 = @iStop or @Ttv_HH or (@xLeto and !@pLprotoc1)
		If ix.0, :NONE
			// Při vypnutí, vysoké teplotě a v létě mimo protočení zavřeno
			Let ioRV_akce[Index, 0] = 0.0
		Else :NONE
			Let ix.1 = (@Ttv_F and @pCprotoc) or (@xLeto and @pLprotoc1)
			If ix.1, :NONE
				// Při protočeních otevřeno
				Let ioRV_akce[Index, 0] = 100.0
			Else :NONE
				// Při poruše snímače náhradní otevření, jinak zavřeno
				Let ioRV_akce[Index, 0] = if(@Ttv_F, RV_tpor, 0.0)
			EndIf
		EndIf
	EndIf
Else :NONE
	// Pokud není ruční režim, akční zásah určený modulem PID
 	Let ioRV_akce[Index, 0] = fx
EndIf

// Řízení čerpadla
// Poloha ventilu pro spuštění čerpadla
Let fx = 0.5 * RV_cerp[1, 0]
Let @RV_AnC = ((ioRV_akce[Index, 0] > (RV_cerp[0, 0] + fx)) or @RV_AnC) and not ((ioRV_akce[Index, 0] < (RV_cerp[0, 0] - fx)) or @iRV_ruc)
// Doběh čerpadla
TimerOff @RV_AnC, tCerpDobeh, @RV_AnC_tof, NONE
// Zapnutí čerpadla
Let @oCerpZap = ((@RV_AnC_tof and !@xLeto and !@Ttv_HH) or @pCprotoc) and !@iStop

// Zápis do vstupně výstupních parametrů
Let ioStav[Index, 0] = tvStav
Let ioRV_rezim[Index, 0] = RV_rezim
]]></Script>
