	  EndIf
         //Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
       Else :NONE
         AnIn #AI00_7, AI0_okamzita[7,0], AI0_params[7,0], AI0_params[7,1], AI0_params[7,2], AI0_params[7,3], AI0_params[7,4]
       EndIf
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AI0_konst[7, 0]*AI0_okamzita[7, 0]+AI0_konst[7, 1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AI0[7,0], AI0_konst[7,2]
	
	//Přepnutí kanálu AO0 na AUTOMATICKÝ nebo MANUÁLNÍ režim
	For Ix, 0.000, 7.000, 1.000
	  Let  AI0[Ix,0] = IF(AI0_XR.(Ix),AI0_R[Ix,0],AI0[Ix,0]); AI0_R[Ix,0] = AI0[Ix,0]
	EndFor
     

	//Načtení NAPÁJECÍho NAPĚTÍ
	AnIn #PWR2_0, Ux_A[0,0], 56.000, 0.000, 56.000, 0.7, 55.000
	//Přepnutí AUTOMATICKÝ nebo MANUÁLNÍ režim
	Let  Ux[0,0] = IF(Ux_XR.0,Ux_R[0,0],Ux_A[0,0]); Ux_R[0,0] = Ux[0,0]
	
	//Načtení NAPĚTÍ BATERIE 
	AnIn #PWR2_1, Ux_A[1,0], 5.000, 0.000, 5.000, 0.000, 5.000
	//Přepnutí AUTOMATICKÝ nebo MANUÁLNÍ režim
	Let Ux[1,0] = IF(Ux_XR.1,Ux_R[1,0],Ux_A[1,0]); Ux_R[1,0] = Ux[1,0]]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="libAO" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Zápis na analogové výstupy</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//Analogový kanál AO0
	
	//Přepnutí AUTOMATICKÝ nebo MANUÁLNÍ režim
	For Ix, 0.000, 3.000, 1.000
	  Let  AO[Ix,0] = IF(AO_XR.(Ix),AO_R[Ix,0],AO_A[Ix,0])
	EndFor
	
	//Beznárazové přepnutí na manuální režim
	MtxCopy AO_R, NONE, NONE, AO, NONE, NONE, NONE, NONE, NONE.0
	
	
	//******************* Zápis na analogové výstupy *******************
	AnOut #AO00_0, AO[0,0], AO_params[0,0], AO_params[0,1], AO_params[0,2], AO_params[0,3], AO_params[0,4]
	AnOut #AO00_1, AO[1,0], AO_params[1,0], AO_params[1,1], AO_params[1,2], AO_params[1,3], AO_params[1,4]
	AnOut #AO00_2, AO[2,0], AO_params[2,0], AO_params[2,1], AO_params[2,2], AO_params[2,3], AO_params[2,4]
	AnOut #AO00_3, AO[3,0], AO_params[3,0], AO_params[3,1], AO_params[3,2], AO_params[3,3], AO_params[3,4]
]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="libDI" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Zpracování digitálních vstupů</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//Digitální vstupy 
	
	//*************************************************
	//******************* KANÁL DI0 *******************
	//*************************************************
	
	//***************** Načtení DI0 v režimu stejnosměrné *****************
	DigIn #0, DI0_DC, 0x0000
	//******************* Načtení DI0 v režimu střídavé *******************
	DigIn #1, DI0_AC, 0x0000
	//Volba zda se bude vstup v aplikaci zpracovávat jako ss. nobo jako stř. 
	Let DI0_A = (DI0_DC & ~DI0_Typ)|(DI0_AC & DI0_Typ)
	//Přepnutí AUTOMATICKÝ nebo MANUÁLNÍ režim + volba negace
	Let DI0 = (~DI0_XR & (DI0_A ^ DI0_neg)) | (DI0_XR  & DI0_R)
	//Beznárazové přepnuti do ručního režimu
	Let DI0_R = DI0
	
	
]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="libDM_DI8_1" Class="Amit.Psp4.Shared.CodeProcedure">
