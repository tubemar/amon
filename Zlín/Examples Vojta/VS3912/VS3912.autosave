        <CodeBlock Name="Cerpadla" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Řízení čerpadel</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[
	Let ix.0 = @ChodTV1di and !Alarm_E1.15 and !Alarm_E1.14 and !Alarm_E1.1 and (PTV1 > 50)
	
	If !@ZpozdStart
		//po restartu zvolit letní čerpadlo
		Let @SelCTV_A = true
	Endif
	
	Let @OptCTV_Man = @VS_ManSelC
	If @OptCTV_Man
		Let @SelCTV_A = @SelCTV_M
	Else
		Let ix.14 = (YCTV1 < CTV_MinMax[0,0]) and !ix.12			//Fláká se zimní čerpadlo?
		TimerOn ix.14, tCTV_MinMax[0,0], ix.13, tCTV_MinMax[1,0]	//Trvá požadavavek dost dlouho?
	
		Let ix.11 = (YCTV2 > CTV_MinMax[1,0]) and !ix.9			//Nestíhá letní čerpadlo?
		TimerOn ix.11, tCTV_MinMax[0,1], ix.10, tCTV_MinMax[1,1]	//Trvá požadavavek dost dlouho?
	
		If (ix.9 or ix.12)
		Else
			If (ix.13 and !@SelCTV_A)
				Let @SelCTV_A = true
			Endif
			If (ix.10 and @SelCTV_A)
				Let @SelCTV_A = false
			Endif
		Endif
		TimerPuls ix.13, tCTV_MinMax[0,2], ix.12, NONE			//Počkat po změně na ustálení
		TimerPuls ix.10, tCTV_MinMax[0,2], ix.9, tCTV_MinMax[1,2]	//Počkat po změně na ustálení
	Endif

	Let @CTV1 = ix.0 and !@SelCTV_A
	Let @CTV2 = ix.0 and  @SelCTV_A
	
	Let RePIDCTV1.1 = !@CTV1
	Let YCTV1 = If (RePIDCTV1.1, 0.0, YCTV1) 
	
	Let RePIDCTV2.1 = !@CTV2
	Let YCTV2 = If (RePIDCTV2.1, 0.0, YCTV2) 
	
	Let @dPTVOptMan = (RemdP1==-9999) or @VS_Man_dP
	If ((WdPTV1_reg==0) or @dPTVOptMan)
		Let WdPTV1_reg=WdPTV1_man
	Else
		Let WdPTV1_reg=WdPTV1_reg+(WdPTV1-RemdP1/10.0)*IKonst1
	Endif
	Limiter WdPTV1_reg, WdPTV_meze[0,0], WdPTV_meze[1,0]
	
	PID WdPTV1_reg,dPTV1,YCTV1,RePIDCTV1,PaPIDCTV1
	PID WdPTV1_reg,dPTV1,YCTV2,RePIDCTV2,PaPIDCTV2
]]></Script>
          </Body>
        </CodeBlock>
