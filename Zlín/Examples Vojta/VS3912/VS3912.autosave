end;

event DateTimeEdit1_OnEditComplete()
	UtlTiKask[LCD_row,LCD_col] = UtlTiAct;
end;

event CaseLabelUpDn1_OnEditComplete()
	UtlKask[LCD_row,LCD_col] = CaseLabelUpDn1.Value;
end;

event Key1_OnKeyPress()								//posun o polozku planu vpred
	if LCD_col>=7 then
		LCD_col=0;
	else
		LCD_col=LCD_col+1;
	endif;
	UtlTiAct = UtlTiKask[LCD_row,LCD_col];
	CaseLabelUpDn1.Value = UtlKask[LCD_row,LCD_col];
	NumericView1.Value = LCD_col+1;
end;

event Key2_OnKeyPress()								//posun o polozku planu vzad
	if LCD_col<=0 then
		LCD_col=7;
	else
		LCD_col=LCD_col-1;
	endif;
	UtlTiAct = UtlTiKask[LCD_row,LCD_col];
	CaseLabelUpDn1.Value = UtlKask[LCD_row,LCD_col];
	NumericView1.Value = LCD_col+1;
end;

event Key4_OnKeyPress()								//posun o den v planu vzad
	if LCD_row<=0 then
		LCD_row=1;
	else
		LCD_row=LCD_row-1;
	endif;
	UtlTiAct = UtlTiKask[LCD_row,LCD_col];
	CaseLabelUpDn1.Value = UtlKask[LCD_row,LCD_col];
	CaseLabelView1.Value = LCD_row;
end;

event Key3_OnKeyPress()								//posun o den v planu vpred
	if LCD_row>=1 then
		LCD_row=0;
	else
		LCD_row=LCD_row+1;
	endif;
	UtlTiAct = UtlTiKask[LCD_row,LCD_col];
	CaseLabelUpDn1.Value = UtlKask[LCD_row,LCD_col];
	CaseLabelView1.Value = LCD_row;
end;]]></Script>
