        <CodeBlock Name="ProcBuffData" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Hlavní proces</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Optimalizace výpočty
Call libSTS_TUV
//Čtení každé 2s
SyncMark 1,2,0,0,0,@SyncRd,NONE
If @SyncRd
	Let @RdReq = true
	Let @RdReqdP1 = true
	Let cnt = if(cnt>8, 0, cnt+1)

	//OPS 4093 - stanice se vzdáleným měřením dP měříme často
	If RdStat5.0
	Else
		If @RdReqdP1
			ReqDbDir 0x0000, 15, NONE, 4934, NONE, 3, NONE, 0, NONE, 0, NONE, 10, NONE, 1, NONE, OPS_Data, 0, NONE, 5, NONE, NONE, RdStat5
			Let @RdReqdP1 = false
		Endif
	Endif	
	//Při poruše komunikace posílám -9999
	Let RemdP1 = if(RdStat5.1, OPS_Data[4,5],-9999)	

	//Data pro VS
	Let MBS_Data[0,8] = RemdP1
Endif

Switch cnt
	//výpočet z načtených dat
	Case 0
		//pouze jeden průchod smyčky
		If @RdReq
			Let maxWUT = 0
			Let maxWTUV= 0
			Let maxValveUT= 0
			Let maxValveTUV=0
			Let minValveUT= 9999
			Let minValveTUV=9999
			
			Let ComCntOK= 0			
			For mtx_cnt, 0, 8, 1
				//Chod regulace ÚT
				Let @ChodUT  = OPS_Data[9,mtx_cnt].2
				Let ComCntOK= if(ComState.(mtx_cnt),ComCntOK+1,0)
				//Maxima
				Let maxWUT     = if(OPS_Data[0,mtx_cnt]*int(@ChodUT)>maxWUT, if(ComState.(mtx_cnt),OPS_Data[0,mtx_cnt],0), maxWUT)
				Let maxWTUV    = if(OPS_Data[1,mtx_cnt]>maxWTUV, if(ComState.(mtx_cnt),OPS_Data[1,mtx_cnt],0), maxWTUV)
				Let maxValveUT = if(OPS_Data[2,mtx_cnt]>maxValveUT, if(ComState.(mtx_cnt),OPS_Data[2,mtx_cnt],0), maxValveUT)
				Let maxValveTUV= if(OPS_Data[3,mtx_cnt]>maxValveTUV, if(ComState.(mtx_cnt),OPS_Data[3,mtx_cnt],0), maxValveTUV)
				//Minima
				Let minValveUT = if(OPS_Data[2,mtx_cnt]<minValveUT, if(ComState.(mtx_cnt),OPS_Data[2,mtx_cnt],0), minValveUT)
				Let minValveTUV= if(OPS_Data[3,mtx_cnt]<minValveTUV, if(ComState.(mtx_cnt),OPS_Data[3,mtx_cnt],0), minValveTUV)
			EndFor
			Let @RdReq = false

			//Data pro VS
			Let MBS_Data[0,0] = maxWUT
			Let MBS_Data[0,1] = maxWTUV
			Let MBS_Data[0,2] = maxValveUT
			Let MBS_Data[0,3] = minValveUT
			Let MBS_Data[0,4] = maxValveTUV
			Let MBS_Data[0,5] = minValveTUV
			Let MBS_Data[0,6] = int(ComState & 0xFFFF)
			Let MBS_Data[0,7] = 0
		Endif
		
	EndCase
	//OPS 3891
	Case 1
		If RdStat0.0
		Else
			If @RdReq
				ReqDbDir 0x0000, 7, NONE, 4934, NONE, 3, NONE, 0, NONE, 0, NONE, 10, NONE, 1, NONE, OPS_Data, 0, NONE, 0, NONE, NONE, RdStat0
				Let @RdReq = false
			Endif
		Endif	
	EndCase

	//OPS 3893
	Case 2
		If RdStat1.0
		Else
			If @RdReq
				ReqDbDir 0x0000, 8, NONE, 4934, NONE, 3, NONE, 0, NONE, 0, NONE, 10, NONE, 1, NONE, OPS_Data, 0, NONE, 1, NONE, NONE, RdStat1
				Let @RdReq = false
			Endif
		Endif	
	EndCase

	//OPS 3897
	Case 3
		If RdStat2.0
		Else
			If @RdReq
				ReqDbDir 0x0000, 9, NONE, 4934, NONE, 3, NONE, 0, NONE, 0, NONE, 10, NONE, 1, NONE, OPS_Data, 0, NONE, 2, NONE, NONE, RdStat2
				Let @RdReq = false
			Endif
		Endif	
	EndCase

	//OPS 3899
	Case 4
		If RdStat3.0
		Else
			If @RdReq
				ReqDbDir 0x0000, 10, NONE, 4934, NONE, 3, NONE, 0, NONE, 0, NONE, 10, NONE, 1, NONE, OPS_Data, 0, NONE, 3, NONE, NONE, RdStat3
				Let @RdReq = false
			Endif
		Endif	
	EndCase

	//OPS 3912
	Case 5
		If RdStat4.0
		Else
			If @RdReq
				ReqDbDir 0x0000, 11, NONE, 4934, NONE, 3, NONE, 0, NONE, 0, NONE, 10, NONE, 1, NONE, OPS_Data, 0, NONE, 4, NONE, NONE, RdStat4
				Let @RdReq = false
			Endif
		Endif	
	EndCase

	//OPS 4093
	Case 6
	EndCase

	//OPS 4094
	Case 7
		If RdStat6.0
		Else
			If @RdReq
				ReqDbDir 0x0000, 16, NONE, 4934, NONE, 3, NONE, 0, NONE, 0, NONE, 10, NONE, 1, NONE, OPS_Data, 0, NONE, 6, NONE, NONE, RdStat6
				Let @RdReq = false
			Endif
		Endif	
	EndCase

	//OPS 4095
	Case 8
		If RdStat7.0
		Else
			If @RdReq
				ReqDbDir 0x0000, 17, NONE, 4934, NONE, 3, NONE, 0, NONE, 0, NONE, 10, NONE, 1, NONE, OPS_Data, 0, NONE, 7, NONE, NONE, RdStat7
				Let @RdReq = false
			Endif
		Endif	
	EndCase

	//OPS 4096
	Case 9
		If RdStat8.0
		Else
			If @RdReq
				ReqDbDir 0x0000, 18, NONE, 4934, NONE, 3, NONE, 0, NONE, 0, NONE, 10, NONE, 1, NONE, OPS_Data, 0, NONE, 8, NONE, NONE, RdStat8
				Let @RdReq = false
			Endif
		Endif	
	EndCase

EndSwitch

//Stavy komunikace
Let ComState.0 = RdStat0.1
Let ComState.1 = RdStat1.1
Let ComState.2 = RdStat2.1
Let ComState.3 = RdStat3.1
Let ComState.4 = RdStat4.1
Let ComState.5 = RdStat5.1
Let ComState.6 = RdStat6.1
Let ComState.7 = RdStat7.1
Let ComState.8 = RdStat8.1
]]></Script>
          </Body>
          <Period>500</Period>
          <Offset>150</Offset>
          <Type>10</Type>
          <InterruptEdge>1</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="ProcCtrl" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Rizeni</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Protaceni cerpadel
DayPlan 0x0000, 1, 0x0001, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, Holidays, ProTVTi, ProTVVal, ProTV

Call Kaskada								//Kaskada PPO
Call RegPPO								//Regulace PPO1
Call Cerpadla								//Ovládání čerpadel
Call Doplnovani

Valve YHV, PrebehHV, PolVHV, @zavHV, @otvHV
