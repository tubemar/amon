          <Script><![CDATA[event Plan1_OnOpen()
	Plan1.FocusFirstControl();
	LCD_row = 0;
	LCD_col = 0;
	UtlTiAct = UtlTiTV1[0,0];
	NumericView1.Value = LCD_col+1;
	CaseLabelView1.Value = LCD_row;
	NumericEdit1.Value = UtlTV1[LCD_row,LCD_col];
end;

event DateTimeEdit1_OnEditComplete()
	UtlTiTV1[LCD_row,LCD_col] = UtlTiAct;
end;

event NumericEdit1_OnEditComplete()
	UtlTV1[LCD_row,LCD_col] = NumericEdit1.Value;
end;

event Key1_OnKeyPress()								//posun o polozku planu vpred
	if LCD_col>=7 then
		LCD_col=0;
	else
		LCD_col=LCD_col+1;
	endif;
	UtlTiAct = UtlTiTV1[LCD_row,LCD_col];
	NumericEdit1.Value = UtlTV1[LCD_row,LCD_col];
	NumericView1.Value = LCD_col+1;
end;

event Key2_OnKeyPress()								//posun o polozku planu vzad
	if LCD_col<=0 then
		LCD_col=7;
	else
		LCD_col=LCD_col-1;
	endif;
	UtlTiAct = UtlTiTV1[LCD_row,LCD_col];
	NumericEdit1.Value = UtlTV1[LCD_row,LCD_col];
	NumericView1.Value = LCD_col+1;
end;

event Key4_OnKeyPress()								//posun o den v planu vzad
	if LCD_row<=0 then
		LCD_row=1;
	else
		LCD_row=LCD_row-1;
	endif;
	UtlTiAct = UtlTiTV1[LCD_row,LCD_col];
	NumericEdit1.Value = UtlTV1[LCD_row,LCD_col];
	CaseLabelView1.Value = LCD_row;
end;

event Key3_OnKeyPress()								//posun o den v planu vpred
	if LCD_row>=1 then
		LCD_row=0;
	else
		LCD_row=LCD_row+1;
	endif;
	UtlTiAct = UtlTiTV1[LCD_row,LCD_col];
	NumericEdit1.Value = UtlTV1[LCD_row,LCD_col];
	CaseLabelView1.Value = LCD_row;
end;]]></Script>
