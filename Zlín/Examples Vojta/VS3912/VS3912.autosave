	Let Read[0, 4]  = int(10*TTVZ1)
	Let Read[0, 5]  = int(10*PHVVred)
	Let Read[0, 6]  = int(10*TTUV1)
	Let Read[0, 7]  = int(10*TTUC1)
	Let Read[0, 8]  = int(10*PTVZ)
	Let Read[0, 9]  = int(10*dPHV1)
	Let Read[0, 10] = int(10*TPPO1)
	Let Read[0, 11] = int(10*TPPO2)
	Let Read[0, 12] = int(10*PHVV)
	Let Read[0, 13] = int(10*PTV1)
	Let Read[0, 14] = int(10*dPTV1)
	Let Read[0, 15] = int(10*PTVV)
	Let Read[0, 16] = int(10*TPPO3)
	Let Read[0, 17] = int(10*PHVZ)
	Let Read[0, 18] = int(10*TVenkLoc1)
	Let Read[0, 19] = int(10*TVenk1)
	Let Read[0, 20] = int(10*WTV1)
	Let Read[0, 21] = int(10*WTV2)
	Let Read[0, 22] = int(WTUVuprdi1*10)
	Let Read[0, 23] = int(10*WTV3)
	Let Read[0, 24] = RemdP1
	//REM ""
	Let Read[0, 25] = DI0
	Let Read[0, 26] = DI1
