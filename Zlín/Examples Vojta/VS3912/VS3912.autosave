ErrSig Alarm_A1, 0x0100, Alarm_Q1, 0x0100, Alarm_E1.8, Alarm_Ig.0, 10, 4, 1010, 39, 0, 0      	//Tlak SV
ErrSig Alarm_A1, 0x0200, Alarm_Q1, 0x0200, Alarm_E1.9, Alarm_Ig.0, 10, 4, 1010, 43, 0, 0     	//Níz. tlak TV
ErrSig Alarm_A1, 0x0400, Alarm_Q1, 0x0400, Alarm_E1.10, Alarm_Ig.0, 10, 4, 1010, 44, 0, 0       	//Vys. tlak TV
ErrSig Alarm_A1, 0x0800, Alarm_Q1, 0x0800, Alarm_E1.11, Alarm_Ig.0, 900, 4, 1010, 45, 0, 0       	//Dlouhé doplň.
ErrSig Alarm_A1, 0x1000, Alarm_Q1, 0x1000, Alarm_E1.12, Alarm_Ig.0, 10, 4, 1010, 52, 0, 0      	//Nízká hladina v nádrži
ErrSig Alarm_A1, 0x2000, Alarm_Q1, 0x2000, Alarm_E1.13, Alarm_Ig.0, 120, 4, 1010, 21, 0, 0       	//Prehrati TV termostat
ErrSig Alarm_A1, 0x4000, Alarm_Q1, 0x4000, Alarm_E1.14, Alarm_Ig.0, 2, 4, 1010, 3, 0, 0       	//Zaplaveni VS
ErrSig Alarm_A1, 0x8000, Alarm_Q1, 0x8000, Alarm_E1.15, Alarm_Ig.0, 1, 1, 1010, 1, 0, 0       	//STOP tlacitko

Let ErrOr = bool(Alarm_E1 | Alarm_E2)	//suma poruch pro LCD

//Autokvitace poruch
Let Alarm_AQ1.0  = Alarm_E1.0  and not Alarm_A1.0
Let Alarm_AQ1.1  = Alarm_E1.1  and not Alarm_A1.1
Let Alarm_AQ1.2  = Alarm_E1.2  and not Alarm_A1.2
Let Alarm_AQ1.3  = Alarm_E1.3  and not Alarm_A1.3
Let Alarm_AQ1.4  = Alarm_E1.4  and not Alarm_A1.4
