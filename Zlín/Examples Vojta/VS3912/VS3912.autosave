        <CodeBlock Name="libSTS_TUV" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Statistika poloh ventilů a archivace</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//Nulování dat
	If @OptClear
		Let @OptClear = false
		MtxMul optData, Nula, optData
	Endif
	
	If (ClrHist!=0)
		If ClrHist.0
			Let dstRow = 0
			MtxCopy optData, dstRow, NONE, RowClr, NONE, NONE, NONE, NONE, NONE.0
			Let @updPlanWD = true
			Let ClrHist.0 = false
		Endif
		If ClrHist.1
			Let dstRow = 1
			MtxCopy optData, dstRow, NONE, RowClr, NONE, NONE, NONE, NONE, NONE.0
			Let @updPlanWD = true
			Let ClrHist.1 = false
		Endif
		If ClrHist.2
			Let dstRow = 2
			MtxCopy optData, dstRow, NONE, RowClr, NONE, NONE, NONE, NONE, NONE.0
			Let @updPlanWD = true
			Let ClrHist.2 = false
		Endif
		If ClrHist.3
			Let dstRow = 3
			MtxCopy optData, dstRow, NONE, RowClr, NONE, NONE, NONE, NONE, NONE.0
			Let @updPlanWD = true
			Let ClrHist.3 = false
		Endif
		If ClrHist.4
			Let dstRow = 4
			MtxCopy optData, dstRow, NONE, RowClr, NONE, NONE, NONE, NONE, NONE.0
			Let @updPlanWE = true
			Let ClrHist.4 = false
		Endif
		If ClrHist.5
			Let dstRow = 5
			MtxCopy optData, dstRow, NONE, RowClr, NONE, NONE, NONE, NONE, NONE.0
			Let @updPlanWE = true
			Let ClrHist.5 = false
		Endif
		If ClrHist.6
			Let dstRow = 6
			MtxCopy optData, dstRow, NONE, RowClr, NONE, NONE, NONE, NONE, NONE.0
			Let @updPlanWE = true
			Let ClrHist.6 = false
		Endif
		If ClrHist.7
			Let dstRow = 7
			MtxCopy optData, dstRow, NONE, RowClr, NONE, NONE, NONE, NONE, NONE.0
			Let @updPlanWE = true
			Let ClrHist.7 = false
		Endif
	Endif
	
	//Každých 10 vteřin
	SyncMark 1, 10, 0, 0, 0, @Sync10s, NONE
	If @Sync10s
		//Index v matici dat (0..95 čtvrthodin ve dni)
		Let aIndex = (60*RTimPart[2, 0] + RTimPart[1, 0]) div 15
		If ((aIndex == 0) and (aIndex != aIndexOld)) or @Debug1
			Let @Debug1 = false
			Let srcCol1 = 0; srcCol2 = 95
			If ((RTimPart[6, 0] > 1) and (RTimPart[6, 0] < 7))
				//všední dny
				For dstRow, 3, 1, -1
					Let srcRow1 = dstRow - 1; srcRow2 = srcRow1
					MtxCopy optData, dstRow, NONE, optData, srcRow1, srcCol1, srcRow2, srcCol2, NONE.0
					Let optDTTM[dstRow,0] = optDTTM[srcRow1,0]
				EndFor
				Let dstRow = 0
				Let @updPlanWD = true
			Else
				//víkendy
				For dstRow, 7, 5, -1
					Let srcRow1 = dstRow - 1; srcRow2 = srcRow1
					MtxCopy optData, dstRow, NONE, optData, srcRow1, srcCol1, srcRow2, srcCol2, NONE.0
					Let optDTTM[dstRow,0] = optDTTM[srcRow1,0]
				EndFor
				Let dstRow = 4
				Let @updPlanWE = true
			Endif
			MtxCopy optData, dstRow, NONE, optToday, NONE, NONE, NONE, NONE, NONE.0
			Let optDTTM[dstRow,0] = if(RTime>86400, RTime-86400, RTime)
			MtxMul optToday, Nula, optToday
		Endif
		Let aIndexOld = aIndex

		//Komunikuje reprezentativní počet OPS
		If @WTV_OptMan
		Else
			//Žádaná TV je vyšší než žádaná z ÚT
			If (WTV1 > (maxWUT/10.0+OffsetTV))
				//Maxima a minima ventilů TUV
				If (maxValveTUV > optLevValTUV[0,0])
					Let optToday[0, aIndex] = optToday[0, aIndex] + 1
				Endif
				If (maxValveTUV < optLevValTUV[1,0])
					Let optToday[0, aIndex] = optToday[0, aIndex] - 1
				Endif
			Endif
		Endif
		
		//Tvorba časového plánu týden
		If @updPlanWD 
			Let dstRow = 0; @updPlanWD = false
			Let fx1 = 0.0
			For i, 0, 95, 1
				Let tim_planOpt[dstRow, i] = long(i)*900
				Let tmp1=(optData[4*dstRow+0, i] + optData[4*dstRow+1, i] + optData[4*dstRow+2, i] + optData[4*dstRow+3, i]) div 4
				If (tmOffset>0)
					Let k = tmOffset-1
					For j, 1.000, k, 1.000
						Let ix = (i+j) mod 96
						Let tmp2 = (optData[4*dstRow+0, ix] + optData[4*dstRow+1, ix] + optData[4*dstRow+2, ix] + optData[4*dstRow+3, ix]) div 4
						Let tmp1 = max(tmp1, tmp2)
					EndFor
				Endif
				//Let ix = (i+tmOffset) mod 96
				Let val_planOpt[dstRow, i] = If(tmp1<-20, -20, tmp1)
				Let fx1 = fx1 + val_planOpt[dstRow, i]
			EndFor			
			Let Opt_WD = 100*fx1/(96*90)
		Endif

		//Tvorba časového plánu víkend
		If (@updPlanWE) 
			Let dstRow = 1; @updPlanWE = false
			Let fx1 = 0.0
			For i, 0, 95, 1
				Let tim_planOpt[dstRow, i] = long(i)*900
				Let tmp1=(optData[4*dstRow+0, i] + optData[4*dstRow+1, i] + optData[4*dstRow+2, i] + optData[4*dstRow+3, i]) div 4
				If (tmOffset>0)
					Let k = tmOffset-1
					For j, 1.000, k, 1.000
						Let ix = (i+j) mod 96
						Let tmp2 = (optData[4*dstRow+0, ix] + optData[4*dstRow+1, ix] + optData[4*dstRow+2, ix] + optData[4*dstRow+3, ix]) div 4
						Let tmp1 = max(tmp1, tmp2)
					EndFor
				Endif
				//Let ix = (i+tmOffset) mod 96
				Let val_planOpt[dstRow, i] = If(tmp1<-20, -20, tmp1)
				Let fx1 = fx1 + val_planOpt[dstRow, i]
			EndFor
			Let Opt_WE = 100*fx1/(96*90)
		Endif

	Endif]]></Script>
          </Body>
