        <CodeBlock Name="RegPPO" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Regulace vetve UT</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	Let @ErrCom_WTV = ComCntOK<5
	Let @WTV_OptMan = @VS_ManWTV or @ErrCom_WTV
	If @WTV_OptMan
		If @KonsPr1, :NONE
			Let WTV1 = KonstPrTV1
		Else :NONE
			//Interpolace ekvitermni krivky
			Interpol TVenk1, WTV1, EkvPoint1
		EndIf 
		//Casovy plan UT
		DayPlan 0x0000, 2, 0x001F, 0x0060, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, Holidays, UtlTiTV1, UtlTV1, PrUtlTV
		//Aplikace útlumu
		Let WTV1 = WTV1-(WTV1/100)*PrUtlTV
	Else
		//Automaticý požadavek na žádanou TV z VS
		DayPlan 0x0000, 2, 0x001F, 0x0060, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, Holidays, tim_planOpt, val_planOpt, WOptPlan
		Let WTV1 = max(maxWUT/10.0+OffsetTV, maxWTUV/10.0+OffsetTUV) + (WOptPlan/10.0)
	Endif
	Limiter WTV1,50,110
	
	Let @ChodPPO1 = @ReqPPO1 and !Alarm_E1.2 and !Alarm_E1.13 and (PTV1 > 50) and @ChodTV1di
	If @ChodPPO1
		Let RePIDTV1 = RePIDTV1 & 0xFFFD
	Else :NONE
		Let RePIDTV1 = RePIDTV1 | 2
		Let YTV1 = 0
	EndIf 
	
	Let @ChodPPO2 = @ReqPPO2 and !Alarm_E1.3 and !Alarm_E1.13 and (PTV1 > 50) and @ChodTV1di
	If @ChodPPO2
		Let RePIDTV2 = RePIDTV2 & 0xFFFD
	Else :NONE
		Let RePIDTV2 = RePIDTV2 | 2
		Let YTV2 = 0
	EndIf 
	
	Let @ChodPPO3 = @ReqPPO3 and !Alarm_E1.4 and !Alarm_E1.13 and (PTV1 > 50) and @ChodTV1di
	If @ChodPPO3
		Let RePIDTV3 = RePIDTV3 & 0xFFFD
	Else :NONE
		Let RePIDTV3 = RePIDTV3 | 2
		Let YTV3 = 0
	EndIf 
	
	//Let @ChodPPO4 = @ReqPPO4 and !Alarm_E1.4 and (PTV1 > 50) and @ChodTV1di
	//If @ChodPPO4
	//	Let RePIDTV4 = RePIDTV4 & 0xFFFD
	//Else :NONE
	//	Let RePIDTV4 = RePIDTV4 | 2
	//	Let YTV4 = 0
	//EndIf 
	
	PID WTV1,TPPO1,YTV1,RePIDTV1,PaPIDTV1
	PID WTV1,TPPO2,YTV2,RePIDTV2,PaPIDTV2
	PID WTV1,TPPO3,YTV3,RePIDTV3,PaPIDTV3
	//PID WTV1, TPPO4, YTV4, RePIDTV4, PaPIDTV4
	
	TimerOff @ChodPPO1, tKlapOff, @KlapTV1, NONE
	TimerOff @ChodPPO2, tKlapOff, @KlapTV2, NONE
	TimerOff @ChodPPO3, tKlapOff, @KlapTV3, NONE
	
	Let RePIDHV.1 = Alarm_E1.1 or Alarm_E1.14 or Alarm_E1.13 or Alarm_E1.15 or Alarm_E1.0 or !@ChodTV1di or (PolVHV == 9999)
	If RePIDHV.1
		Let YHV = 0
		Let @HavFazHV = false
	Else
		Let @HavFazHV = @ChodTV1di
	Endif
	
	PID WdPHV1,dPHV1,YHV,RePIDHV,PaPIDHV
]]></Script>
          </Body>
