Let Alarm_A1.14 = @STOPBtn and @OverVS and !@LeakVS	//Zaplaveni VS
Let Alarm_A1.15 = !@STOPBtn	       			//STOP tlacitko


//Kvit pro vsechny chyby
Let Alarm_Q1 = If(KvitOut.0, ~Alarm_A1, Alarm_Q1)
Let KvitOut.0 = false

ErrSig Alarm_A1, 0x0001, Alarm_Q1, 0x0001, Alarm_E1.0, Alarm_Ig.0, 300, 4, 1010, 9, 0, 0		//Dlouhodobe prehrati
