          <Script><![CDATA[event S_Address_OnOpen()
	//cz// Aktuální adresa. 
	//en// Actual address.
	NumericView1.Value = SerialBusN.Address;
	
	//cz// ARION
	//en// ARION is active.
	if SerialBusN.ProtocolMode == 0 then	
		//cz// Maximální hodnota adresy může být 63.
		//en// Maximal value can be 63.
		if NumericView1.Value > 63 then
			NumericView1.Value = 1;
		endif;
	endif;
end;

//cz// Inkrementace adresy.
//en// Address inctement.
event Button3_OnButtonPress()
	if SerialBusN.ProtocolMode == 0 then
		// ARION
		if NumericView1.Value < 63 then
			NumericView1.Value = NumericView1.Value + 1;
		else
			NumericView1.Value = 1;
		endif;
	else
	// MODBUS (Max. address can be 247)
		if SerialBusN.ProtocolMode == 1 then
			if NumericView1.Value < 247 then
				NumericView1.Value = NumericView1.Value + 1;
			else
				NumericView1.Value = 1;
			endif;
		endif;
	endif;
end;

//cz// Dekrementace adresy.
//en// Address decrement.
event Button4_OnButtonPress()
	// ARION
	if SerialBusN.ProtocolMode == 0 then
		if NumericView1.Value > 1 then
			NumericView1.Value = NumericView1.Value - 1;
		else
			NumericView1.Value = 63;
		endif;
	else
	// MODBUS
		if SerialBusN.ProtocolMode==1 then 	
			if NumericView1.Value > 1 then
				NumericView1.Value = NumericView1.Value - 1;
			else
				NumericView1.Value = 247;
			endif;
		endif;
	endif;
end;

//cz// Návrat beze změny.
//en// Cancel changes.
event Button2_OnButtonDown()	
	S_Connection.Show();
end;

//cz// Uložení nové adresy.
//en// Storage a new address.
event Button1_OnButtonDown()	
	SerialBusN.Address = NumericView1.Value;
	S_Connection.Show();
end;]]></Script>
