          <Script><![CDATA[event S_SensCorr_OnOpen()
	//cz// Aktuální hodnota korekce čidla.
	//en// Actual value of correction of temperature sensor.
	NumericView1.Value = EEprom.TempSenzorCorrection;
end;

//cz// Inkrementace hodnoty.
//en// Correction inctement.
event Button3_OnButtonPress()
	if NumericView1.Value < 10 then
		NumericView1.Value = NumericView1.Value + 0.1;
	endif;
end;

//cz// Dekrementace hodnoty.
//en// Correction decrement.
event Button4_OnButtonPress()
	if NumericView1.Value > -10 then
		NumericView1.Value = NumericView1.Value - 0.1;
	endif;
end;

//cz// Uložení hodnoty.
//en// Storage value.
event Button1_OnButtonDown()
	EEprom.TempSenzorCorrection = NumericView1.Value;
	S_SystMenu.Show();
end;

//cz// Návrat beze změny.
//en// Cancel changes.
event Button2_OnButtonDown()
	S_SystMenu.Show();
end;]]></Script>
