      <Object Name="Process1" Class="Amit.Ngs.Core.NgsProcessPeriod" Def="Process">
        <Id>6</Id>
        <Body Class="Amit.Ngs.Core.NgsCodeBodyText"><![CDATA[//cz// Periodické čtení teploty a korekce, nastavení příznaku zápisu do registru ze strany ovladače.
//en// Cyclic temperature and correction readouts, "write to register from the controllers site" flag is set.
if Timer1.Elapsed() then
	SerialBusN.TempMeasured = IO.DeviceTemperature + EEprom.TempSenzorCorrection;
	SerialBusN.Status.0 = true;
endif;		
	
if SerialBusN.Connect then
	//cz// Hodnota proměnné popisující stav komunikace pro zobrazovaný text na obrazovce.
	//cz// 0 - po restartu, 2 - nekomunikuje, 3 - komunikuje.
	//en// Value of variable describing communication status for on-screen displayed text.
	//en// 0 - after reset, 2 - not communicating, 3 - communicating.
	Ram.OP7_ComState = 3;
	
	//cz// Musí být zapsána platná hodnota režimu místnosti.
	//en// Correct value must be entered for the room mode.
	if ((SerialBusN.Status >> 1) & 0x3) < 3 then
		//cz// Změna z ovladače.
		//en// Change from the controller.
		if Ram.SlaveW then
			Ram.SlaveW = false;
			Timer1.Start();
			SerialBusN.Status.0 = true;
			SerialBusN.RoomMode = Ram.RoomMode;
			SerialBusN.FanMode = Ram.FanMode;
			SerialBusN.SwitchOnOff = Ram.SwitchOnOff;
			SerialBusN.TempMeasured = IO.DeviceTemperature + EEprom.TempSenzorCorrection;
			SerialBusN.TempSetpointCorr = Ram.TempSetpointCorr;
			SerialBusN.StatusReset = 0;
			SerialBusN.StatusSet = 0;				
		else 
			//cz// Master může zapisovat novou hodnotu.
			//en// Master can enter new value.
			if  (SerialBusN.StatusSet > 0) or (SerialBusN.StatusReset > 0) then	
				SerialBusN.Status = SerialBusN.Status & ~(SerialBusN.StatusReset);
				SerialBusN.Status = SerialBusN.Status | SerialBusN.StatusSet;
				//cz// Hodnota režimu místnosti.
				//en// Room mode value.
				SerialBusN.RoomMode = SerialBusN.RoomMode < 3 ? SerialBusN.RoomMode : Ram.RoomMode;
				Ram.RoomMode = SerialBusN.RoomMode;
				//cz// Hodnota režimu ventilátoru.
				//en// Fan mode value.
				SerialBusN.FanMode = SerialBusN.FanMode < 5 ? SerialBusN.FanMode : Ram.FanMode;
				Ram.FanMode = SerialBusN.FanMode;
				Ram.SwitchOnOff = SerialBusN.SwitchOnOff;
				SerialBusN.StatusReset = 0;
				SerialBusN.StatusSet = 0;
			endif;				
			//cz// Ošetření hodnoty korekce pokud by nebyla v kroku 20, rozsahu -100 až 100..
			//en// Verification of entered correction value (if it´s not a step of 20) in range <-100 .. 100>
			if SerialBusN.TempSetpointCorr < -89 then
				Ram.TempSetpointCorr = -100;												
			else		
				if SerialBusN.TempSetpointCorr < -69 then
					Ram.TempSetpointCorr = -80;
				else	
					if SerialBusN.TempSetpointCorr < -49 then
						Ram.TempSetpointCorr = -60;
					else
						if SerialBusN.TempSetpointCorr < -29 then
							Ram.TempSetpointCorr = -40;
						else
							if SerialBusN.TempSetpointCorr < -9 then
								Ram.TempSetpointCorr = -20;
							else			
								if SerialBusN.TempSetpointCorr < 10 then
									Ram.TempSetpointCorr = 0;
								else			
									if SerialBusN.TempSetpointCorr > 89 then
										Ram.TempSetpointCorr = 100;
									else		
										if SerialBusN.TempSetpointCorr > 69 then
											Ram.TempSetpointCorr = 80;
										else	
											if SerialBusN.TempSetpointCorr > 49 then
												Ram.TempSetpointCorr = 60;
											else			
												if SerialBusN.TempSetpointCorr > 29 then
													Ram.TempSetpointCorr = 40;
												else											
													if SerialBusN.TempSetpointCorr > 9 then
														Ram.TempSetpointCorr = 20;
													endif;			
												endif;
											endif;
										endif;
									endif;
								endif;
							endif;
						endif;
					endif;
				endif;
			endif;
		endif;	
	else
		//cz// Mazání příznaku změny z ovladače.
		//en// "write to register from the controllers site" flag is withdrawn.
		Ram.SlaveW = false;
		//cz// Čeká se na zápis platné hodnoty režimu místnosti.
		//en// Waiting for a valid value of the room mode.
		
		if  (SerialBusN.StatusSet > 0) or (SerialBusN.StatusReset > 0) then	
			SerialBusN.Status = SerialBusN.Status & ~(SerialBusN.StatusReset);
			SerialBusN.Status = SerialBusN.Status | SerialBusN.StatusSet;	
			//cz// Hodnota režimu místnosti.
			//en// Room mode value.
			SerialBusN.RoomMode = SerialBusN.RoomMode < 3 ? SerialBusN.RoomMode : Ram.RoomMode;
			Ram.RoomMode = SerialBusN.RoomMode;
			SerialBusN.StatusReset = 0;
			SerialBusN.StatusSet = 0;
		endif;
	endif;
else
	//cz// Ztráta komunikace, bity 0 až 7 registru Status na hodnotu TRUE.
	//en// Communication loss, bit 0 to 7 of the Status register are TRUE. 
	SerialBusN.Status = 0x00FF;
	//cz// Hodnota pro popis stavu na obrazovce.
	//en// Value for on-screen displayed status. 	
	if Ram.OP7_ComState == 3 then	
		Ram.OP7_ComState = 2;
	endif;
endif;		

//cz// Změna korekce nebo režimu z ovladače, čeká se na novou hodnotu žádané teploty od mastera.
//en// Change of correction value or mode from the controller. Waiting for new desired temperature from the master.
if Ram.WaitForData then
	if Math.CompareFloat(Ram.TempSetpoint_Last, SerialBusN.TempSetpoint, 0.09) then
		Ram.WaitForData = false;
		Timer2.Stop();
	endif;
endif;

//cz// Spuštění Timeru2. Max. doba čekání na novou hodnotu od mastera.
//en// Timer2 launched. Max time waiting for new value from master.
if Ram.Timer2Start then
	Ram.Timer2Start = false;
	Timer2.Start();
endif;

//cz// Po dobu 20 s není detekována změna hodnoty žádané teploty, hodnota je pravděpodobně shodná. Mazání příznaku čekání.
//en// If the new value of the desired temperature is not detected within 20 s, the new value is probably equal. Waiting flag withdrawn. 
if Timer2.Elapsed() then
	Ram.WaitForData = false;
endif;

//cz// Uložení hodnoty žádané teploty.
//en// New desired temperature value is stored.
Ram.TempSetpoint_Last = SerialBusN.TempSetpoint;

//cz// Kontrola, zda byl zapsán čas z nadřazeného systému po restartu.
//en// Check if the new time value has been written from the above system after reset.
if Timer3.Elapsed() then
	if Math.Abs(Ram.ChckTime - DateTime.NowLong) > 5 then
		Timer3.Stop();
		Ram.TimeWritten = true;
	endif;
	Ram.ChckTime = DateTime.NowLong;
endif;]]><Regions /></Body>
        <Period>200</Period>
        <Offset>0</Offset>
        <Priority>1</Priority>
      </Object>
      <Object Name="ProcessInit" Class="Amit.Ngs.Core.NgsProcessInit" Def="ProcessInit">
        <Id>7</Id>
        <Body Class="Amit.Ngs.Core.NgsCodeBodyText"><![CDATA[//cz// Načtení teploty po restartu.
//en// temperature readout after reset.
SerialBusN.TempMeasured = IO.DeviceTemperature + EEprom.TempSenzorCorrection;
SerialBusN.Status = 0x00FF;

//cz// Spuštění Timeru1 pro periodické čtení teploty.
//en// Timer1 launch for cyclic temperature readouts.
Timer1.Start();

//cz// Spuštění Timeru3 pro kontrolu zápisu času z nadřazeného systému.
//en// Timer3 launch for check of the host system time.
Timer3.Start();


//cz// Aktuální čas.
//en// Current time.
Ram.ChckTime = DateTime.NowLong;]]><Regions /></Body>
      </Object>
