          <Script><![CDATA[event S_Parity_OnOpen()
	//cz// Aktuální hodnota parity.
	//en// Actual value of parity.
	if SerialBusN.ProtocolMode == 0 then
	// ARION
		RadioButton1.Visible = false;		
		RadioButton1.Enabled = false;
		Label4.Visible = true;
		Label2.Visible = true;
		Label3.Visible = true;
	else
	// MODBUS
		RadioButton1.Visible = true;
		RadioButton1.Enabled = true;
		Label4.Visible = false;
		Label2.Visible = false;
		Label3.Visible = false;
		if SerialBusN.ModbusParity == 0 then
			RadioButton1.SelectedIndex = 0;	
		else 
			if SerialBusN.ModbusParity == 2 then
	   			RadioButton1.SelectedIndex = 1;	
			else
				RadioButton1.SelectedIndex = 2;
				SerialBusN.ModbusParity = 1;		
			endif;
    	endif;
	endif;	
end;

//cz// Uložení parity.
//en// Storage value of parity.
event Button1_OnButtonDown()
	if RadioButton1.SelectedIndex == 0 then
		SerialBusN.ModbusParity = 0;
	else
		if RadioButton1.SelectedIndex == 1 then
			SerialBusN.ModbusParity = 2;
		else 
			SerialBusN.ModbusParity = 1;			
		endif;
	endif;
	S_Connection.Show();
end;

//cz// Návrat beze změny.
//en// Cancel changes.
event Button2_OnButtonDown()
	S_Connection.Show();
end;]]></Script>
