          <Script><![CDATA[event S_Main_V7_OnOpen()	
	//cz// Nulování pomocné proměnné.
	//en// Erasing auxiliary variable.
	Ram.CounterScr = 0;
	//cz// Aktivace spořiče displeje.
	//en// Activation of screensaver.
	Global.ScreenSaver1.Enabled = true;

	//cz// Jestliže je režim časový plán, zobrazí se patřičné editační prvky.
	//en// If is active room mode, then are visible proper elements.
	if Ram.RoomMode == 0 then	
		CaseImage1.Visible = true;
		ImageMap1.Enabled = true;
		ImageMap1.Visible = true;
		Image1.Visible = true;
		Image2.Visible = true;
	else	
	//cz// Režim komfort, útlum.	
	//en// Comfort mode, Attenuation mode.
		CaseImage1.Visible = false;
		ImageMap1.Enabled = false;
		ImageMap1.Visible = false;
		Image1.Visible = false;
		Image2.Visible = false;
	endif;
	
	//cz// Stavu vypínače.
	//en// State of switch.
	ToggleButton1.Checked = Ram.SwitchOnOff;
	
	//cz// Čeká se na vyčtení dat nebo je rozpadlá komunikace. Skryje se žádaná teplota. Zobrazí se pomlčky.
	//en// Waiting for new data or on-wall room unit not communicates. Setpoint temperature is hidden. Label with dashes is visible.
	if Ram.WaitForData or (Ram.OP7_ComState != 3) then
		NumericView2.Visible = false;	
		Label3.Visible=true;
	else					
		NumericView2.Visible = true;	
		Label3.Visible = false;
	endif;
end;

//cz// Inkrementace korekce. Skryje se žádaná teplota. Zobrazí se pomlčky. Nastaví se potřebné příznaky pro čekání nové hodnoty.
//en// Correction increment. Setpoint temperature is hidden. Label with dashes is visible. Set up flags for waiting new value.
event ImageMap1_OnRegionDown0()
	NumericView2.Visible = false;	
	Label3.Visible = true;		
	if Ram.TempSetpointCorr < 100 then
		Ram.TempSetpointCorr = Ram.TempSetpointCorr + 20;
	else
		Ram.TempSetpointCorr = 100;
	endif;
	Ram.SlaveW = true;
	Ram.WaitForData = true;
	Ram.Timer2Start = true;		
end;

//cz// Dekrementace korekce. Skryje se žádaná teplota. Zobrazí se pomlčky. Nastaví se potřebné příznaky pro čekání nové hodnoty.
//en// Correction decrement. Setpoint temperature is hidden. Label with dashes is visible. Set up flags for waiting new value.
event ImageMap1_OnRegionDown1()
	NumericView2.Visible = false;
	Label3.Visible = true;		
	if Ram.TempSetpointCorr > -100 then
		Ram.TempSetpointCorr = Ram.TempSetpointCorr -20;
	else
		Ram.TempSetpointCorr = -100;
	endif;
	Ram.SlaveW = true;
	Ram.WaitForData = true;
	Ram.Timer2Start = true;		
end;

//cz// Zobrazení uživatelského menu.
//en// UserMenu is showed.	
event Button1_OnButtonUp()
	Ram.CounterScr = 0;
	S_UserMenu.Show();
end;

//cz// Zobrazení systémového menu.
//en// SystemMenu is showed.
event Button1_OnButtonPress()
	Ram.CounterScr = Ram.CounterScr + 1;
	if Ram.CounterScr > 50 then	
		Ram.CounterScr = 0;
		S_SystMenu.Show();
	endif;
end;

event S_Main_V7_OnRefresh()
//cz// Režim místnosti. Skryje se žádaná teplota. Zobrazí se pomlčky. Nastaví se potřebné příznaky pro čekání nové hodnoty.
//en// Room mode. Setpoint temperature is hidden. Label with dashes is visible. Label with dashes is visible. Set up flags for waiting new value.
	if Ram.RoomMode == 0 then
	//cz// Režim časový plán.
	//en// Time-schedule mode.	
		CaseImage1.Visible = true;
		ImageMap1.Enabled = true;
		ImageMap1.Visible = true;
		Image1.Visible = true;
		Image2.Visible = true;
	else					
	//cz// Režim komfort, útlum.	
	//en// Comfort mode, Attenuation mode.
		CaseImage1.Visible = false;
		ImageMap1.Enabled = false;
		ImageMap1.Visible = false;
		Image1.Visible = false;
		Image2.Visible = false;
	endif;
	
	//cz// Stavu vypínače.
	//en// State of switch.
	ToggleButton1.Checked = Ram.SwitchOnOff;
	
	//cz// Čeká se na vyčtení dat nebo je rozpadlá komunikace. Skryje se žádaná teplota. Zobrazí se pomlčky.
	//en// Waiting for new data or on-wall room unit not communicates. Setpoint temperature is hidden. Label with dashes is visible.
	if Ram.WaitForData or (Ram.OP7_ComState != 3) then
		NumericView2.Visible = false;	
		Label3.Visible=true;
	else					
		NumericView2.Visible = true;	
		Label3.Visible = false;
	endif;
end;

//cz// Vypínač - ZAP
//en// Switch - ON
event ToggleButton1_OnToggleButtonDown()
	Ram.SwitchOnOff = true;
	Ram.SlaveW = true;
end;

//cz// Vypínač - VYP
//en// Switch - OFF
event ToggleButton1_OnToggleButtonUp()
	Ram.SwitchOnOff = false;
	Ram.SlaveW = true;
end;

event CaseButton2_OnButtonDown()
//cz// Režim místnosti. Skryje se žádaná teplota. Zobrazí se pomlčky. Nastaví se potřebné příznaky pro čekání nové hodnoty.
//en// Room mode. Setpoint temperature is hidden. Label with dashes is visible. Label with dashes is visible. Set up flags for waiting new value.
	NumericView2.Visible = false;	
	Label3.Visible = true;		
	Ram.SlaveW = true;
	Ram.WaitForData = true;
	Ram.Timer2Start = true;		
	if SerialBusN.RoomMode == 0 then
	//cz// Režim časový plán.
	//en// Time-schedule mode.	
		CaseImage1.Visible = true;
		ImageMap1.Enabled = true;
