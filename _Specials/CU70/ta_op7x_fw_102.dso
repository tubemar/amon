          <Script><![CDATA[event S_ScreenSaver_OnOpen()
	//cz// Deaktivace spořiče.
	//en// Deactivation of screensaver.
	Global.ScreenSaver1.Enabled = false;
	
	//cz// Čeká se na vyčtení dat nebo je rozpadlá komunikace. Skryje se žádaná teplota. Zobrazí se pomlčky.
	//en// Waiting for new data or on-wall room unit not communicates. Setpoint temperature is hidden. Label with dashes is visible.
	if Ram.WaitForData or (Ram.OP7_ComState != 3) then
		NumericView2.Visible = false;	
		Label3.Visible = true;
	else					
		NumericView2.Visible = true;	
		Label3.Visible = false;
	endif;
	
	// Kontrola zápisu času z nadřazeného systému
	if Ram.TimeWritten then
		Label5.Visible = false;
		Label6.Visible = false;
		DateTimeView1.Visible = true;
		DateTimeView2.Visible = true;
	else
		Label5.Visible = true;
		Label6.Visible = true;
		DateTimeView1.Visible = false;
		DateTimeView2.Visible = false;		
	endif;		
end;

event S_ScreenSaver_OnRefresh()
	//cz// Odpočet času do vypnutí podsvitu.
	//en// Coundown of delay to switch-off brightness.
	if Ram.TwilightTime > 0 then
  		Ram.TwilightTime = Ram.TwilightTime - 1;
		if Ram.TwilightTime == 0 then
	  		Application.Brightness = 0;
		endif;
	endif;

	//cz// Čeká se na vyčtení dat nebo je rozpadlá komunikace. Skryje se žádaná teplota. Zobrazí se pomlčky.
	//en// Waiting for new data or on-wall room unit not communicates. Setpoint temperature is hidden. Label with dashes is visible.
	if Ram.WaitForData or (Ram.OP7_ComState != 3) then
		NumericView2.Visible = false;	
		Label3.Visible = true;	
	else				
		NumericView2.Visible = true;	
		Label3.Visible = false;
	endif;

	//cz// Zobrazení vteřin.
	//en// Seconds hand.
	Label4.Visible = not Label4.Visible;
	
	// Kontrola zápisu času z nadřazeného systému
	if Ram.TimeWritten then
		Label5.Visible = false;
		Label6.Visible = false;
		DateTimeView1.Visible = true;
		DateTimeView2.Visible = true;
	else
		Label5.Visible = true;
		Label6.Visible = true;
		DateTimeView1.Visible = false;
		DateTimeView2.Visible = false;		
	endif;		
end;

//cz// "Probuzení" displeje.
//en// Wake up touchscreen.
event ImageMap1_OnRegionDown0()
	Application.Brightness = EEprom.Brightness;
	if Ram.TwilightTime > 0 then
		S_Start.Show();
	endif;
	Ram.TwilightTime = 5;
end;]]></Script>
