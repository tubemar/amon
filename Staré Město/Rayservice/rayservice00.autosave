        <CodeBlock Name="P02_Regulace" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Rizeni</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[// Časový plán
DayPlan 0x0000, 1, 0x007F, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, 0x0000, DP_feasts, DP_times, DP_values, Ix
Let @DPklid = If (ix > 0, true, false)

// Regulace hladiny

Hyst2 AI0[0,0], @PozadChod, LevelPar[1,0], LevelPar[2,0], 0x0000		// provozní hladina
Hyst2 AI0[0,0], @PozadChodBl, LevelPar[1,0], LevelPar[3,0], 0x0000		// akumulační hladina
Let @CerpZap = (not @DPklid and @PozadChod) or @PozadChodBl			// čerpá se buď v době klidu nebo ihned, pokud je hladina přes akumulační

//*******************************************************************
// Reakce na poruchy
//*******************************************************************

Let @Test = Alarm_E.1 or Alarm_E.2	or Alarm_E.4	or Alarm_E.5			// nízká hladina nebo přerušené čidlo nebo dlouhý chod
Let @CerpZap = If (@Test, false, @CerpZap)]]></Script>
          </Body>
          <Period>1000</Period>
          <Offset>0</Offset>
          <Type>2</Type>
          <InterruptEdge>0</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="P03_SendSMS" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Posílání SMS</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[If @Zap_SMS
	If not StavSMS.0	// SMS zpráva není odesílána
		If StavSMS.1	// SMS zpráva byla úspěšně odeslána
			Let Gsm_AdrOrder = If (GSM_TelCis[Gsm_AdrOrder,0] > 0, Gsm_AdrOrder + 1, 0)    	//Pokod je další tel. cislo 0, konec vysílání
		EndIf
		Let StavSMS = 0
		
		If Gsm_AdrOrder > 0										// Stavový automat, pokud 0 - nevysílá se
			Let tLong = GSM_TelCis[Gsm_AdrOrder-1,0]						// Pomocná proměnná pro sest. čísla
			StrFormat SmsAdr, 0, NONE, "+", 11, 45, -1, -1, Gsm_Pred1, NONE.0		// Předčíslí
			StrFormat SmsAdr, 4, NONE, "", 11, 45, -1, -1, tLong, NONE.0			// Číslo
			SMSSend :1111, "%s", SMS_Text, NONE, StavSMS, "", SmsAdr[0,*], 1440
		
		Else	// Nic se nevysílá, je čas na měření
			GSMDiag :1111, Gsm_Ntw, Gsm_StvSig, NONE						// GSM diagnostika
			Let Gsm_Sig = If (Gsm_StvSig >= 99, 0, (100 * Gsm_StvSig) DIV 31)		// Přepočet síly signálu
		EndIf
	EndIf
Else
	Let Gsm_AdrOrder = 0
	Let StavSMS = 0
EndIf]]></Script>
          </Body>
          <Period>5000</Period>
          <Offset>0</Offset>
          <Type>3</Type>
          <InterruptEdge>0</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="P04_Alarm" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Poruchy, kvitace</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Příznak restartu
TimerOn @Restart, Restart_time, ix.0, NONE
Let @Restart = If(ix.0,false,@Restart)

Let fX = AI0_params[0,4] / 3
Let LevelPar[0,0] = If (LevelPar[0,0]>fX, fX, LevelPar[0,0])		// havarijní min. musí být v první třetině

Let fX = AI0_params[0,4] / 25						// při rozsahu 2.5m je to 10 cm min. rozdíl hladin
For ix, 0, 3, 1
	Let LevelPar[ix+1,0] = If (LevelPar[ix+1,0]<LevelPar[ix,0]+fX, LevelPar[ix,0]+fX, LevelPar[ix+1,0])		// zabezpečení logické posloupnosti hladin
EndFor

// *****************************************************************************
//                    PORUCHY
// *****************************************************************************

Let Alarm_A.0 = AI0[0,0] > LevelPar[4,0]			// hladina nad hav. maximem
Let Alarm_A.1 = AI0[0,0] < LevelPar[0,0]			// hladina pod hav. minimem

Let fX = AI0_params[0,3]-AI0_params[0,4]/10.000		// min. je -10% z rozsahu, pod tuto hranici chyba snímače
Let fY = (AI0_params[0,4]-AI0_params[0,3])/100.0
Hyst AI0[0,0], Alarm_A.2, fX, fY, 0x0001			// přerušení snímače

// Hlídání změny hladiny při čerpání
If @CerpZap
	If ChckLev_poc == 0							// náběh čerpadla nebo další interval
		Let ChckLev_act = AI0[0,0]
	Else 
		If ChckLev_poc == ChckLev_time				// interval kontroly je odvozeny od periody procesu!
			Let @ChckLev = false
			If (ChckLev_act - AI0[0,0]) < ChckLev_chng
				Let @ChckLev = true				// zamrznutí snímače nebo ucpané čerpadlo
			EndIf
			Let ChckLev_poc = -1
		EndIf
	EndIf
	Let ChckLev_poc = ChckLev_poc + 1
Else
	Let ChckLev_poc = 0					// nulování kontroly
	Let @ChckLev = false
EndIf

Let Alarm_A.2 = Alarm_A.2 or @ChckLev			// porucha čidla buď kvuli přerušení nebo zamrznutí

Let Alarm_A.3 = @CerpZap and not @ChodCerp		// chod čerpadla

TimerOn @CerpZap, Cerp_timeout, Alarm_A.4, NONE		// dlouhy chod čerpadla

Let Alarm_A.5 = not @Tesnost

Let  Alarm_I = If (@Restart, 0x000F, 0)			// zpoždění detekce poruch po restartu

//Kvitace alarmů
Let Alarm_Q = If(pQuit.0, ~(Alarm_I | Alarm_A), Alarm_Q)
Let pQuit.0 = false

ErrSig Alarm_A, 0x0001, Alarm_Q, 0x0001, Alarm_E.0, Alarm_I.0, 3, 0, 4096, 2000, 0, 0
ErrSig Alarm_A, 0x0002, Alarm_Q, 0x0002, Alarm_E.1, Alarm_I.1, 3, 0, 4096, 2001, 0, 0
ErrSig Alarm_A, 0x0004, Alarm_Q, 0x0004, Alarm_E.2, Alarm_I.2, 3, 0, 4096, 2002, 0, 0
ErrSig Alarm_A, 0x0008, Alarm_Q, 0x0008, Alarm_E.3, Alarm_I.3, 3, 0, 4096, 2003, 0, 0
ErrSig Alarm_A, 0x0010, Alarm_Q, 0x0010, Alarm_E.4, Alarm_I.4, 3, 0, 4096, 2004, 0, 0
ErrSig Alarm_A, 0x0020, Alarm_Q, 0x0020, Alarm_E.5, Alarm_I.5, 3, 0, 4096, 2005, 0, 0


Let @ErrSign = Alarm_E != 0
If not @ErrSignOld and @ErrSign		// náběh některé z poruch
	Let Gsm_AdrOrder = 1			// spuštění posílání SMS
EndIf
Let @ErrSignOld = @ErrSign

StrFormat SMS_Text, 0, StrIndex, "Jimka: ", -1, -1, -1, -1, NONE[0,0], NONE.0
Let ListStart = StrIndex
If Alarm_E.0
	StrFormat SMS_Text, StrIndex, StrIndex, "Hladina nad max.,", -1, -1, -1, -1, NONE[0,0], @SmsLenOK
EndIf
If Alarm_E.1
	StrFormat SMS_Text, StrIndex, StrIndex, "Hladina pod min.,", -1, -1, -1, -1, NONE[0,0], @SmsLenOK
EndIf
If Alarm_E.2
	StrFormat SMS_Text, StrIndex, StrIndex, "Por. snimace,", -1, -1, -1, -1, NONE[0,0], @SmsLenOK
EndIf
If Alarm_E.3
	StrFormat SMS_Text, StrIndex, StrIndex, "Por. cerpadla,", -1, -1, -1, -1, NONE[0,0], @SmsLenOK
EndIf
If Alarm_E.4
	StrFormat SMS_Text, StrIndex, StrIndex, "Dlouhy chod cerp.,", -1, -1, -1, -1, NONE[0,0], @SmsLenOK
EndIf
If Alarm_E.5
	StrFormat SMS_Text, StrIndex, StrIndex, "Netesnost potrubi,", -1, -1, -1, -1, NONE[0,0], @SmsLenOK
EndIf

// Je-li porucha, umaž poslední znak (přebývající ",")	
Let ix.0 = StrIndex > ListStart
If ix.0
	If @SmsLenOK
		Let SMS_Text[0,StrIndex-1] = 0
	Else
		//Pokud se text nevleze do SMS
		StrFormat SMS_Text, 151, StrIndex, ",+dalsi!!", -1, -1, -1, -1, NONE[0,0], @SmsLenOK
	Endif
Else
	StrFormat SMS_Text, StrIndex, StrIndex, "bez poruchy", -1, -1, -1, -1, NONE[0,0], NONE.0
EndIf]]></Script>
          </Body>
          <Period>1000</Period>
          <Offset>0</Offset>
          <Type>4</Type>
          <InterruptEdge>0</InterruptEdge>
        </CodeBlock>
