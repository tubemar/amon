        <CodeBlock Name="libAI" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Zpracování analogových vstupů</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[     	//Analogové vstupy
	//*************************************************
	//******************* KANÁL AI0 *******************
	//*************************************************
	
	//******************* ANALOGOVÝ VSTUP AI0.0 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AI0_typ.0, :NONE
	  //Pt1000
	  If TC0_typ.0, :NONE
	    //Měření napětí na čidle v rozsahu 0 .. 5 V
	    AnIn #Ni10001_0, AI0_Pom[0,0], 5.000, 0.000, 5.000, 0.000, 5.000
	    //Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
	    Interpol AI0_Pom[0,0], AI0_okamzita[0,0], Pt1000_Kriv
	  //Ni1000
	  Else :NONE
           //5000ppm
           If Ni0_typ.0, :NONE
             Ni1000 #Ni10001_0, AI0_okamzita[0,0], 5000
             //6180ppm
           Else :NONE
             Ni1000 #Ni10001_0, AI0_okamzita[0,0], 6180
           EndIf 
	  EndIf
         //Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
       Else :NONE
         AnIn #AI00_0, AI0_okamzita[0,0], AI0_params[0,0], AI0_params[0,1], AI0_params[0,2], AI0_params[0,3], AI0_params[0,4]
       EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AI0_konst[0,0]*AI0_okamzita[0,0]+AI0_konst[0,1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AI0[0,0], AI0_konst[0,2]
	
	
       //******************* ANALOGOVÝ VSTUP AI0.1 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AI0_typ.1, :NONE
	  //Pt1000
	  If TC0_typ.1, :NONE
	    //Měření napětí na čidle v rozsahu 0 .. 5 V
	    AnIn #Ni10001_1, AI0_Pom[1,0], 5.000, 0.000, 5.000, 0.000, 5.000
	    //Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
	    Interpol AI0_Pom[1,0], AI0_okamzita[1,0], Pt1000_Kriv
	  //Ni1000
	  Else :NONE
           //5000ppm
           If Ni0_typ.1, :NONE
             Ni1000 #Ni10001_1, AI0_okamzita[1,0], 5000
             //6180ppm
           Else :NONE
             Ni1000 #Ni10001_1, AI0_okamzita[1,0], 6180
           EndIf 
	  EndIf
         //Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
       Else :NONE
         AnIn #AI00_1, AI0_okamzita[1,0], AI0_params[1,0], AI0_params[1,1], AI0_params[1,2], AI0_params[1,3], AI0_params[1,4]
       EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AI0_konst[1,0]*AI0_okamzita[1,0]+AI0_konst[1,1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AI0[1,0], AI0_konst[1,2]
       
	//******************* ANALOGOVÝ VSTUP AI0.2 *******************
