          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Režim
Let @AUT = Rezim_A == 1; @VYP = Rezim_A == 2; @ZAP = Rezim_A == 3

//Aktivní skupina
Let @Grp1_Act = Grp_Act.0; @Grp2_Act = Grp_Act.1; @Grp3_Act = Grp_Act.2; @Grp4_Act = Grp_Act.3

//Příslušnost ke skupině
Let @Grp1 = Grp_A.0; @Grp2 = Grp_A.1; @Grp3 = Grp_A.2; @Grp4 = Grp_A.3

//Sepnutí atrakce
Let @Cmd = @Enable AND !@Error AND !@VYP AND ((@CP_En AND ((@Grp1_Act AND @Grp1) OR (@Grp2_Act AND @Grp2) OR (@Grp3_Act AND @Grp3) OR (@Grp4_Act AND @Grp4))) OR @ZAP)

//Výpočet poruchy

Let @Err_Vyp = @Cmd AND !@CmdFdb
TimerOn @Err_Vyp, TimeCmd, @Err_Dly, TimerOut
  If @Err_Dly, :NONE
    Let @Error = True
  EndIf

If @Reset, :NONE
	Let @Error = False
EndIf

