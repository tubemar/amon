        <CodeBlock Name="sb_RTC" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>
          </Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[// Čas RTC
GetTime RTC_time, RTC_slozky, NONE
// Změna zimní / letní čas
Let UTC_time = if(@tmLetniCas, RTC_time - 10800, RTC_time - 7200)
If @SysStart, :NONE
Else :NONE
  Let @tmLetoInit = if((UTC_time < LastUTC_t) or (UTC_time > LastUTC_t + 2), bool(0), @tmLetoInit)
EndIf
Let LastUTC_t = UTC_time
ParseTime UTC_time, NONE, NONE, UTC_slozky
Let @tmLeto = ((UTC_slozky[4, 0] > 3) and (UTC_slozky[4, 0] < 10) or (UTC_slozky[4, 0] == 3) and ((UTC_slozky[3, 0] + 7) - UTC_slozky[6, 0] > 31)) or (UTC_slozky[4, 0] == 10) and ((UTC_slozky[3, 0] + 7) - UTC_slozky[6, 0] <= 31)
If @tmLetoInit, :NONE
	Let @tmZmena = @tmLeto xor @tmLetniCas
	If @tmZmena, :NONE
      		GetTime RTC_time, NONE, NONE
      		Let RTC_time = if(@tmLeto, RTC_time + 3600, RTC_time - 3600)
      		SetTime RTC_time
      	EndIf 
EndIf 
Let @tmLetniCas = @tmLeto
Let @tmLetoInit = true
Let @SysStart = false]]></Script>
          </Body>
          <DBase>
            <VariableList>
              <Variable Name="RTC_bin" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>4913</Wid>
                <Station>4</Station>
                <Type>dbtInt</Type>
                <Comment>Čas RTC, logické hodnoty</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
              </Variable>
              <Variable Name="UTC_time" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>4914</Wid>
                <Station>4</Station>
                <Type>dbtLong</Type>
                <Comment>Čas Azory, formát DB-Net</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
              </Variable>
              <Variable Name="UTC_slozky" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>4915</Wid>
                <Station>4</Station>
                <Type>dbtIntMtx</Type>
                <Comment>Čas Azory, složkový formát</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
                <Row>8</Row>
                <Col>1</Col>
              </Variable>
              <Variable Name="LastUTC_t" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>1000</Wid>
                <Station>1</Station>
                <Type>dbtLong</Type>
                <Comment>Čas Azory, předchozí hodnota</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
              </Variable>
              <Variable Name="Restart" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>1001</Wid>
                <Station>1</Station>
                <Type>dbtInt</Type>
                <Comment>Příznak restartu</Comment>
                <WarmInit>True</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
                <InitialValue>1</InitialValue>
              </Variable>
            </VariableList>
            <AliasList>
              <Alias Name="@tmLetniCas" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Byl proveden přechod na letní čas</Comment>
                <VarName>RTC_bin</VarName>
                <Bit>0</Bit>
              </Alias>
              <Alias Name="@tmLeto" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Je letní období</Comment>
                <VarName>RTC_bin</VarName>
                <Bit>1</Bit>
              </Alias>
              <Alias Name="@tmZmena" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Změna období</Comment>
                <VarName>RTC_bin</VarName>
                <Bit>2</Bit>
              </Alias>
              <Alias Name="@tmLetoInit" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Bylo provedeno počáteční nastavení času</Comment>
                <VarName>RTC_bin</VarName>
                <Bit>3</Bit>
              </Alias>
              <Alias Name="@SysStart" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Došlo k restartu řídicího systému</Comment>
                <VarName>Restart</VarName>
                <Bit>0</Bit>
              </Alias>
            </AliasList>
