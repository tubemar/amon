        <CodeBlock Name="fb_rekup" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>Rekuperace</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Rezim, parametry
Let Rezim_D = RezimRekup_D 
Let @Automat = RezimRekup_A==1; @Rucne = RezimRekup_A==2
Let param = parametry


//Porucha zámraz - Ochrana proti zamrazu
Let @Bit1 = (@diference AND @DiffEn)OR((TzaRekup<TzRmin) AND @TepEn)
TimerOff @Bit1, TimeZamraz, @ErrZamraz, TimerOut
//Externí porucha
Let @ErrExt = @ErrExtEn AND @PoruchaExt
//Porucha pro VZT
Let @Err = @ErrExt // OR @ErrZamraz


//Regulace
Let TD1 = VenkovniTep - ZadanaTep
Let TD2 = VenkovniTep - OdtahovaTep
//Chladit?
Hyst TD1, @rBit1, MezePlus[0,0], MezePlus[1,0], 0x0000
Hyst TD2, @rBit2, MezePlus[0,0], MezePlus[1,0], 0x0000
Let @chladit = @rBit1 AND @rBit2 AND @P_Chladit
//Topit?
Hyst TD1, @rBit3, MezeMinus[0,0], MezeMinus[1,0], 0x0001
Hyst TD2, @rBit4, MezeMinus[0,0], MezeMinus[1,0], 0x0001
Let @topit = @rBit3 AND @rBit4  AND !@P_Chladit
//Rekuperovat?
Let @Rekuperovat= @chladit OR @topit

//Vystup do řízení regulace
Let @Rekuperuje = @rekuperovat AND @Automat AND @ProgEn AND !@Err 	//Rekuperuje v automatu a neni v zamrazu

Let Odchylka = PrivodniTep - ZadanaTep					//Odchylka žádané a měřené (žádaná odchylka, ideálně=0)
Let Odchylka = IF(@chladit, -Odchylka, Odchylka)

Let PIDrezim.0 = !@rekuperuje						//nulový regulační zásah
Let PIDrezim.1 = false							//pozastavení regulace
Let @bit1 = @startVZT AND (!@Automat OR @Blok)
if @bit1
	Let Odchylka = (Vystup_PID - Vystup)/4
endif

Let Vystup_PID = IF(@blok, Vystup, Vystup_PID)
PID Z_Odchylka, Odchylka, Vystup_PID, PIDrezim, PIDparametry
Let Vystup = Vystup_PID

Let @Blok_on_s = ((Vystup + MezMinMax) > RekuperMax) AND !@Blok			//Zablokovani rekuperace naplno -> povoleni topení, chlazení VZT
TimerOn @Blok_on_s, TimeBlokuj, @Blok_on, TimerOut
Let @Blok_on = @Blok_on OR (@StartVZT AND !@ChodVZT)
if @Blok_on
  Let @Blok = true
endif

if @Blok_off
  Let @Blok = false
endif
Let @Blok_off_s = @Odblokuj AND @Blok					//Odblokování běhu naplno jestliže neni požadavek topení a chlazení po stanovenou minimalni dobu
TimerOn @Blok_off_s, TimeOdblokuj, @Blok_off, TimerOut
Let @Blok_off = @Blok_off OR !@Rekuperuje

//Vystupy z bloku
Let Vystup = IF(@blok, 100, Vystup)
Let @Naplno = @Blok


//Omezeni od teploty za rekuperátorem
if @TepEn,:NONE
  Interpol TZaRekup, RekuperMax, Omezeni
  Limiter Vystup, 0.000, RekuperMax
endif

Let Vystup = IF(@Rekuperuje, Vystup, 0)
Let Vystup = IF(@ChodVZT, Vystup, 0)

//Rucni Rezim
Let Vystup = IF(@Rucne, Vystup_ruc, Vystup)


//Stavy
Let Vystup = IF(@OvrEn, 100, Vystup)
//Odstavení rekuperátoru
Let @Bit1 = @EmOff OR @ErrZamraz OR @ErrExt
Let Vystup = IF(@Bit1, 0, Vystup)
//Povel zapnout
Let @povel = Vystup > minZap

//Reset
if @Reset, :NONE
  Let @ErrZamraz = false
  Let @ErrExt = false
  Let @Err = false
endif


//Výstup na minimu
Let @NaNule = (Vystup<MezMinMax)OR !@Automat //Vystup v nule, nebo neni v automatu

//Rezim
Let RezimRekup_D = Rezim_D



//***
Let @StartVZT = @StartVZT
Let @OdstavVZT = @OdstavVZT
]]></Script>
          </Body>
