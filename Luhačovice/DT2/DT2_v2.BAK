        <CodeBlock Name="fb_Cerpadlo" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>Čerpadlo s doběhem</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[// Parametry
Let Param=Parametry
// Režim motoru - Vstup
Let @Automat=(Rezim_A==1); @Vypnuto=(Rezim_A==2); @Zapnuto=(Rezim_A==3)
// Stav motoru - Vstup
Let @ProgEn=Rezim_D.0; @ProgSt=Rezim_D.1; @OvrEn=Rezim_D.2; @OvrSt=Rezim_D.3; @EmOff=Rezim_D.4
Let @Reset=Rezim_D.15

//Doběh čerpadla
Let @pom_bit=@ProgEn AND @ProgSt AND @Automat
TimerOff @pom_bit, Dobeh, @Poz_Ch_Dob, TimerOut

// Zapnutí motoru
Let @Cmd=((@Poz_Ch_Dob OR @Zapnuto) AND !@Vypnuto AND !@Err AND !@EmOff AND (!@OvrEn OR (@OvrEn AND @OvrSt))) OR (@OvrEn AND @OvrSt)

// Vyhodnocení poruch
If Param.0, :NONE
  Let @pom_bit=@Cmd AND !@DPMon_In
  TimerOn @pom_bit, TimerDif, @pom_bit2, TimerOut
  If @pom_bit2, :NONE
    Let @ErrDPMon = true
  EndIf
EndIf


If Param.1, :NONE
  Let @pom_bit=@Cmd AND !@CmdFdb_In
  TimerOn @pom_bit, TimerCon, @pom_bit, TimerOut
  If @pom_bit, :NONE
    Let @ErrCmd = true
  EndIf
EndIf

If Param.2, :NONE
  If @ThOvrld_In
    Let @ErrThOvrld = true
  endif
EndIf

If @reset, :NONE
Let @ErrDPMon = false; @ErrCmd = false; @ErrThOvrld = false
Let @reset=false
EndIf

Let @Err=@ErrDPMon OR @ErrCmd OR @ErrThOvrld


// Stav motoru - výstup
Let Rezim_D.5 = @Cmd
Let Rezim_D.6 = ((@DPMon_IN AND Param.0) OR (@CmdFdb_IN AND Param.1)) OR (!Param.0 AND !Param.1 AND @Cmd)
Let Rezim_D.7 = @ErrDPMon; Rezim_D.8=@ErrCmd; Rezim_D.9=@ErrThOvrld; Rezim_D.10=@Err
Let @Cmd_out = @cmd
]]></Script>
