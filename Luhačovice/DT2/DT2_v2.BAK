          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Vzduchotechnika VZT3 - Šatny + Sprchy
	//Ovládání 
	Let VZT3_D.0 = true  		//přepínač - povolení chodu automatu
	Let VZT3_D.1 = false 		//přepínač - zapnutí VZT ručně
	Let VZT3_D.15 = @VZT3_Res	 	//Reset VZT

	//Řízení VZT
	Let @EPS_VZT=False
	Let @PPK_VZT = False
	Let @DPMonFp = @DT3_1; @DPMonFo = @DT3_2
	Let Nic=0; Emax=0
	fb_VZT VZT3_A, VZT3_D, FMM3_2_D, FMM3_1_D, SP3_D, Kl3_3_D, Nic, Nic, CPY_VZT3, CPC_VZT3, Al_VZT3, Ao_VZT3, Emax, @EPS_VZT, @PPK_VZT, @DPMonFp, @DPMonFo, @Kl3_1_Zo, @Kl3_2_Zo, 447, CP_VZT3
	
	//Přívodní+Odtahový ventilátor
	Let @Reset=@VZT3_Res
	Let FMM3_2_D.15=@Reset
	Let @DPMon=False; @CmdFdb=@FM3_2_Chod; @Error=@FM3_2_Por
	fb_MotorFM FMM3_2_A, FMM3_2_D, CP_VZT3, VZT3_Otacky, @Cmd, @DPMon, @CmdFdb, @Error, FMM3_2_ruc, FMM3_2, 6
	Let @FM3_2_Zo=@Cmd
	
	Let @Reset=@VZT3_Res
	Let FMM3_1_D.15=@Reset
	Let @DPMon=False; @CmdFdb=@FM3_1_Chod; @Error=@FM3_1_Por
	fb_MotorFM FMM3_1_A, FMM3_1_D, CP_VZT3, VZT3_Otacky, @Cmd, @DPMon, @CmdFdb, @Error, FMM3_1_ruc, FMM3_1, 6
	Let @FM3_1_Zo=@Cmd
	
	//Řízení regulace + korekce teploty
	Let Nic = 0
	fb_Regulace VZT3_ZT, VZT3_ZTK, TVenk, TVV_3, TNV_3_2, SP3_D, Nic, Kl3_3_D, Nic, 11
	
	//Teplovodní ohřev + čerpadlo
	Let @Zamraz = @TZ3
	fb_TVohrev SP3_A, SP3_D, CTV3_D, PID_VZT3_Top, VZT3_ZTK, TVV_3, TVenk, TVV_3, SP3_ruc, SP3, @Zamraz
	
	Let @DPMon=False; @CmdFdb=@CTV3_Chod; @ThOvrld=False
	fb_Cerpadlo CTV3_A, CTV3_D, TimeDobeh, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 2
	Let @CTV3_Zo=@Cmd
	
	//Rekuperace
	Let @Error=False; @DPMon=@DT3_3
	Let NicF = 0
	fb_rekup Kl3_3_A, Kl3_3_D, PID_VZT3_Rek, VZT3_ZTK, TVenk, TVV_3, TNV_3_2, NicF, Kl3_3_ruc, Kl3_3, 1, @DPMon, @Error, @Cmd
	Valve Kl3_3, Kl3_3_Prebeh, Kl3_3_Poloha, @Kl3_3_ZAVo, @Kl3_3_OTVo
	

//Vzduchotechnika VZT4 - Foyer
	//Ovládání 
	Let VZT4_D.0 = true  		//přepínač - povolení chodu automatu
	Let VZT4_D.1 = false 		//přepínač - zapnutí VZT ručně
	Let VZT4_D.15 = @VZT4_Res	 	//Reset VZT

	//Řízení VZT
	Let @EPS_VZT=False
	Let @PPK_VZT = False
	Let @DPMonFp = @DT4_1; @DPMonFo = @DT4_2
	Let Nic=0; Emax=0
	fb_VZT VZT4_A, VZT4_D, FMM4_2_D, FMM4_1_D, SP4_D, Kl4_3_D, Chl4_D, Nic, CPY_VZT4, CPC_VZT4, Al_VZT4, Ao_VZT4, Emax, @EPS_VZT, @PPK_VZT, @DPMonFp, @DPMonFo, @Kl4_1_Zo, @Kl4_2_Zo, 511, CP_VZT4
	
	//Přívodní+Odtahový ventilátor
	Let @Reset=@VZT4_Res
	Let FMM4_2_D.15=@Reset
	Let @DPMon=False; @CmdFdb=@FM4_2_Chod; @Error=@FM4_2_Por
	fb_MotorFM FMM4_2_A, FMM4_2_D, CP_VZT4, VZT4_Otacky, @Cmd, @DPMon, @CmdFdb, @Error, FMM4_2_ruc, FMM4_2, 6
	Let @FM4_2_Zo=@Cmd
	
	Let @Reset=@VZT4_Res
	Let FMM4_1_D.15=@Reset
	Let @DPMon=False; @CmdFdb=@FM4_1_Chod; @Error=@FM4_1_Por
	fb_MotorFM FMM4_1_A, FMM4_1_D, CP_VZT4, VZT4_Otacky, @Cmd, @DPMon, @CmdFdb, @Error, FMM4_1_ruc, FMM4_1, 6
	Let @FM4_1_Zo=@Cmd
	
	//Řízení regulace + korekce teploty
	Let Nic = 0
	fb_Regulace VZT4_ZT, VZT4_ZTK, TVenk, TVV_4, TNV_4_2, SP4_D, Chl4_D, Kl4_3_D, Nic, 15
	
	//Teplovodní ohřev + čerpadlo
	Let @Zamraz = @TZ4
	fb_TVohrev SP4_A, SP4_D, CTV4_D, PID_VZT4_Top, VZT4_ZTK, TVV_4, TVenk, TVV_4, SP4_ruc, SP4, @Zamraz
	
	Let @DPMon=False; @CmdFdb=@CTV4_Chod; @ThOvrld=False
	fb_Cerpadlo CTV4_A, CTV4_D, TimeDobeh, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 2
	Let @CTV4_Zo=@Cmd
	
	//Rekuperace
	Let @Error=False; @DPMon=@DT4_3
	Let NicF = 0
	fb_rekup Kl4_3_A, Kl4_3_D, PID_VZT4_Rek, VZT4_ZTK, TVenk, TVV_4, TNV_4_2, NicF, Kl4_3_ruc, Kl4_3, 1, @DPMon, @Error, @Cmd
	Valve Kl4_3, Kl4_3_Prebeh, Kl4_3_Poloha, @Kl4_3_ZAVo, @Kl4_3_OTVo
	
	//Chlazení
	Let @Error = @Chl4_Por
	fb_Chlazeni Chl4_A, Chl4_D, PID_VZT4_Chl, VZT4_ZTK, TVV_4, Chl4_ruc, Chl4, 0, @Error
	Hyst Chl4, @Chl4_1st_Zo, Chl4_Meze[0,0], Chl4_Meze[0,1], 0x0000
	Hyst Chl4, @Chl4_2st_Zo, Chl4_Meze[1,0], Chl4_Meze[1,1], 0x0000

	

]]></Script>
