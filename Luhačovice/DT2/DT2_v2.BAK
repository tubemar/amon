        <CodeBlock Name="fb_chlazeni" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>Chlazení spojité</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Rezim
Let Rezim_D = RezimChlad_D 
Let param = parametry
Let @Automat = RezimChlad_A==1; @Rucne = RezimChlad_A==2

//Regulace
Let PIDrezim.0= !@ProgEn
PID MerenaTep, ZadanaTep, Vystup, PIDrezim, PIDparametry

//Mezni stavy
Let Vystup = IF(@OvrEn, 100, Vystup)
//Vypnutí při nechodu VZT
Let Vystup = IF(@ChodVZT, Vystup, 0)
//Rucni Rezim
Let Vystup = IF(@Rucne, Vystup_ruc, Vystup)
//Mezni stavy
Let Vystup = IF(@EmOff,   0, Vystup)

//Externí porucha
Let @ErrExt = @ErrExtEn AND @PoruchaExt
//Sumární porucha pro VZT
Let @ErrVZT = @ErrExt


//Reset
if @reset, :NONE
  Let @ErrExt = false
endif


Let @NaNule = (Vystup<0.1) OR !@automat //Vystup v nule, nebo neni v autmatu


//Rezim
Let RezimChlad_D = Rezim_D


]]></Script>
