        <CodeBlock Name="fb_Regulace" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>Řízení regulace + korekce teploty</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Parametry
Let RParametry = parametry

//Korekce teploty
if @ZapKorekci ,:NONE
  Let PIDrezim.0 = !RezimOhrev_D.4 //Vypnutí regulace při vypnuté VZT
  PID ZadanaTep, OdtahovaTep, PID_Vystup, PIDrezim, PIDparametry
  Let TD1 = abs(ZadanaTep-OdtahovaTep)
  Let ZadanaTepKor = ZadanaTep + (PID_Vystup * TD1 * KKorekce)
  Limiter ZadanaTepKor, 15, 35  
else
  Let ZadanaTepKor = ZadanaTep
endif


//Vyhodnocení teplot
Let TD1 = ZadanaTepKor - VenkovniTep
Let TD2 = ZadanaTepKor - OdtahovaTep
Hyst TD1, @Bit1, MezeDif[0,0], MezeDif[1,0], 0x0001
Hyst TD2, @Bit2, MezeDif[0,0], MezeDif[1,0], 0x0001
//Zadana<Venkovni a Zadana<Odtahovana -> Chladit
Let @Chladit_poz = @Bit1 AND @Bit2

//Nastavení po startu VZT
fb_Nabeh RezimOhrev_D.4, @Bit1
if @bit1
	Let @Chladit = @Chladit_poz
endif

Let @bit1 = (@Chladit_poz == @Chladit)
if @bit1
	Let Cas_zmeny = C_Z_pomalu
else
	Let Cas_zmeny = C_Z_rychle
endif

//Rekuperace/Topení/Chlazení na nule?
Let @Stop_rekup = (RezimRek2_D.7 OR !@ZapRekup2) AND (RezimRek1_D.7 OR !@ZapRekup1)
Let @Stop_zasah = (RezimOhrev_D.7 OR !@ZapOhrev) AND (RezimChlad_D.7 OR !@ZapChlad)
//Bloky rekuperatorů  (Nerekuperuje nebo jedo naplno nebo neni zapnutý)
Let @Blok_rek1 = (!RezimRek1_D.8 OR RezimRek1_D.9 OR !@ZapRekup1)
Let @Blok_rek2 = (!RezimRek2_D.8 OR RezimRek2_D.9 OR !@ZapRekup2)
//Zastaveni rekuperace
Let @Povol_rekup = (RezimRek1_D.8 AND !RezimRek1_D.9 AND @ZapRekup1)OR(RezimRek2_D.8 AND !RezimRek2_D.9 AND @ZapRekup2)	//Spuštení rekuperace

//Vypnutí chlazeni
Let @Bit1 = @Stop_rekup AND (RezimChlad_D.7 OR !@ZapChlad) AND @chladit
TimerOn @Bit1, Cas_zmeny, @Bit2, NONE
if @Bit2
  Let @chladit = false
endif
//Zapnutí chlazení
Let @Bit1 = @Stop_rekup AND (RezimOhrev_D.7 OR !@ZapOhrev) AND !@chladit
TimerOn @Bit1, Cas_zmeny, @Bit2, NONE
if @Bit2
  Let @chladit = true
endif



//Povoleni topeni (podle teplot A při chodu VZT A (když nejde rekuperace NEBO jde ale naplno NEBO při zakázaný rekuperaci)
Let RezimOhrev_D.0 = (!@chladit AND RezimOhrev_D.4)AND @Blok_rek1 AND @Blok_rek2
Let RezimOhrev_D.6 = @Povol_rekup 
//Povoleni chlazeni (podle teplot A při chodu VZT A (když nejde rekuperace NEBO jde ale naplno NEBO při zakázaný rekuperaci)
Let RezimChlad_D.0 = (@chladit AND RezimChlad_D.4) AND @Blok_rek1 AND @Blok_rek2
Let RezimChlad_D.6 = @Povol_rekup



//Omezení teploty na přívodu VZT
Hyst PrivodniTep, @minimum, PrivodMeze[0,0], 2.000, 0x0001	//minimum
Hyst PrivodniTep, @maximum, PrivodMeze[1,0], 2.000, 0x0000	//maximum

//Povely ohřevu
Let RezimOhrev_D.1 = @minimum AND RezimOhrev_D.4	//Ovrstart - povel na 100%
Let RezimOhrev_D.2 = @maximum AND RezimOhrev_D.4	//Emergenci off - povel na 0%

//Povely chlazení
Let RezimChlad_D.1 = @maximum AND RezimChlad_D.4	//Ovrstart - povel na 100%
Let RezimChlad_D.2 = (@minimum AND RezimChlad_D.4) OR (RezimOhrev_D.11 AND @ZapOhrev)	//Emergenci off - povel na 0% (při nízké teplotě přívodu nebo zámrazu na ohřívači)


//Povely pro rekuperaci1
//Odblokovani rekuperace ktera jede naplno při nulovém akčním zásahu na topení i na chladu
Let RezimRek1_D.0  = @Blok_rek2 	//Povolení chodu
Let RezimRek1_D.6  = @Stop_zasah	//Odblokování
Let RezimRek1_D.13 = @chladit	//Příkaz chladit/topit

//Povely pro rekuperaci2
Let RezimRek2_D.0  = true 							//Povolení chodu
Let RezimRek2_D.6  = @Stop_zasah AND (RezimRek1_D.7 OR !@ZapRekup1)	//Odblokování
Let RezimRek2_D.13 = @chladit						//Příkaz chladit/topit








]]></Script>
          </Body>
