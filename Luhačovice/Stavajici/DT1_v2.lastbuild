          <Comment>Regulace UT, TUV</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Povolení topení od venkovní teploty (filtrované)
Filtr1R TVenk, TVenk_F, TVenk_FK

//ÚT1 - Podlahové topení 1
if @UT1_AUT
  if @UT1_ZAP_CP
    Let TTV1_Z=TTV1_Z_Kom
  else
    Let TTV1_Z=TTV1_Z_Utl
  endif
else
  if @UT1_Kom
    Let TTV1_Z=TTV1_Z_Kom
  else
    Let TTV1_Z=TTV1_Z_Utl
  endif
endif

Hyst TVenk_F, @EnUT1, TVenk_EnT[0,0], TVenk_EnT[1,0], 0x0001

EqLine TTV1_Z, TVenk, TTV1_Z_Ekv, UT1_EkvK
//Let TTV1_Z_Ekv=if(@EnUT1,TTV1_Z_Ekv,20)

Let SP_1_D.0=True
fb_Ventil SP_1_A, SP_1_D, PID_UT1, TTV1_Z_Ekv, TTV_1, SP_1_ruc, SP_1, 0

Let @ProgEn=(SP_1!=0); @ProgSt=(SP_1!=0); @Reset=@UT_Res
Let CTV1_D.0=@ProgEn; CTV1_D.1=@ProgSt; CTV1_D.15=@Reset
Let @DPMon=False; @CmdFdb=@CTV1_Chod; @ThOvrld=False
fb_Cerpadlo CTV1_A, CTV1_D, TimeDobeh, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 2
Let @CTV1_Zo=@Cmd

//ÚT2 - Statický topný systém
if @UT2_AUT
  if @UT2_ZAP_CP
    Let TTV2_Z=TTV2_Z_Kom
  else
    Let TTV2_Z=TTV2_Z_Utl
  endif
else
  if @UT2_Kom
    Let TTV2_Z=TTV2_Z_Kom
  else
    Let TTV2_Z=TTV2_Z_Utl
  endif
endif

Let TCP_UT2_Max = Max(TCP_1, TCP_2, TCP_3)
Hyst TCP_UT2_Max, @EnUT2, TTV2_Z, 0.5, 0x0001

EqLine TTV2_Z, TVenk, TTV2_Z_Ekv, UT2_EkvK
//Let TTV2_Z_Ekv=if(@EnUT2,TTV2_Z_Ekv,20)

Let SP_2_D.0=True
fb_Ventil SP_2_A, SP_2_D, PID_UT2, TTV2_Z_Ekv, TTV_2, SP_2_ruc, SP_2, 0

Let @ProgEn=(SP_2!=0); @ProgSt=(SP_2!=0); @Reset=@UT_Res
Let CTV2_D.0=@ProgEn; CTV2_D.1=@ProgSt; CTV2_D.15=@Reset
Let @DPMon=False; @CmdFdb=@CTV2_Chod; @ThOvrld=False
fb_Cerpadlo CTV2_A, CTV2_D, TimeDobeh, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 2
Let @CTV2_Zo=@Cmd

//ÚT4 - Podlahové topení 2
If @UT4_AUT
  If @UT4_ZAP_CP
    Let TTV4_Z=TTV4_Z_Kom
  else
    Let TTV4_Z=TTV4_Z_Utl
  endif
else
  If @UT4_Kom
    Let TTV4_Z=TTV4_Z_Kom
  else
    Let TTV4_Z=TTV4_Z_Utl
  endif
endif

Let TCP_UT4_Max = Max(TCP_4, TCP_5, TCP_6)
Hyst TCP_UT4_Max, @EnUT4, TTV4_Z, 0.5, 0x0001

EqLine TTV4_Z, TVenk, TTV4_Z_Ekv, UT4_EkvK
//Let TTV4_Z_Ekv=if(@EnUT4,TTV4_Z_Ekv,20)

Let SP_4_D.0=True
fb_Ventil SP_4_A, SP_4_D, PID_UT4, TTV4_Z_Ekv, TTV_4, SP_4_ruc, SP_4, 0

Let @ProgEn=(SP_4!=0); @ProgSt=(SP_4!=0); @Reset=@UT_Res
Let CTV4_D.0=@ProgEn; CTV4_D.1=@ProgSt; CTV4_D.15=@Reset
Let @DPMon=False; @CmdFdb=@CTV4_Chod; @ThOvrld=False
fb_Cerpadlo CTV4_A, CTV4_D, TimeDobeh, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 2
Let @CTV4_Zo=@Cmd



//TUV
If @TUV_AUT
  If @TUV_Z_CPn
    Let TTUV_Z=TTUV_Z_Kom
  else
    Let TTUV_Z=TTUV_Z_Utl
  endif
else
  If @TUV_Kom
    Let TTUV_Z=TTUV_Z_Kom
  else
    Let TTUV_Z=TTUV_Z_Utl
  endif
endif


//Nabíjení TUV
Hyst TAN_1, @TUV1_Z_nab, TTUV_Z, TTUV_Z_Hyst, 0x0001
Hyst TAN_2, @TUV2_Z_nab, TTUV_Z, TTUV_Z_Hyst, 0x0001

//Nabíjecí čerpadlo TUV
Let @ProgEn=(@TUV1_Z_nab OR @TUV2_Z_nab); @ProgSt=(@TUV1_Z_nab OR @TUV2_Z_nab); @EmOff=@TTUV; @Reset=@TUV_Res
Let CTUV1_D.0=@ProgEn; CTUV1_D.1=@ProgSt; CTUV1_D.4=@EmOff; CTUV1_D.15=@Reset
Let @DPMon=False; @CmdFdb=@CTUV1_Chod; @ThOvrld=False
fb_Motor1ST CTUV1_A, CTUV1_D, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 2
Let @CTUV1_Zo=@Cmd

//Čerpadlo cirkulace TUV
Let @ProgEn=@TUV_Z_CPc; @ProgSt=@TUV_Z_CPc; @EmOff=@TTUV; @Reset=@TUV_Res
Let CTUV2_D.0=@ProgEn; CTUV2_D.1=@ProgSt; CTUV2_D.4=@EmOff; CTUV2_D.15=@Reset
Let @DPMon=False; @CmdFdb=@CTUV2_Chod; @ThOvrld=False
fb_Motor1ST CTUV2_A, CTUV2_D, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 2
Let @CTUV2_Zo=@Cmd

//Ventil TUV primár - určení minimálního požadavku
Let SP_7_D.0 = True
fb_Ventil SP_7_A, SP_7_D, PID_TUV, TTUV_Z, TTUV_3, SP_7_ruc, SP_7_Max, 0
Let SP_7_D.0 = True
fb_Ventil SP_7_A, SP_7_D, PID_TUV, TTUV_Z, TTUV_2, SP_7_ruc, SP_7_Poz, 0

Let SP_7 = Min(SP_7_Max, SP_7_Poz)

//Čerpadlo TUV primár
Let @ProgEn = (SP_7 != 0); @ProgSt = (SP_7 != 0); @EmOff = False; @Reset = @TUV_Res
Let CTV7_D.0 = @ProgEn; CTV7_D.1 = @ProgSt; CTV7_D.4 = @EmOff; CTV7_D.15 = @Reset
Let @DPMon = False; @CmdFdb = @CTV7_Chod; @ThOvrld = False
fb_Cerpadlo CTV7_A, CTV7_D, TimeDobeh, @Cmd, @DpMon, @CmdFdb, @ThOvrld, 2
Let @CTV7_Zo = @Cmd


//Požadovaná teplota pro kotelnu
Let TZ_Kot_DT1 = Max (TTV1_Z*INT(@CTV1_Zo), TTV2_Z*INT(@CTV2_Zo), TTV4_Z*INT(@CTV4_Zo), TTUV_Z*INT(@CTV7_Zo))]]></Script>
          </Body>
