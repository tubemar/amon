        <CodeBlock Name="fb_VZT" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>Řízení VZT</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[// Parametry
Let Param=Parametry

// Režim VZT
Let @Automat=(RezimVZT_A==1); @Vypnuto=(RezimVZT_A==2); @Zapnuto=(RezimVZT_A>=3)
// Dig. Režim 
Let Rezim_D=RezimVZT_D


//Casovy program
Let CP_stupen = 0
DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0000, Svatky, CP_casy, CP_hodnoty, CP_stupen
Let @CP_poz = (CP_stupen!=0)

//Stupen otáček
Let Stupen = IF(@Automat AND @Start, CP_stupen   , 0)
Let Stupen = IF(@Zapnuto AND @Start, RezimVZT_A-2, Stupen)


//Emax
Let @EmaxVyp = @EmaxEn AND (Emax>=EmaxVyp)

//Zapnuti VZT
Let @Start = ((@ProgEn AND !@Vypnuto AND ((@CP_poz AND @Automat) OR @Zapnuto)) OR @OvrEn) AND !@EmaxVyp AND !@EmOff AND !@Odstaveno

//Zimni start
//povolení chodu VZT od teplovodního ohřívače, je-li aktivován
Let @Chod = @Start AND (RezimOhrev_D.8 OR !@OhrevEn) 

//EPS
Let @ErrEPS = @EPS AND @EpsEn
//PPK
Let @ErrPPK = @PPK AND @PpkEn

//Privodni Klapka
Let @KlapkaP=@Start OR RezimVp_D.5 //Otevření klapky od startu VZT nebo povelu na ventilátor

//Odtahova Klapka
Let @KlapkaO=@Start OR RezimVo_D.5 //Otevření klapky od startu VZT nebo povelu na ventilátor

//Privodni Ventilator
Let RezimVp_D.4=@Odstaveno; RezimVp_D.0=@Chod
TimerOn @Start,CasKlapkyP,@Bit1,TimerOut
Let @Chod_V1     = @Bit1 AND @Chod
Let RezimVp_D.1  = @Chod_V1
Let RezimVp_D.15 = @Reset
//
Let @ErrVp_pd  = RezimVp_D.7 AND @VpEn
Let @ErrVp_cmd = RezimVp_D.8 AND @VpEn
Let @ErrVp_to  = RezimVp_D.9 AND @VpEn


//Odtahovy Ventilator
Let RezimVo_D.4=@Odstaveno; RezimVo_D.0=@Chod
TimerOn @Start,CasKlapkyO,@Bit1,TimerOut
Let @Chod_V2     = @Bit1 AND @Chod
Let RezimVo_D.1  = @Chod_V2
Let RezimVo_D.15 = @Reset
//
Let @ErrVo_pd  = RezimVo_D.7 AND @VoEn
Let @ErrVo_cmd = RezimVo_D.8 AND @VoEn
Let @ErrVo_to  = RezimVo_D.9 AND @VoEn


//Privodni filtr
If @FpEn,:NONE
  timeron @DpFp,TimerDpF,@Bit1,TimerOut
  if @Bit1,:NONE
    let @ErrFp=true
  endif
else
  let @ErrFp=false
endif

//Odtahovy filtr
If @FoEn,:NONE
  timeron @DpFo,TimerDpF,@Bit1,TimerOut
  if @Bit1,:NONE
    let @ErrFo=true
  endif
else
  let @ErrFo=false
endif

//Ohrev
Let RezimOhrev_D.3=@start; RezimOhrev_D.4=@chod; RezimOhrev_D.5=@Odstaveno
Let RezimOhrev_D.15=@reset
Let @ErrZamraz   = RezimOhrev_D.11 AND @OhrevEn
Let @ErrCerpadlo = RezimOhrev_D.12 AND @OhrevEn

//Rekuperace1
Let RezimRek1_D.3=@start; RezimRek1_D.4=@chod; RezimRek1_D.5=@Odstaveno
Let RezimRek1_D.15=@reset
Let @ErrRek1Zam = RezimRek1_D.11 AND @Rekup1En
Let @ErrRek1Ext = RezimRek1_D.12 AND @Rekup1En

//Rekuperace2
Let RezimRek2_D.3=@start; RezimRek2_D.4=@chod; RezimRek2_D.5=@Odstaveno
Let RezimRek2_D.15=@reset
Let @ErrRek2Zam = RezimRek2_D.11 AND @Rekup2En
Let @ErrRek2Ext = RezimRek2_D.12 AND @Rekup2En

//Chlazeni
Let RezimChlad_D.2=@ErrZamraz; RezimChlad_D.3=@start; RezimChlad_D.5=@Odstaveno
Let RezimChlad_D.4=RezimVp_D.6 //@chod_V1 //Chod přívodního ventilátoru
Let RezimChlad_D.15=@reset
Let @ErrChlad=RezimChlad_D.10 AND @ChladEn



//Odstaveni
Let @Odstaveno= (Poruchy&Poruchy_mask) != 0

//Reset
if @Reset,:NONE
Let Poruchy=0 
endif


//Výstupy
Let RezimVZT_D=Rezim_D
//Poruchy
Let Poruchy_out=Poruchy
]]></Script>
