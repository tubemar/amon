        <CodeBlock Name="fb_Eohrev" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>Teplovodni ohrev</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Rezim
Let Rezim_D=RezimOhrev_D
Let @Automat=(RezimOhrev_A==1); @Rucne=(RezimOhrev_A==2)

//Regulace
Let PIDrezim.0 = !@ProgEn  //; PIDrezim.1 = !@ProgEn
PID ZadanaTep, MerenaTep, Vystup, PIDrezim, PIDparametry

//Povoleni startu VZT bez zimniho startu
Let @PovolVZT = true

//Rucni rezim
Let Vystup = IF(@Rucne, Vystup_ruc, Vystup)

//Stavy
Let Vystup = IF(@OvrEn, 100, Vystup)
Let Vystup = IF(@EmOff,   0, Vystup)

//Reset
if @reset,:NONE
  Let @ErrVZT=false
endif

//Rezim
Let RezimOhrev_D=Rezim_D


]]></Script>
          </Body>
