        <CodeBlock Name="Rezimy_CP" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Režimy, Čas. prog</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Kotelna
Let @Kotle_AUT=(Kotelna_A==1); @Kotle_VYP=(Kotelna_A==2); @Kotle_ZAP=(Kotelna_A==3)

//Čerpadla TV
Let @CTVS_AUT=(CTVS_A==1); @CTVS_VYP=(CTVS_A==2); @CTVS_ZAP=(CTVS_A==3)

//TUV
Let @TUV_AUT=(TUV_A==1); @TUV_Utl=(TUV_A==2); @TUV_Kom=(TUV_A==3)
DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0000, CP_Svatky, CPC_TUV_nab, CPY_TUV_nab, CP_TUV_nab
Let @TUV_Z_CPn=(CP_TUV_nab!=0)
DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0000, CP_Svatky, CPC_TUV_cir, CPY_TUV_cir, CP_TUV_cir
Let @TUV_Z_CPc=(CP_TUV_cir!=0)

//UT 1,2,3
Let @UT1_AUT=(UT1_A==1); @UT1_Utl=(UT1_A==2); @UT1_Kom=(UT1_A==3)
DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0000, CP_Svatky, CPC_UT1, CPY_UT1, CP_UT1
Let @UT1_ZAP_CP=(CP_UT1!=0)

Let @UT2_AUT=(UT2_A==1); @UT2_Utl=(UT2_A==2); @UT2_Kom=(UT2_A==3)
DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0000, CP_Svatky, CPC_UT2, CPY_UT2, CP_UT2
Let @UT2_ZAP_CP=(CP_UT2!=0)

Let @UT4_AUT=(UT4_A==1); @UT4_Utl=(UT4_A==2); @UT4_Kom=(UT4_A==3)
DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0000, CP_Svatky, CPC_UT4, CPY_UT4, CP_UT4
Let @UT4_ZAP_CP=(CP_UT4!=0)

//Konvektory a FCU
DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0000, CP_Svatky, CPC_Konv, CPY_Konv, CP_Konv
Let @Konv_ZAP_CP=(CP_Konv!=0)
Let @Konv_AUT=(Konv_A==1); @Konv_VYP=(Konv_A==2); @Konv_1st=(Konv_A==3); @Konv_2st=(Konv_A==4); @Konv_3st=(Konv_A==5)

DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0000, CP_Svatky, CPC_FCU, CPY_FCU, CP_FCU
Let @FCU_ZAP_CP=(CP_FCU!=0)
Let @FCU_AUT=(FCU_A==1); @FCU_VYP=(FCU_A==2); @FCU_1st=(FCU_A==3); @FCU_2st=(FCU_A==4); @FCU_3st=(FCU_A==5)

]]></Script>
