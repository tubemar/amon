            <Script><![CDATA[//Vzduchotechnika VZT9 - Topný zdroj
//Ovládání 
Let VZT9_D.0 = true  		//přepínač - povolení chodu automatu
Let VZT9_D.1 = false 		//přepínač - zapnutí VZT ručně
Let VZT9_D.15 = @VZT9_Res	 	//Reset VZT

//Řízení VZT
Let @EPS_VZT=False
Let @PPK_VZT = False
Let @DPMonFp = @DP9; @DPMonFo = False
Let Nic=0; Emax=0
fb_VZT VZT9_A, VZT9_D, V9_1_D, V9_2_D, SP_9_D, Nic, Nic, Nic, CPY_VZT9, CPC_VZT9, Al_VZT9, Ao_VZT9, Emax, @EPS_VZT, @PPK_VZT, @DPMonFp, @DPMonFo, @Kl9_Zo, @KlapkaO, 159, CP_VZT9

//Přívodní+Odtahový ventilátor
Let @Reset=@VZT9_Res
Let V9_1_D.15=@Reset
Let @DPMon=@DP9_1; @CmdFdb=False; @ThOvrld=False
fb_Motor1ST V9_1_A, V9_1_D, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 1
Let @V9_1_Zo=@Cmd
PID TR_K, TR_K_Z, V9_1, PID_R_FM9, PID_P_FM9
Let V9_1 = if (V9_1_A == 3, V9_1_ruc, V9_1)

Let @Reset=@VZT9_Res
Let V9_2_D.15=@Reset
Let @DPMon=False; @CmdFdb=@V9_2_Chod; @ThOvrld=False
fb_Motor1ST V9_2_A, V9_2_D, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 2
Let @V9_2_Zo=@Cmd

//Řízení regulace + korekce teploty
Let Nic = 0
fb_Regulace VZT9_ZT, VZT9_ZTK, TVenk, TCVV_9, TCVV_9, SP_9_D, Nic, Nic, Nic, 2

//Teplovodní ohřev + čerpadlo
Let @Zamraz = @TZ9
fb_TVohrev SP_9_A, SP_9_D, CTV9_D, PID_VZT9_Top, VZT9_ZTK, TCVV_9, TVenk, TVV_9, SP_9_ruc, SP_9, @Zamraz

Let @DPMon=False; @CmdFdb=@CTV9_Chod; @ThOvrld=False
fb_Cerpadlo CTV9_A, CTV9_D, TimeDobeh, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 2
Let @CTV9_Zo=@Cmd


//Vzduchotechnika VZT7 - Místnost plavčíka
//Ovládání 
Let VZT7_D.0 = true  		//přepínač - povolení chodu automatu
Let VZT7_D.1 = false 		//přepínač - zapnutí VZT ručně
Let VZT7_D.2 = @TZ7 			//EmOff - Zámraz VZT
Let VZT7_D.15 = @VZT7_Res	 	//Reset VZT

//Řízení VZT
Let @EPS_VZT=False
Let @PPK_VZT = False
Let @DPMonFp = @DP7; @DPMonFo = False
Let Nic=0; Emax=0
fb_VZT VZT7_A, VZT7_D, V7_1_D, V7_2_D, EO_7_D, Nic, Nic, Nic, CPY_VZT7, CPC_VZT7, Al_VZT7, Ao_VZT7, Emax, @EPS_VZT, @PPK_VZT, @DPMonFp, @DPMonFo, @KlapkaP, @KlapkaO, 159, CP_VZT7

//Přívodní+Odtahový ventilátor
Let @Reset=@VZT7_Res
Let V7_1_D.15=@Reset
Let @DPMon=False; @CmdFdb=@V7_1_Chod; @ThOvrld=False
fb_Motor1ST V7_1_A, V7_1_D, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 2
Let @V7_1_Zo=@Cmd

Let @Reset=@VZT9_Res
Let V7_2_D.15=@Reset
Let @DPMon=False; @CmdFdb=@V7_2_Chod; @ThOvrld=False
fb_Motor1ST V7_2_A, V7_2_D, @Cmd, @DPMon, @CmdFdb, @ThOvrld, 2
Let @V7_2_Zo=@Cmd

//Řízení regulace + korekce teploty
Let Nic = 0
fb_Regulace VZT7_ZT, VZT7_ZTK, TVenk, TCVV_7, TCVV_7, EO_7_D, Nic, Nic, Nic, 2

//Elektroohřev
fb_Eohrev EO_7_A, EO_7_D, PID_VZT7_Top, VZT7_ZT, TCVV_7, TVenk, EO_7_ruc, EO_7
Hyst EO_7, @EO7_1st_Zo, EO_7_Meze[0,0], EO_7_Meze[0,1], 0x0000
Hyst EO_7, @EO7_2st_Zo, EO_7_Meze[1,0], EO_7_Meze[1,1], 0x0000
Hyst EO_7, @EO7_3st_Zo, EO_7_Meze[2,0], EO_7_Meze[2,1], 0x0000


//Požadavek na topení
Let @VZT = @CTV9_Zo]]></Script>
          </Body>
