        <CodeBlock Name="Ruzne" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Detekce chloru, ...</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Detekce chlóru
	Hyst QZA_300, @QZA_1st, QZA_meze[0,0], QZA_meze[2,0], 0x0000
	Hyst QZA_300, @QZA_2st, QZA_meze[1,0], QZA_meze[2,0], 0x0000
	Let @QZA_SvSig = @QZA_1st
	Let @QZA_Sirena = @QZA_2st
	
////Dopouštění systému TV - zrušeno
//Hyst PSSV, @SVDS_Poz, PSSV_Meze[0,0], PSSV_Meze[1,0], 0x0001
//Let @SVDS_Zo = @SVDS_Poz AND !@SVDS_Err
//TimerOn @SVDS_Zo, TimeDopoust, @SVDS_Err, Timerout
//Let @SVDS_Err = if(@Dopoust_Res, False, @SVDS_Err)

//Vyhřívání vpustí
	Hyst TVenk, @Vpuste_Poz, Vpuste_T_Mez[0,0], Vpuste_T_Mez[1,0], 0x0001
	Let @Vpuste_Zo = if(Vpuste_A !=1 , False, @Vpuste_Poz)
	

//Řízení podlahových konvektorů v hale
	Let Konv_TZ = if(@Konv_ZAP_CP, Konv_TZ_Kom, Konv_TZ_Utl)
	Let Konv_T_Min = MIN(TCP_2, TCP_3)
	PID Konv_TZ, Konv_T_Min, Konv, PID_R_Konv, PID_P_Konv
	if @Konv_AUT
		Hyst Konv, @Konv_1st_Po, Konv_ST[0,0], Konv_ST[0,1], 0x0000
		Hyst Konv, @Konv_2st_Po, Konv_ST[1,0], Konv_ST[1,1], 0x0000
		Hyst Konv, @Konv_3st_Po, Konv_ST[2,0], Konv_ST[2,1], 0x0000

		Let @Konv_1ST_Zo = @Konv_1st_Po AND !@Konv_2st_Po AND !@Konv_3st_Po
		Let @Konv_2ST_Zo = @Konv_2st_Po AND !@Konv_3st_Po
		Let @Konv_3ST_Zo = @Konv_3st_Po
	  else
		Let @Konv_1ST_Zo = @Konv_1ST	
		Let @Konv_2ST_Zo = @Konv_2ST	
		Let @Konv_3ST_Zo = @Konv_3ST	
	endif

	Let @Konv_Y_OTVo = @Konv_1ST_Zo OR @Konv_2ST_Zo OR @Konv_3ST_Zo

//Řízení FCU na chodbě
	Let FCU_TZ = if(@FCU_ZAP_CP, FCU_TZ_Kom, FCU_TZ_Utl)
	PID FCU_TZ, TCP_4, FCU, PID_R_Konv, PID_P_Konv
	if @FCU_AUT
		Hyst FCU, @FCU_1st_Poz, FCU_ST[0,0], FCU_ST[0,1], 0x0000
		Hyst FCU, @FCU_2st_Poz, FCU_ST[1,0], FCU_ST[1,1], 0x0000
		Hyst FCU, @FCU_3st_Poz, FCU_ST[2,0], FCU_ST[2,1], 0x0000

		Let @FCU_1ST_Zo = @FCU_1st_Poz AND !@FCU_2st_Poz AND !@FCU_3st_Poz
		Let @FCU_2ST_Zo = @FCU_2st_Poz AND !@FCU_3st_Poz
		Let @FCU_3ST_Zo = @FCU_3st_Poz
	  else
		Let @FCU_1ST_Zo = @FCU_1ST	
		Let @FCU_2ST_Zo = @FCU_2ST	
		Let @FCU_3ST_Zo = @FCU_3ST	
	endif

	Let @FCU_Y_OTVo = @FCU_1ST_Zo OR @FCU_2ST_Zo OR @FCU_3ST_Zo

	]]></Script>
          </Body>
