        <CodeBlock Name="Komunikace" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Komunikace s ostatními rozvaděči</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[
Let cykl_kom = cykl_kom + 1
Let @pom_bit = cykl_kom > cykl_kom_max  //po 5 minutách
If @pom_bit,:NONE
	Let cykl_kom  = 0
	
	//Venkovní teplota
	Let TVenk_DT2 = TVenk; TVenk_DT3 = TVenk
	
	EthReqDb :00002, 0x0001, 2, TVenk_DT2, 1, 1, -1, NONE, NONE
	EthReqDb :00003, 0x0001, 3, TVenk_DT3, 1, 1, -1, NONE, NONE
//	ReqDb 0x0001, TVenk_DT2, 0, 0, NONE, NONE
//	ReqDb 0x0001, TVenk_DT3, 0, 0, NONE, NONE
	
	//Požadavky na topení
	EthReqDb :00002, 0x0000, 2, Poz_Top_DT2, 1, 1, -1, NONE, NONE
	EthReqDb :00003, 0x0000, 3, Poz_Top_DT3, 1, 1, -1, NONE, NONE
//	ReqDb 0x0000, Poz_Top_DT2, 0, 0, NONE, NONE
//	ReqDb 0x0000, Poz_Top_DT3, 0, 0, NONE, NONE
EndIf


//Synchronizace času
Let cykl_cas = cykl_cas + 1
Let @pom_bit = cykl_cas > cykl_cas_max  //po 1 hodině
If @pom_bit,:NONE
	GetTime Cas_DT1, Cas_DT1_MAT, NONE
	Let Cas_DT2 = Cas_DT1; Cas_DT3 = Cas_DT1
		
	EthReqDb :00002, 0x0001, 2, Cas_DT2, 1, 1, -1, NONE, NONE
	EthReqDb :00003, 0x0001, 3, Cas_DT3, 1, 1, -1, NONE, NONE
	EthReqDb :00004, 0x0001, 4, Cas_DT4, 1, 1, -1, NONE, NONE

//	ReqTime 0x0001, 2, Cas_DT1, NONE, NONE
//	ReqTime 0x0001, 3, Cas_DT1, NONE, NONE
	
EndIf

]]></Script>
          </Body>
          <Period>30000</Period>
          <Offset>0</Offset>
          <Type>10</Type>
          <InterruptEdge>0</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="Kotelna" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Regulace kotelny</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Poruchy v kotelně
RS @Q_2st, @Kotelna_Res, Al_Kotelna.0
RS @NZ_Stop, @Kotelna_Res, Al_Kotelna.1
RS @CZ, @Kotelna_Res, Al_Kotelna.2
Let Al_Kotelna.3 = @TPK
Let Al_Kotelna.4 = @PSTS
Let Al_Kotelna.5 = PSSV_par[0,0].0
Let Al_Kotelna.6 = PSSV_par[0,0].1
//Let Al_Kotelna.7 = @PK1_Por
//Let Al_Kotelna.8 = @PK2_Por
Let Al_Kotelna.9 = @Kotle_Por
//Let Al_Kotelna.10 = @PK4_Por
Let Al_Kotelna.11 = TVK_par[0,0].1

//Signalizace poruchy
Let @HL18 = Al_Kotelna != 0

//Odstavení kotelny 
Let @Kotle_ODS = BOOL(Al_Kotelna & Ao_Kotelna)
Let @Kotle_STOP = @Kotle_ODS

//Požadavek na topení
Let @VZT = @VZT_DT2 OR @VZT_DT3 OR @CTV9_Zo
Let @Baz = @Baz_DT2 OR @Baz_DT3
Let @UT = @CTV1_Zo OR @CTV2_Zo OR @CTV4_Zo

//Žádaná teplota
Interpol TVenk, T_VZT_Z, T_VZT_Int
Interpol TVenk, T_Baz_Z, T_Baz_Int

//HUP + Siréna
Let @HUP = !@Q_2st
Let @Sir = @Q_2st

//Určení požadované teploty
if @Kotle_AUT
  Let TVK_Z=TVK_Nav+Max(TTV1_Z_Ekv*INT(@CTV1_Zo), TTV2_Z_Ekv*INT(@CTV2_Zo), TTV4_Z_Ekv*INT(@CTV4_Zo), T_VZT_Z*INT(@VZT), T_Baz_Z*INT(@Baz), TTUV_Z*INT(SP_7!=0)) //Doplnit přenos požadovaných teplot z ostatních stanic
else
  if @Kotle_ZAP
    Let TVK_Z=TVK_ZAP
  else
    Let @Kotle_Poz=False
    Let TVK_Z=10
  endif
endif

Limiter TVK_Z, 10.000, 90.000

//Ovládání kaskádového modulu
if Kotle_A.0		//Automat
	Interpol TVK_Z, TVK_Z_Kas, Kotle_Int
  else			//Ručně
	Interpol Kotle_ruc, TVK_Z_Kas, Kotle_Int
endif
Let Kotle = if(@Kotle_ODS, 0, TVK_Z_Kas)

//Zrušeno řízení kaskády - obstará kotlový modul
//SyncMark 4, 1, 12, 0, 0, @Kotle_ZMEN, NONE
//CasCon TVK_Z, TVK, @Kotle_STOP, @Kotle_ZMEN, CC_Perioda, CC_MezeT[0,0], CC_MezeT[1,0], CC_Typ_kotlu, CC_Alarmy, CC_Kotle_Zap, CC_Vykon, CC_Provoz, CC_Hladina, CC_params[0,0], CC_params[1,0], CC_params[2,0], CC_params[3,0], CC_params[4,0], CC_Integral

//Kotel1
//	Let K1 = if(K1_A==1, CC_Vykon[0,0],K1_ruc)
//	Let @PK1_Zo = (K1 != 0)
	//Cerpadlo
TimerOff @PK1_Chod, tCerpDobeh, @CK1_Zo, NONE

//Kotel2
//	Let K2 = if(K2_A==1, CC_Vykon[1,0],K2_ruc)
//	Let @PK2_Zo = (K2 != 0)
	//Cerpadlo
TimerOff @PK2_Chod, tCerpDobeh, @CK2_Zo, NONE	

//Kotel3
//	Let K3 = if(K3_A==1, CC_Vykon[2,0],K3_ruc)
//	Let @PK3_Zo = (K3 != 0)
	//Cerpadlo
	Let @DPMon = False; @ThOvrld = False; CK3_D.15 = @Kotelna_Res; CK3_D.4 = @Kotle_STOP
	Let CK3_D.0 = Kotle > 20; CK3_D.1 = Kotle > 20
	fb_Cerpadlo CK3_A, CK3_D, TimeDobeh, @CK3_Zo, @DPMon, @CK3_Chod, @ThOvrld, 2

//Kotel4
//	Let K4 = if(K4_A==1, CC_Vykon[3,0],K4_ruc)
//	Let @PK4_Zo = (K4 != 0)
	//Cerpadlo
	Let @DPMon = False; @ThOvrld = False; CK4_D.15 = @Kotelna_Res; CK4_D.4 = @Kotle_STOP
	Let CK4_D.0 = Kotle > 20; CK4_D.1 = Kotle > 20
	fb_Cerpadlo CK4_A, CK4_D, TimeDobeh, @CK4_Zo, @DPMon, @CK4_Chod, @ThOvrld, 2


//Vyjmutí ovládání kotlů z CASCON
//Let CC_Alarmy[0,0].0 = @PK1_por OR (K1_A != 1)
//Let CC_Alarmy[1,0].0 = @PK2_por OR (K2_A != 1)
//Let CC_Alarmy[2,0].0 = @PK3_por OR (K3_A != 1)
//Let CC_Alarmy[3,0].0 = @PK4_por OR (K4_A != 1)


//Čerpadla topného okruhu
Let @CTVS_Poz=(@CK1_Zo OR @CK2_Zo OR @CK3_Zo OR @CK4_Zo OR @CTVS_ZAP) AND !@CTVS_VYP
TimerOff @CTVS_Poz, TimeDobeh, @CTVS_Start, TimerOut

Let @Cerp_Ods=@Kotle_ODS

Stand_in @CTVS_Start, @pom_bit1, @pom_bit2, CTVS1_D.10, @Cerp_Ods, CTVS2_D.10, @Cerp_Ods, TimeOper, 0, 1, 0x007F, 43200

Let CTVS1_D.0=@pom_bit1; CTVS1_D.1=@pom_bit1
Let CTVS2_D.0=@pom_bit2; CTVS2_D.1=@pom_bit2

Let @DPMon = False; @ThOvrld = False
Let CTVS1_D.15=@Kotelna_Res
fb_Motor1ST CTVS1_A, CTVS1_D, @CTVS1_Zo, @DPMon, @CTVS1_Chod, @ThOvrld, 2

Let @DPMon = False; @ThOvrld = False
Let CTVS2_D.15=@Kotelna_Res
fb_Motor1ST CTVS2_A, CTVS2_D, @CTVS2_Zo, @DPMon, @CTVS2_Chod, @ThOvrld, 2

]]></Script>
