        <CodeBlock Name="fb_Ventil" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>Rizeni ventilu</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Rezim
Let Rezim_D=RezVentil_D
Let @Automat=(RezVentil_A==1); @Rucne=(RezVentil_A==2)

//Regulace
Let PIDrezim.0 = !@ProgEn  //; PIDrezim.1 = !@ProgEn
PID ZadanaTep, MerenaTep, Vystup, PIDrezim, PIDparametry


//Omezení teploty
//minimum
//Hyst MerenaTep, @Bit1, TepMeze[0,0], TepMeze[2,0], 0x0001
//Let Vystup = IF(@Bit1, 100, Vystup)
//maximum
//Hyst MerenaTep, @Bit1, TepMeze[1,0], TepMeze[2,0], 0x0000
//Let Vystup = IF(@Bit1, 0, Vystup)

Let @Bit1 = inverzne!=0
Let Vystup = IF(@bit1, 100-Vystup, Vystup)

//Rucni rezim
Let Vystup = IF(@Rucne, Vystup_ruc, Vystup)

//Rezim
Let RezVentil_D=Rezim_D


]]></Script>
