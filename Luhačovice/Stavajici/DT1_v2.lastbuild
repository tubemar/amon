        <CodeBlock Name="fb_TVohrev" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>Teplovodni ohrev</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[//Režim
Let Rezim_D=RezimOhrev_D
Let @Automat=(RezimOhrev_A==1); @Rucne=(RezimOhrev_A==2)

//Regulace ohřevu
Let PIDrezim.0 = !@ProgEn  //; PIDrezim.1 = !@ProgEn
PID ZadanaTep, MerenaTep, Vystup_PID1, PIDrezim, PIDparametry
//Doběh ohřevu
if @StopRekup
	Filtr1R Vystup_PID1, Vystup, Cas_Filtru
else
	Let Vystup = Vystup_PID1
endif


//Regulace zpátečky
Hyst VenkovniTep, @Bit1, 10.000, 2.000, 0x0001
Let @TZ_poz = @Bit1 AND !@TZ_vyp
//Let @ZR_poz = @Bit1 AND @ZR_zap 

Let PIDrezim.0 = false
PID     TVmin,   VodaTep, Vystup_PID2, PIDrezim, PIDparametry
if @TZ_poz
	Let Vystup = MAX(Vystup, Vystup_PID2)
endif





//Zimni start VZT
TimerOn @startVZT, 10000,@Bit1,TimerOut    
TimerOn @startVZT, ZimniStart,@Bit2,TimerOut    
If @startVZT,:NONE
  if @chodVZT,:NONE
    Let @PovolVZT = !@zamrznuto
  else
    Let @PovolVZT = ((@Bit1 AND (@Bit2 OR (VodaTep>TVstart))) OR(VenkovniTep>LetniStart))AND(!@Zamrznuto)
    Let Vystup=IF(!@PovolVZT, 100, Vystup)
  endif
else
  Let @PovolVZT=false
endif



//Rucni rezim
Let Vystup = IF(@Rucne, Vystup_ruc, Vystup)

//Stavy
Let Vystup = IF(@OvrEn, 100, Vystup)
Let Vystup = IF(@EmOff,   0, Vystup)


//Porucha zámraz
Let @Bit1=@Zamraz OR (VodaTep<TVzamraz)
if @Bit1,:NONE
  Let @Zamrznuto=true
endif
Let @ErrZamraz = @Zamrznuto
Let Vystup = IF(@zamrznuto, 100, Vystup)


//Čerpadlo
Let @Bit1 = Vystup > 0.5
TimerOff @Bit1,DobehCerp,@Bit2,TimerOut
Let @CerpadloPoz = @Bit2 OR @Zamrznuto //OR @ZR_poz
Let RezimCerp_D.0 = @CerpadloPoz
Let RezimCerp_D.1 = @CerpadloPoz
//Havarijni spusteni čerpadla při zámrazu
Let RezimCerp_D.2 = @Zamrznuto
Let RezimCerp_D.3 = @Zamrznuto
//Porucha od čerpadla
Let @ErrCerp = RezimCerp_D.10
//reset čerpadla
Let RezimCerp_D.15=@reset 


//Porucha pro VZT od zámrazu nebo poruchy čerpadla
Let @ErrVZT = @Zamrznuto OR @ErrCerp 


//Reset
if @reset,:NONE
  Let @Zamrznuto=false
  Let @ErrZamraz=false
  Let @ErrVZT=false
endif


Let @NaNule = (Vystup<0.1)OR !@automat //Vystup v nule, nebo neni v automatu

//Rezim
Let RezimOhrev_D=Rezim_D



]]></Script>
