        <CodeBlock Name="libAMR_RDO0" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Zápis na digitální výstupy AMR-RDO0</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//Digitální výstupy
	
	//*************************************************
	//******************* KANÁL RDO0 *******************
	//*************************************************
	
	//Přepnutí AUTOMATICKÝ nebo MANUÁLNÍ režim
	Let AR_DO0 = (~AR_DO0_XR & AR_DO0_A) | (AR_DO0_XR & AR_DO0_R)
	//Beznárazové přepnuti do ručního režimu  
	Let AR_DO0_R = AR_DO0
	
	//*************** Zápis na digitální výstupy kanálu DO0 ***************
	ARI_DigOut 2, 0, 2, AR_DO0, 0x00000000
	
	
]]></Script>
          </Body>
