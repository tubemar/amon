        <CodeBlock Name="fb_TUV_ohaku_leg" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>
          </Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[// Verze 1.00
// Doplněno přehřívání TUV kvůli Legionelle
// Vstupně výstupní parametry do vnitřních proměnných
Let tuvStav = ioStav[Index, 0]
Let RV_rezim = ioRV_rezim[Index, 0]

// V případě odstavení Vyp, jinak podle povelu
// Default režim Aut
Let @xVyp = !@iZap or   @iStop
Let @xZap =  @iZap and  @iMode and !@iStop
Let @xAut =  @iZap and !@iMode and !@iStop

// Žádaná zásobníku
If @iReqLeg
	Let Tzz = if(@xVyp, 5.0, iTzz[Index, 1])
Else
	If @xAut, :NONE
		DayPlan 0x0000, 8, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0080, iSvatky, iCP_casy, iCP_tepl, fx
		Let Tzz = max(5.0, fx)
	Else :NONE
		Let Tzz = if(@xVyp, 5.0, iTzz[Index, 0])
	EndIf
Endif

TimerOff @iReqLeg, iLegActDelay[Index, 0], @oLegAct, NONE 

// Věrohodnost měření teploty zásobníku
Let @Tz_FAKU = (iTzm[Index, 0] < -30) or (iTzm[Index, 0] > 130)
Let @Tz_FOhr = (iTom[Index, 0] < -30) or (iTom[Index, 0] > 130)

// Havarijní meze teploty zásobníku
Let @Tz_LL = ((iTzm[Index, 0] < Tz_meze[0, 0]) or @Tz_LL) and !((iTzm[Index, 0] > (Tz_meze[0, 0] + Tz_hyst)) or @Tz_FAKU)
Let @Tz_HH = !@iTemp_OK or ((iTzm[Index, 0] > Tz_meze[if(@oLegAct,2,1), 0]) or @Tz_HH) and !((iTzm[Index, 0] < (Tz_meze[if(@oLegAct,2,1), 0] - Tz_hyst)) or @Tz_FAKU)

// Regulace teploty zásobníku
Let Ttuvo = iTom[Index, 0]
Let Ttuva = iTzm[Index, 0]

// Upravená žádaná pro výstup z ohřívače
Let dtTUV = dtTUV + (Tzz-Ttuva)/I_Konst
Limiter dtTUV, dtLim[0,0], dtLim[1,0]
If @xVyp
	Let dtTUV = 0
Endif
Let oToz[Index, 0] = if(@xVyp, Tzz, Tzz + dtTUV)

// Ruční režim regulátoru
Let RV_rezim.1 = @iRV_ruc or @iStop or @Tz_HH or @Tz_FOhr or (@xVyp and !@Tz_LL)

// Výpočet akčního zásahu
Let fx = ioRV_akce[Index, 0]
PID oToz[Index, 0], Ttuvo, fx, RV_rezim, iRV_PID

// Akční zásah v ručním režimu
If RV_rezim.1, :NONE
	If @iRV_ruc, :NONE
		// V ručním režimu externě zapsaný zásah
	Else :NONE
		Let ix.0 = @iStop or @Tz_HH or @xVyp or @Tz_FOhr
		If ix.0, :NONE
			// Při vypnutí, vysoké teplotě a v létě mimo protočení zavřeno
			Let ioRV_akce[Index, 0] = 0.0
		EndIf
	EndIf
Else :NONE
	// Pokud není ruční režim, akční zásah určený modulem PID
 	Let ioRV_akce[Index, 0] = fx
EndIf

Let fx = 0.5 * RV_cerp[1, 0]
Let @RV_AnC = ((ioRV_akce[Index, 0] > (RV_cerp[0, 0] + fx)) or @RV_AnC) and !((ioRV_akce[Index, 0] < (RV_cerp[0, 0] - fx)) or @iRV_ruc)
// Doběh čerpadla
TimerOff @RV_AnC, tCerpDobeh, @RV_AnC_tof, NONE

// Požadavek na spuštění čerpadla z mimoregulačních důvodů
Let @eCerpZap = (@iCerpZap or @Tz_LL) and !@iStop

// Řízení čerpadla nabíjení
Let @oCNabZap = ((@RV_AnC_tof and !@xVyp) or (@eCerpZap and !@iStop)) and !@Tz_HH

// Řízení čerpadla cirkulace (při žádané > 5°C)
Let @oCCirkZap = (!@xVyp or @iCerpZap) and (Tzz > 5.5) and !@iStop

// Zápis do vstupně výstupních parametrů
Let ioStav[Index, 0] = tuvStav
Let ioRV_rezim[Index, 0] = RV_rezim
Let oTzz[Index, 0] = Tzz]]></Script>
          </Body>
          <DBase>
