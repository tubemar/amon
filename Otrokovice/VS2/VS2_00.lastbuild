            </FBParam>
            <FBParam Name="@iCerpZap" Class="Amit.Psp4.Shared.FBParamUni">
              <PinType>0</PinType>
              <Comment>Externí požadavek spuštění čerpadla</Comment>
              <DataType>6</DataType>
            </FBParam>
            <FBParam Name="@oCNabZap" Class="Amit.Psp4.Shared.FBParamUni">
              <PinType>0</PinType>
              <Comment>Čerpadlo nabíjení, zapnutí</Comment>
