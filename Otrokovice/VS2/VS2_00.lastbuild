        <CodeBlock Name="fb_TV_smes_teplozl" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>Topná větev směšovaná</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[// Verze 1.02
// 2009-10-13
// Požadavek na maximální teplotu se předává pouze PID regulátoru,
// do parametru oTtvz se zapisuje vypočítaná ekvitermní teplota.
// Verze 1.01
// 2009-07-09
// Místo požadavku na maximální otevření ventilu požadavek na maximální teplotu topné vody.
// 2009-07-08
// Úprava protáčení pohonů. Nejprve se otevře a zavře ventil, teprve pak se pouští čerpadlo.
// Úprava určení žádané hodnoty topné vody při vypnutí.
// 2009-07-02
// Meze teploty topné vody jako parametr.

// Vstupně výstupní parametry do vnitřních proměnných
Let tvStav = ioStav[Index, 0]
Let RV_rezim = ioRV_rezim[Index, 0]

// Věrohodnost měření teploty topné vody
Let @Ttv_F = (iTtvm[Index, 0] < -30) or (iTtvm[Index, 0] > 130)
// Havarijní meze teploty topné vody
Let @Ttv_LL = ((iTtvm[Index, 0] < iTtv_meze[Index, 0]) or @Ttv_LL) and !((iTtvm[Index, 0] > (iTtv_meze[Index, 0] + Ttv_hyst)) or @Ttv_F)
Let @Ttv_HH = ((iTtvm[Index, 0] > iTtv_meze[Index, 3]) or @Ttv_HH) and !((iTtvm[Index, 0] < (iTtv_meze[Index, 3] - Ttv_hyst)) or @Ttv_F)

// Žádaná teplota topné vody
If @iKonstPr
	Let fx = iTvzKonst[Index, 0]
Else :NONE
	// V provozu regulace na ekvitermní teplotu
	//Interpolace ekvitermni krivky
	//Najdeme 5. bod křivky - při jaké venkovce bude W=0
	Let iEqCurve[4,0] = if((iEqCurve[3,1]-iEqCurve[2,1])!=0, iEqCurve[2,0] - (iEqCurve[2,1]*(iEqCurve[3,0]-iEqCurve[2,0])/(iEqCurve[3,1]-iEqCurve[2,1])), 30)
	Let iEqCurve[4,1] = 0
	Interpol iTE, Ttvp, iEqCurve

	DayPlan 0x0000, 7, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0080, iSvatky, iCP_casy, iCP_tepl, TpZ
	//Aplikace útlumu
	Let fx = Ttvp-(Ttvp/100)*TpZ
EndIf
Let oTtvz[Index, 0] = if(fx < iTtv_meze[Index, 1], iTtv_meze[Index, 1], if(fx > iTtv_meze[Index, 2], iTtv_meze[Index, 2], fx))

// Letní režim topné větve
Let @xLeto = oTtvz[Index, 0] < iTNaj[0,0]-iTNaj[1,0]*int(!@xLeto) 

// Protočení čerpadla
SyncMark 5, 1, 32, 10, 0, ix.0, NONE
TimerOn @pCprotoc, tCerpProtoc, ix.3, NONE
Let @pCprotoc = ix.0 and !@iStop
		
// Ruční režim regulátoru
Let RV_rezim.1 = @iRV_ruc or @iStop or @xLeto or @Ttv_HH or @Ttv_F or !(@iZap or @Ttv_LL)

// Výpočet akčního zásahu
Let Ttvz = oTtvz[Index, 0]
Let Ttvm = iTtvm[Index, 0]
Let fx = ioRV_akce[Index, 0]
PID Ttvz, Ttvm, fx, RV_rezim, iRV_PID

// Akční zásah v ručním režimu
If RV_rezim.1, :NONE
	If @iRV_ruc, :NONE
		// V ručním režimu externě zapsaný zásah
	Else :NONE
		Let ix.0 = @iStop or @Ttv_HH or @xLeto or !@iZap
		If ix.0, :NONE
			// Při vypnutí, vysoké teplotě a v létě mimo protočení zavřeno
			Let ioRV_akce[Index, 0] = 0.0
		Else :NONE
			If @Ttv_F
				// Při poruše snímače náhradní otevření, jinak zavřeno
				Let ioRV_akce[Index, 0] = RV_tpor
			EndIf
		EndIf
	EndIf
Else :NONE
	// Pokud není ruční režim, akční zásah určený modulem PID
 	Let ioRV_akce[Index, 0] = fx
EndIf

// Řízení čerpadla
// Doběh čerpadla
Let @RV_AnC = (@iZap or @Ttv_LL) and !@xLeto
TimerOff @RV_AnC, tCerpDobeh, @RV_AnC_tof, NONE
// Zapnutí čerpadla
Let @oCerpZap = (@RV_AnC_tof or @pCprotoc) and !@iStop
// Zapnutí pro signalizaci
Let @CStart = @oCerpZap
// Zápis do vstupně výstupních parametrů
Let ioStav[Index, 0] = tvStav
Let ioRV_rezim[Index, 0] = RV_rezim
]]></Script>
          </Body>
