                <Type>dbtFloat</Type>
                <Comment>Integrační konstanta</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
                <InitialValue>1000</InitialValue>
              </Variable>
              <Variable Name="dtLim" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>4003</Wid>
                <Station>4</Station>
                <Type>dbtFloatMtx</Type>
                <Comment>Navýšení ohřevu, meze</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
                <Row>2</Row>
                <Col>1</Col>
                <InitialValue>-3;3;</InitialValue>
              </Variable>
              <Variable Name="RV_rezim" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>4336</Wid>
                <Station>4</Station>
                <Type>dbtInt</Type>
                <Comment>PID topné vody, režim regulátoru</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
                <InitialValue>4</InitialValue>
              </Variable>
              <Variable Name="RV_cerp" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>4536</Wid>
                <Station>4</Station>
                <Type>dbtFloatMtx</Type>
                <Comment>Parametry otevření ventilu pro zapnutí čerpadla</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
                <Row>2</Row>
                <Col>1</Col>
                <InitialValue>2;1;</InitialValue>
              </Variable>
              <Variable Name="tCerpDobeh" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>4551</Wid>
                <Station>4</Station>
                <Type>dbtLong</Type>
                <Comment>Čerpadlo, čas doběhu</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
                <InitialValue>120000</InitialValue>
              </Variable>
              <Variable Name="ix" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>4004</Wid>
                <Station>4</Station>
                <Type>dbtInt</Type>
                <Comment>Integer</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
              </Variable>
              <Variable Name="Ttuva" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>4005</Wid>
                <Station>4</Station>
                <Type>dbtFloat</Type>
                <Comment>Teplota měřená, zásobník</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
              </Variable>
            </VariableList>
            <AliasList>
              <Alias Name="@xAut" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Stav Aut</Comment>
                <VarName>tuvStav</VarName>
                <Bit>0</Bit>
              </Alias>
              <Alias Name="@xVyp" Class="Amit.Psp4.Shared.Psp4Alias">
