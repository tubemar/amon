        <CodeBlock Name="IPConf" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>IP konfigurace</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[If @LoadIP
	Let @LoadIP = false
	GetIPConf 1, 1, EthAddr, EthPort, EthMaskBin, NONE, NONE, EthPassword, EthGtw, EthTrueType, NONE
	Let ix1 = EthMaskBin
	Let lx = 0
	Let ix.0 = ix1 > 0
	If ix.0
		Repeat 
			Let lx.(32-ix1) = true
			Let ix1 = ix1 - 1
			Let ix.1 = ix1 == 0
		Until ix.1
	Endif	
	Let EthMask[0,0] = int((lx & 0xFF000000) >> 24)
	Let EthMask[0,1] = int((lx & 0x00FF0000) >> 16)
	Let EthMask[0,2] = int((lx & 0x0000FF00) >> 8)
	Let EthMask[0,3] = int(lx & 0x000000FF)
Endif

If @SaveIP
	Let @SaveIP = false
	Let lx = (long(EthMask[0,0]) << 24) | (long(EthMask[0,1]) << 16) | (EthMask[0,2] << 8) | EthMask[0,3]
	Let ix1 = 32
	Let EthMaskBin = 0
	Repeat
		Let ix1 = ix1 - 1
		Let ix2.0 = lx.(ix1)
		If ix2.0
			Let EthMaskBin = EthMaskBin + 1
		Endif
		Let ix2.1 = !ix2.0 or (ix1 == 0)
	Until ix2.1
	SetIPConf 1, 1, EthAddr, EthPort, EthMaskBin, -1, 0, EthPassword, EthGtw, EthTrueType, NONE
Endif]]></Script>
          </Body>
        </CodeBlock>
