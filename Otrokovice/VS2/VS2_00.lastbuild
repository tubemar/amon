        <CodeBlock Name="libAMR_DI0" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Čtení DI z AMR</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//*************************************************
	//***************** Čtení AMR-DI0 *****************
	//*************************************************

	ARI_DigIn 2, 0, AR_DI0_A, 0x00000000
	//Přepnutí AUTOMATICKÝ nebo MANUÁLNÍ režim + volba negace
	Let AR_DI0 = (~AR_DI0_XR & (AR_DI0_A ^ AR_DI0_neg)) | (AR_DI0_XR & AR_DI0_R)
	//Beznárazové přepnuti do ručního režimu
	Let AR_DI0_R = AR_DI0
]]></Script>
          </Body>
        </CodeBlock>
