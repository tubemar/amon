        <CodeBlock Name="ProcAlarm" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Poruchy</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//Příznak restartu
	TimerOn @Restart, tRestart, ix.0, NONE
	Let @Restart = If(ix.0,false,@Restart)

	ARI_State 1, AR_State[0,0], 5, NONE				//Komunikace ARION
	ARI_State 2, AR_State[1,0], 5, NONE
	ARI_State 3, AR_State[2,0], 5, NONE

	//Alarmy
	Hyst PTV1, @PressTV_OK, 50.000, 5.000, 0x0000			//Náhrada manostatu na DI

	Let ix.0 = TTVSpol > 95
	Timer 0x0008, ix.0, ix.1, 115, 0					//Casova necitlivost
	Let Alarm_A1.0 = (ix.1 or !@TempTVS_OK) and !@Restart		//Prehrati TV spol
	Let Alarm_A1.1 = @STOP_btnOK and !@OverVS_OK and !@Restart	//Prehrati prostoru VS
	Let Alarm_A1.3 = ioTVstat[9,0].6 or !@TempTUV1_OK		//Přehřátí TUV a havarijní termostat
	Let Alarm_A1.4 = TTUV[9,0]<(WTV[9,0]-7.0)				//Nizka tepl. TUV vystup
	Let Alarm_A1.5 = !@chCTV1 and @CTV1  				//Porucha čerp. UT1
	Let Alarm_A1.6 = !@chCTV2 and @CTV2				//Porucha čerp. UT2
	Let Alarm_A1.7 = !@chCTUC and @CTUC       			//Porucha TUV cirk.
	Let Alarm_A1.8 = !@SV_Press_OK and !@Restart    			//Tlak SV
	Let Alarm_A1.9 = @SolDop       					//Dlouhé doplň.
	Let Alarm_A1.10 = PTV1 < PTV1min					//Nízký tlak ÚT
	Let Alarm_A1.11 = PTV1 > PTV1max  					//Vysoký tlak ÚT
	Let Alarm_A1.12 = TTUC1<(WTV[9,0]-12.0)				//Nizka tepl. TUV cirk.
	Let Alarm_A1.14 = @OverVS_OK and !@LeakVS_OK and !@Restart	//Zaplavení VS
	Let Alarm_A1.15 = !@STOP_btnOK and !@Restart     		//STOP tlačítko
	Let Alarm_A1.16 = !@chCTV3 and @CTV3  				//Porucha čerp. UT3
	Let Alarm_A1.17 = !@chCTV4 and @CTV4  				//Porucha čerp. UT4

	Let Alarm_A1.24 = (AR_State[0,0]!=1) and !@Restart		//Komunikace s A2
	Let Alarm_A1.25 = (AR_State[1,0]!=1) and !@Restart 		//Komunikace s A3
	Let Alarm_A1.26 = (AR_State[2,0]!=1) and !@Restart 		//Komunikace s A4
	
	//Kvit pro vsechny chyby
	Let Alarm_Q1 = If(KvitOut.0, ~Alarm_A1, Alarm_Q1)
	Let KvitOut.0 = false
	
	ErrSig32 Alarm_A1, 0x00000001, Alarm_Q1, 0x00000001, Alarm_E1.0, Alarm_I1.0, 5, 4, 1010, 9, 0, 0		//Přehřátí TV spol
	ErrSig32 Alarm_A1, 0x00000002, Alarm_Q1, 0x00000002, Alarm_E1.1, Alarm_I1.1, 2, 4, 1010, 2, 0, 0		//Prehrati prostoru VS
	ErrSig32 Alarm_A1, 0x00000004, Alarm_Q1, 0x00000004, Alarm_E1.2, Alarm_I1.2, 4, 4, 1010, 20, 0, 0		//Porucha čerpadla TV spol.
	ErrSig32 Alarm_A1, 0x00000008, Alarm_Q1, 0x00000008, Alarm_E1.3, Alarm_I1.3, 4, 4, 1010, 30, 0, 0		//Přehřátí TUV
	ErrSig32 Alarm_A1, 0x00000010, Alarm_Q1, 0x00000010, Alarm_E1.4, Alarm_I1.4, 1800, 4, 1010, 51, 0, 0	//Nizka tepl. TUV
	ErrSig32 Alarm_A1, 0x00000020, Alarm_Q1, 0x00000020, Alarm_E1.5, Alarm_I1.5, 4, 4, 1010, 40, 0, 0		//Cerp. UT1
	ErrSig32 Alarm_A1, 0x00000040, Alarm_Q1, 0x00000040, Alarm_E1.6, Alarm_I1.6, 4, 4, 1010, 41, 0, 0		//Cerp. UT2
	ErrSig32 Alarm_A1, 0x00000080, Alarm_Q1, 0x00000080, Alarm_E1.7, Alarm_I1.7, 4, 4, 1010, 42, 0, 0		//Cerp. TUC1
	ErrSig32 Alarm_A1, 0x00000100, Alarm_Q1, 0x00000100, Alarm_E1.8, Alarm_I1.8, 10, 4, 1010, 39, 0, 0	//Tlak SV
	ErrSig32 Alarm_A1, 0x00000200, Alarm_Q1, 0x00000200, Alarm_E1.9, Alarm_I1.9, 900, 4, 1010, 50, 0, 0	//Dlouhe doplnovani
	ErrSig32 Alarm_A1, 0x00000400, Alarm_Q1, 0x00000400, Alarm_E1.10, Alarm_I1.10, 10, 4, 1010, 30, 0, 0	//Nizky tlak UT
	ErrSig32 Alarm_A1, 0x00000800, Alarm_Q1, 0x00000800, Alarm_E1.11, Alarm_I1.11, 10, 4, 1010, 31, 0, 0	//Vysoky tlak UT
	ErrSig32 Alarm_A1, 0x00001000, Alarm_Q1, 0x00001000, Alarm_E1.12, Alarm_I1.12, 2400, 4, 1010, 52, 0, 0	//Nizka tepl. TUC
	ErrSig32 Alarm_A1, 0x00002000, Alarm_Q1, 0x00002000, Alarm_E1.13, Alarm_I1.13, 5, 4, 1010, 21, 0, 0	//Přehřátí UT2
	ErrSig32 Alarm_A1, 0x00004000, Alarm_Q1, 0x00004000, Alarm_E1.14, Alarm_I1.14, 2, 4, 1010, 3, 0, 0	//Zaplaveni VS
	ErrSig32 Alarm_A1, 0x00008000, Alarm_Q1, 0x00008000, Alarm_E1.15, Alarm_I1.15, 2, 1, 1010, 1, 0, 0	//STOP tlacitko
	ErrSig32 Alarm_A1, 0x00010000, Alarm_Q1, 0x00010000, Alarm_E1.16, Alarm_I1.16, 5, 0, 1010, 16, 0, 0	//Porucha čerp. UT3
	ErrSig32 Alarm_A1, 0x00020000, Alarm_Q1, 0x00020000, Alarm_E1.17, Alarm_I1.17, 5, 0, 1010, 17, 0, 0	//Porucha čerp. UT4
	ErrSig32 Alarm_A1, 0x00040000, Alarm_Q1, 0x00040000, Alarm_E1.18, Alarm_I1.18, 5, 0, 1010, 18, 0, 0	//Porucha čerp. UT5
	ErrSig32 Alarm_A1, 0x00080000, Alarm_Q1, 0x00080000, Alarm_E1.19, Alarm_I1.19, 5, 0, 1010, 19, 0, 0	//Porucha čerp. UT6
	ErrSig32 Alarm_A1, 0x00100000, Alarm_Q1, 0x00100000, Alarm_E1.20, Alarm_I1.20, 5, 0, 1010, 20, 0, 0
	ErrSig32 Alarm_A1, 0x00200000, Alarm_Q1, 0x00200000, Alarm_E1.21, Alarm_I1.21, 5, 0, 1010, 21, 0, 0
	ErrSig32 Alarm_A1, 0x00400000, Alarm_Q1, 0x00400000, Alarm_E1.22, Alarm_I1.22, 5, 0, 1010, 22, 0, 0
	ErrSig32 Alarm_A1, 0x00800000, Alarm_Q1, 0x00800000, Alarm_E1.23, Alarm_I1.23, 5, 0, 1010, 23, 0, 0
	ErrSig32 Alarm_A1, 0x01000000, Alarm_Q1, 0x01000000, Alarm_E1.24, Alarm_I1.24, 5, 0, 1010, 24, 0, 0
	ErrSig32 Alarm_A1, 0x02000000, Alarm_Q1, 0x02000000, Alarm_E1.25, Alarm_I1.25, 5, 0, 1010, 25, 0, 0
	ErrSig32 Alarm_A1, 0x04000000, Alarm_Q1, 0x04000000, Alarm_E1.26, Alarm_I1.26, 5, 0, 1010, 26, 0, 0
	ErrSig32 Alarm_A1, 0x08000000, Alarm_Q1, 0x08000000, Alarm_E1.27, Alarm_I1.27, 5, 0, 1010, 27, 0, 0
	ErrSig32 Alarm_A1, 0x10000000, Alarm_Q1, 0x10000000, Alarm_E1.28, Alarm_I1.28, 1, 0, 1010, 28, 0, 0
	ErrSig32 Alarm_A1, 0x20000000, Alarm_Q1, 0x20000000, Alarm_E1.29, Alarm_I1.29, 5, 0, 1010, 29, 0, 0
	ErrSig32 Alarm_A1, 0x40000000, Alarm_Q1, 0x40000000, Alarm_E1.30, Alarm_I1.30, 5, 0, 1010, 30, 0, 0
	ErrSig32 Alarm_A1, 0x80000000, Alarm_Q1, 0x80000000, Alarm_E1.31, Alarm_I1.31, 5, 0, 1010, 31, 0, 0
	
	Let @StopVS = Alarm_A1.15 or Alarm_E1.15 or Alarm_E1.1 or Alarm_E1.14
	Let @StopTV = @StopVS or !@PressTV_OK
	Let @StopTUV = @StopVS or Alarm_A1.8
	
	Let ErrOr = bool(Alarm_E1)	//suma poruch pro LCD
