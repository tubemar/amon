	NumericView1.Value = Memory1.Value + 1;
	CaseLabelView1.Value = Memory2.Value;
	DateTimeUpDn1.Col = Memory1.Value;
	DateTimeUpDn1.Row = Memory2.Value;
	NumericUpDn1.Col = Memory1.Value;
	NumericUpDn1.Row = Memory2.Value;
end;

event Key1_OnKeyPress()
	if (Memory1.Value > 0) then
		Memory1.Value = Memory1.Value - 1;
	else
		Memory1.Value = 7;
	endif;
	Actual();
end;

event Key2_OnKeyPress()
	if (Memory1.Value >= 7) then
		Memory1.Value = 0;
	else
		Memory1.Value = Memory1.Value + 1;
	endif;
	Actual();
end;

