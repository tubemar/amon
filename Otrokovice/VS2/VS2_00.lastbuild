                <Comment>Protočení čerpadla</Comment>
                <VarName>tvStav</VarName>
                <Bit>14</Bit>
              </Alias>
              <Alias Name="@CStart" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Zapnutí čerpadla</Comment>
                <VarName>tvStav</VarName>
                <Bit>11</Bit>
              </Alias>
            </AliasList>
          </DBase>
