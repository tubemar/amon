              <Variable Name="tuvStav" Class="Amit.Psp4.Shared.Psp4VariableLocal">
                <Wid>4300</Wid>
                <Station>4</Station>
                <Type>dbtInt</Type>
                <Comment>Topná větev, stav</Comment>
                <WarmInit>False</WarmInit>
                <InitialValueFormat>0</InitialValueFormat>
              </Variable>
              <Variable Name="Tzz" Class="Amit.Psp4.Shared.Psp4VariableLocal">
