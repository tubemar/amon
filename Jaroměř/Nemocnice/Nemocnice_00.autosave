        <CodeBlock Name="libDM_UI8_1" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Čtení UI vstupů pro UI8xxx</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[     	//Analogové vstupy
	//*************************************************
	//******************* KANÁL UI0 *******************
	//*************************************************
	ARI_AnIn 1, 0, 8, AR_UI0_pom[0,0], AR_UI0_par[0,0], 10.000, 0.000, 10.000, 0.000, 10.000

	//******************* ANALOGOVÝ VSTUP UI0 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AR_UI0_typ.0, :NONE
		//Pro převod U->T používám rozsah 0-5V (není třeba měnit default params)
		Let fX = AR_UI0_pom[0,0]/20.0
		//Pt1000
		If AR_TC0_typ.0
			//Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
			Interpol fX, AR_UI0_okam[0,0], Pt1000_Kriv
		//Ni1000
		Else :NONE
			//5000ppm
			If AR_Ni0_typ.0
				Ni1000U2T fX, AR_UI0_okam[0,0], 5000, 15.000, 3920.000
			//6180ppm
			Else :NONE
				Ni1000U2T fX, AR_UI0_okam[0,0], 6180, 15.000, 3920.000
			EndIf 
		EndIf
	//Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
	Else :NONE
		Let AR_UI0_okam[0,0] = AR_UI0_pom[0,0]
	EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AR_UI0_konst[0, 0]*AR_UI0_okam[0, 0]+AR_UI0_konst[0, 1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AR_UI0[0,0], AR_UI0_konst[0,2]

	//******************* ANALOGOVÝ VSTUP UI1 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AR_UI0_typ.1, :NONE
		//Pro převod U->T používám rozsah 0-5V (není třeba měnit default params)
		Let fX = AR_UI0_pom[1,0]/20.0
		//Pt1000
		If AR_TC0_typ.1
			//Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
			Interpol fX, AR_UI0_okam[1,0], Pt1000_Kriv
		//Ni1000
		Else :NONE
			//5000ppm
			If AR_Ni0_typ.1
				Ni1000U2T fX, AR_UI0_okam[1,0], 5000, 15.000, 3920.000
			//6180ppm
			Else :NONE
				Ni1000U2T fX, AR_UI0_okam[1,0], 6180, 15.000, 3920.000
			EndIf 
		EndIf
