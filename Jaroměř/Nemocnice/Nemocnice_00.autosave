			Interpol fX, AR_UI0_okam[2,0], Pt1000_Kriv
		//Ni1000
		Else :NONE
			//5000ppm
			If AR_Ni0_typ.2
				Ni1000U2T fX, AR_UI0_okam[2,0], 5000, 15.000, 3920.000
			//6180ppm
			Else :NONE
				Ni1000U2T fX, AR_UI0_okam[2,0], 6180, 15.000, 3920.000
			EndIf 
		EndIf
	//Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
	Else :NONE
		Let AR_UI0_okam[2,0] = AR_UI0_pom[2,0]
	EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AR_UI0_konst[2, 0]*AR_UI0_okam[2, 0]+AR_UI0_konst[2, 1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AR_UI0[2,0], AR_UI0_konst[2,2]

	//******************* ANALOGOVÝ VSTUP UI3 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AR_UI0_typ.3, :NONE
