	ErrSig32 Alarm_A2, 0x00010000, Alarm_Q2, 0x00010000, Alarm_E2.16, Alarm_I2.16, 1, 0, 4096, 2016, 0, 0
	ErrSig32 Alarm_A2, 0x00020000, Alarm_Q2, 0x00020000, Alarm_E2.17, Alarm_I2.17, 1, 0, 1010, 17, 0, 0	
	ErrSig32 Alarm_A2, 0x00040000, Alarm_Q2, 0x00040000, Alarm_E2.18, Alarm_I2.18, 1, 0, 1010, 18, 0, 0	
	ErrSig32 Alarm_A2, 0x00080000, Alarm_Q2, 0x00080000, Alarm_E2.19, Alarm_I2.19, 1, 0, 1010, 19, 0, 0	
	ErrSig32 Alarm_A2, 0x00100000, Alarm_Q2, 0x00100000, Alarm_E2.20, Alarm_I2.20, 1, 0, 1010, 20, 0, 0
	ErrSig32 Alarm_A2, 0x00200000, Alarm_Q2, 0x00200000, Alarm_E2.21, Alarm_I2.21, 1, 0, 1010, 21, 0, 0
	ErrSig32 Alarm_A2, 0x00400000, Alarm_Q2, 0x00400000, Alarm_E2.22, Alarm_I2.22, 1, 0, 1010, 22, 0, 0
	ErrSig32 Alarm_A2, 0x00800000, Alarm_Q2, 0x00800000, Alarm_E2.23, Alarm_I2.23, 1, 0, 1010, 23, 0, 0
	ErrSig32 Alarm_A2, 0x01000000, Alarm_Q2, 0x01000000, Alarm_E2.24, Alarm_I2.24, 1, 0, 1010, 24, 0, 0
	ErrSig32 Alarm_A2, 0x02000000, Alarm_Q2, 0x02000000, Alarm_E2.25, Alarm_I2.25, 1, 0, 1010, 25, 0, 0
	ErrSig32 Alarm_A2, 0x04000000, Alarm_Q2, 0x04000000, Alarm_E2.26, Alarm_I2.26, 1, 0, 1010, 26, 0, 0
	ErrSig32 Alarm_A2, 0x08000000, Alarm_Q2, 0x08000000, Alarm_E2.27, Alarm_I2.27, 1, 0, 1010, 27, 0, 0
	ErrSig32 Alarm_A2, 0x10000000, Alarm_Q2, 0x10000000, Alarm_E2.28, Alarm_I2.28, 1, 0, 1010, 28, 0, 0
	ErrSig32 Alarm_A2, 0x20000000, Alarm_Q2, 0x20000000, Alarm_E2.29, Alarm_I2.29, 1, 0, 1010, 29, 0, 0
	ErrSig32 Alarm_A2, 0x40000000, Alarm_Q2, 0x40000000, Alarm_E2.30, Alarm_I2.30, 1, 0, 1010, 30, 0, 0
	ErrSig32 Alarm_A2, 0x80000000, Alarm_Q2, 0x80000000, Alarm_E2.31, Alarm_I2.31, 1, 0, 1010, 31, 0, 0


	// *****************************************************************************
	//                    Vjacerý ENABLES
	// *****************************************************************************
	
	If @DEnStanice								// chod jednotlivých FUB větví
		Let ReqN = 0					
	Else
		Let ReqN = 0xFFFF
	EndIf
	
	Let @HUP = @DEnStanice  							// uzávěr plynu
	
	Let @EnDopl = @DEnStanice  							// doplňování tlaku
	Let @EnRegTProst = @DEnStanice  						// regulace teploty prostoru 
	
	// Spouštění řízení kotlů
	Let ix1.0 = tvx_stav[0,0].3 or tvx_stav[1,0].3				// některá z TUV větví potřebuje ohřev
	Let ix1.1 = ((ReqN & 0x3FF8) != 0x3FF8) and !@Leto			// některá z topných větví běží
	Let ix1.0 = ix1.0 or ix1.1
	Let @DisKask = !@DEnStanice		//prozatim takhle
	
	Let @EnCCir_TUV1 = !tvx_stav[0,0].1 or !tvx_stav[1,0].1
	Let @EnCCir_TUV2 = !tvx_stav[1,0].1
	
	// Podávací čerpadlo
	Let @CTVpod = @CTV9 or @CTV10 or @CTV11					// některá ze vzdálených topných větví běží
	
	// Odvětrání
	Let @FanOut1 = false			
	Let @FanOut2 = false
	
	
	
	// *****************************************************************************
	//                    PORUCHY Reakce
	// *****************************************************************************

	// Stop celé kotelny
	Let @ErrGroup = Alarm_E1.0 or Alarm_E1.2 or Alarm_E1.3 or Alarm_E1.4		// havarijni řetězec
	If @ErrGroup
		Let @HUP = false
		Let ReqN = 0xFFFF			// vypnuti vsech vetvi a TUV
		Let @EnCCir_TUV1 = false
		Let @EnCCir_TUV2 = false
		Let @EnDopl = false  		// vypnutí doplňování tlaku
		Let @EnRegTProst = false  		// vypnutí regulace teploty prostoru
		Let @FanOut1 = true			// odvětrání
		Let @FanOut2 = true
	EndIf
	
	Let @ErrGroup = Alarm_E1.1 or Alarm_E1.6						// plyn 1.st nebo přehřátí kotlů
	If @ErrGroup
		Let @DisKask = true
	EndIf
	
	Let @ErrGroup = Alarm_E1.5								// nízký tlak SV
	If @ErrGroup
		Let @DisTUV1 = true
		Let @DisTUV2 = true
		Let @EnCCir_TUV1 = false
		Let @EnCCir_TUV2 = false
	EndIf
	
	// Odstavení všech čerpadel
	Let @ErrGroup = Alarm_E1.9								// nízký tlak TV
	If @ErrGroup
		Let @DisKask = true
		Let ReqN = ReqN & 0xFFFF
		Let @EnCCir_TUV1 = false
		Let @EnCCir_TUV2 = false
	EndIf

	// Odstavení TUV1
	Let @ErrGroup = Alarm_E1.7								//  TUV1
	If @ErrGroup
		Let @DisTUV1 = True
	EndIf
	// Odstavení TUV2
	Let @ErrGroup = Alarm_E1.8								//  TUV2
	If @ErrGroup
		Let @DisTUV2 = True
	EndIf
	
	Let @AlarmSign =  bool(Alarm_E1) or bool(Alarm_E2)
	// GSM signalizace
	Let ix1.0 = bool(Alarm_E1)
	TimerPuls ix1.0, tRestart[0,0], @GSM_start, NONE]]></Script>
