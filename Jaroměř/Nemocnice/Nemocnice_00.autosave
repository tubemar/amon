         AnIn #AI00_5, AI0_okamzita[5,0], AI0_params[5,0], AI0_params[5,1], AI0_params[5,2], AI0_params[5,3], AI0_params[5,4]
       EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AI0_konst[5,0]*AI0_okamzita[5,0]+AI0_konst[5,1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AI0[5,0], AI0_konst[5,2]
	
	//******************* ANALOGOVÝ VSTUP AI0.6 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AI0_typ.6, :NONE
	  //Pt1000
	  If TC0_typ.6, :NONE
	    //Měření napětí na čidle v rozsahu 0 .. 5 V
	    AnIn #Ni10001_6, AI0_Pom[6,0], 5.000, 0.000, 5.000, 0.000, 5.000
	    //Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
	    Interpol AI0_Pom[6,0], AI0_okamzita[6,0], Pt1000_Kriv
	  //Ni1000
	  Else :NONE
           //5000ppm
           If Ni0_typ.6, :NONE
             Ni1000 #Ni10001_6, AI0_okamzita[6,0], 5000
             //6180ppm
           Else :NONE
             Ni1000 #Ni10001_6, AI0_okamzita[6,0], 6180
           EndIf 
	  EndIf
         //Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
       Else :NONE
         AnIn #AI00_6, AI0_okamzita[6,0], AI0_params[6,0], AI0_params[6,1], AI0_params[6,2], AI0_params[6,3], AI0_params[6,4]
       EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AI0_konst[6, 0]*AI0_okamzita[6, 0]+AI0_konst[6, 1]
	//Výslednou hodnotu filtrujeme
