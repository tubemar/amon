	Valve tvx_akce[8,0], tvx_prebeh[8,0], tvx_poloha[8,0], @ValUT6_Z, @ValUT6_O
	Valve tvx_akce[9,0], tvx_prebeh[9,0], tvx_poloha[9,0], @ValUT7_Z, @ValUT7_O
	Valve tvx_akce[10,0], tvx_prebeh[10,0], tvx_poloha[10,0], @ValUT8_Z, @ValUT8_O
	Valve tvx_akce[11,0], tvx_prebeh[11,0], tvx_poloha[11,0], @ValUT9_Z, @ValUT9_O
	Valve tvx_akce[12,0], tvx_prebeh[12,0], tvx_poloha[12,0], @ValUT10_Z, @ValUT10_O
	Valve tvx_akce[13,0], tvx_prebeh[13,0], tvx_poloha[13,0], @ValUT11_Z, @ValUT11_O

	//******************************************************************************
	//Regulace tlaku a dopouštění
	//******************************************************************************
	
	Call libDopln]]></Script>
          </Body>
          <Period>1000</Period>
          <Offset>300</Offset>
          <Type>4</Type>
          <InterruptEdge>1</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="procIdle" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Obsluha obrazovek</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	// Obsluha terminálu
	Lcw3Idle NONE
	// Konfigurace, požadavek na reset
	If @Reset, :NONE
		Let @Reset = false
		WatchDog 1, 100
		Let ix1.0 = false
		Repeat :NONE
		Until ix1.0
	EndIf
	]]></Script>
          </Body>
          <Period>1000</Period>
          <Offset>0</Offset>
          <Type>16</Type>
          <InterruptEdge>1</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="procInit" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Inicializace</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	// Příznak náběhu
       Let @Restart = true

:3000	Arion 1, 38400, 3

	ARI_DINode :3000, 9, 500, 30000, 2
	ARI_DONode :3000, 9, 500, 30000, 2
	

	For ix1,0,3,1
		Let RoZ_PIDrezim[ix1,0] = 20
	EndFor
	
	For ix1,0,15,1
		Let tvx_PIDrezim[ix1,0] = 20
	EndFor]]></Script>
          </Body>
          <Period>1000</Period>
          <Offset>0</Offset>
          <Type>36</Type>
          <InterruptEdge>1</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="procInputs" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Digitální a analogové vstupy</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//Čtení digitálních vstupů
	Call libDI					//interní digitální vstupy
	Call libDM_DIx_0
	Call libDM_DIx_1
	Call libDM_DIx_2
	Call libAMR_DI0

	//Čtení analogových vstupů
	Call libAI

	//Čtení UI0
	Call libDM_UI8_1

	//Čtení UI1
	Call libDM_UI8_2

	//Čtení UI2
	Call libDM_UI8_3

	//Přiřazení analogových vstupů
	Let TVenk 	= AI0[0, 0]
	Let TProstKot	= AI0[1, 0]
	Let TKotZp1	= AI0[2, 0]
	Let TKotZp2	= AI0[3, 0]
	Let TKotZp3	= AI0[4, 0]
	Let TKotZp4	= AI0[5, 0]
	Let TKotVyst	= AI0[6, 0]
	Let TTVAnul	= AI0[7, 0]
	
	Let tvx_Tm[0, 0] = AR_UI1[0, 0]		//TUV1
	Let tvx_Tm[1, 0] = AR_UI1[1, 0]		//TUV2
	Let tvx_Tm[2, 0] = AR_UI1[2, 0]		//TUV3

	Let tvx_Tm[3, 0] = AR_UI0[0, 0]		//UT1 HK2
	Let tvx_Tm[4, 0] = AR_UI0[1, 0]		//UT2 HK3
	Let tvx_Tm[5, 0] = AR_UI0[2, 0]		//UT3 HK4
	Let tvx_Tm[6, 0] = AR_UI0[3, 0]		//UT4 HK5
	Let tvx_Tm[7, 0] = AR_UI0[4, 0]		//UT5 HK6
	Let tvx_Tm[8, 0] = AR_UI0[5, 0]		//UT6 HK7
	Let tvx_Tm[9, 0] = AR_UI0[6, 0]		//UT7 HK8
	Let tvx_Tm[10,0] = AR_UI0[7, 0]		//UT8 HK9
	Let tvx_Tm[11,0] = AR_UI2[0, 0]		//UT9 HK10
	Let tvx_Tm[12,0] = AR_UI2[1, 0]		//UT10 HK11
	Let tvx_Tm[13,0] = AR_UI2[2, 0]		//UT11 HK12
	
	
	Let TTUV1_vyst   = AR_UI1[3, 0]		//Teplota TUV výstup 1
	Let TTUV2_vyst   = AR_UI1[4, 0]		//Teplota TUV výstup 2
	Let PTV1	   = AR_UI1[5, 0]		//Tlak TV
	
]]></Script>
          </Body>
          <Period>500</Period>
          <Offset>200</Offset>
          <Type>0</Type>
          <InterruptEdge>1</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="procOutAn" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Analogové výstupy</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	// Přiřazení výstupů
	// Amini
	If AO_XR.0, :NONE
	Else :NONE
		Let AO[0, 0] = Ka_VykonKot[0, 0]
	EndIf
	If AO_XR.1, :NONE
	Else :NONE
		Let AO[1, 0] = Ka_VykonKot[1, 0]
	EndIf
	If AO_XR.2, :NONE
	Else :NONE
		Let AO[2, 0] = Ka_VykonKot[2, 0]
	EndIf
	If AO_XR.3, :NONE
	Else :NONE
		Let AO[3, 0] = Yodp_akce
	EndIf


	// Zápis na výstupy
	AnOut #0.0, AO[0,0], AO_params[0,0], AO_params[0,1], AO_params[0,2], AO_params[0,3], AO_params[0,4]
	AnOut #0.1, AO[1,0], AO_params[1,0], AO_params[1,1], AO_params[1,2], AO_params[1,3], AO_params[1,4]
	AnOut #0.2, AO[2,0], AO_params[2,0], AO_params[2,1], AO_params[2,2], AO_params[2,3], AO_params[2,4]
	AnOut #0.3, AO[3,0], AO_params[3,0], AO_params[3,1], AO_params[3,2], AO_params[3,3], AO_params[3,4]		
	]]></Script>
          </Body>
          <Period>1000</Period>
          <Offset>400</Offset>
          <Type>9</Type>
          <InterruptEdge>1</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="procOutDig" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Digitální výstupy</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[
	// Režim automaticky / ručně
       Let DO0 = (~DO0_XR & DO0_A) | (DO0_XR & DO0_R)
       // Beznárazové přepnutí do ručního režimu
       Let DO0_R = DO0
       // Zápis na výstupy
       DigOut DO0, #0, 0x0000

	Call libDM_RDO8_1
	Call libDM_RDO8_2
	Call libDM_DO18_1
	Call libDM_DO18_2
	Call libDM_RDO8_3
	Call libAMR_RDO0	
]]></Script>
          </Body>
          <Period>500</Period>
          <Offset>400</Offset>
          <Type>8</Type>
          <InterruptEdge>1</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="procRTC" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Čas RTC</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[       // Čas RTC
       GetTime RTC_time, RTC_slozky, NONE
       // Změna zimní / letní čas
       Let UTC_time = if(@tmLetniCas, RTC_time - 10800, RTC_time - 7200)
       ParseTime UTC_time, NONE, NONE, UTC_slozky
       Let @tmLeto = ((UTC_slozky[4, 0] > 3) and (UTC_slozky[4, 0] < 10) or (UTC_slozky[4, 0] == 3) and ((UTC_slozky[3, 0] + 7) - UTC_slozky[6, 0] > 31)) or (UTC_slozky[4, 0] == 10) and ((UTC_slozky[3, 0] + 7) - UTC_slozky[6, 0] <= 31)
       If @tmLetoInit, :NONE
       	Let @tmZmena = @tmLeto xor @tmLetniCas
       	If @tmZmena, :NONE
       		GetTime RTC_time, NONE, NONE
       		Let RTC_time = if(@tmLeto, RTC_time + 3600, RTC_time - 3600)
       		SetTime RTC_time
       	EndIf 
       EndIf 
       Let @tmLetniCas = @tmLeto
       Let @tmLetoInit = true]]></Script>
          </Body>
          <Period>1000</Period>
          <Offset>100</Offset>
          <Type>12</Type>
          <InterruptEdge>1</InterruptEdge>
        </CodeBlock>
        <CodeBlock Name="ProcZdroje" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Řízení kaskády kotlů</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[
	call libZdroje]]></Script>
