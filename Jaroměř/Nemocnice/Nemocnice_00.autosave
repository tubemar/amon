	Hyst2 PTV1, @Dopln, P_dopl[0,0], P_dopl[1,0], 0x0001
	
	
	//**********************************************************
	//*************** Výběr způsobu doplnění *****************
	//**********************************************************
	If (not @NadrzMin) 
		Let @PriorVoda = true 
	EndIf
	If (@NadrzMax) 
		Let @PriorVoda = false 
	EndIf
	If (@PriorVoda)
		Let @DoplNadrz = @Dopln
		Let @KomprDopl = false
	Else
		Let @KomprDopl = @Dopln
		Let @DoplNadrz = false
	EndIf			
	
	// Odstavení
	If (!@EnDopl)
		Let @KomprDopl = false
		Let @DoplNadrz = false
	EndIf
	
	//**********************************************************
	//*************** Řízení teploty prostoru ******************
	//**********************************************************
	Hyst2 TProstKot, ix1.0, TProstZad[0,0], TProstZad[1,0], 0x0000
	// Odstavení
	If (!@EnRegTProst)
		Let ix1.0 = false
	EndIf
	//Let @FanOut1 = ix1.0
	//Let @FanOut2 = ix1.0]]></Script>
          </Body>
