      <CodeFBList>
        <CodeBlock Name="fb_TUV_zas_leg" Class="Amit.Psp4.Shared.CodeFB">
          <Comment>
          </Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[// Verze 2.00
// Doplněno přehřívání TUV kvůli legionele
// Žádaná při legionelle v parametru iTzz[Index, 1]
// Vstupně výstupní parametry do vnitřních proměnných
Let tuvStav = ioStav[Index, 0]

// Režim teploty zásobníku
Let tuvRezimBin = Int(Pow(2, iRezim[Index, 0]))
// V případě odstavení Vyp, jinak podle povelu
// Default režim Aut
Let @xVyp =   (@povVyp and !@povZap and !@povAut) or  @iStop
Let @xZap =   !@povVyp and  @povZap and !@povAut and !@iStop
Let @xAut = ((!@povVyp and !@povZap and  @povAut) or (!@xVyp and !@xZap)) and !@iStop

// Žádaná zásobníku
If @iReqLeg
	Let Tzz = if(@xVyp, 8.0, iTzz[Index, 1])
Else
	If @xAut
		DayPlan 0x0000, 8, 0x0001, 0x0002, 0x0004, 0x0008, 0x0010, 0x0020, 0x0040, 0x0080, iSvatky, iCP_casy, iCP_tepl, Tzz
	Else :NONE
		Let Tzz = if(@xVyp, 8.0, iTzz[Index, 0])
	EndIf
Endif

TimerOff @iReqLeg, iLegActDelay[Index, 0], @oLegAct, NONE 

// Věrohodnost měření teploty zásobníku
Let @Tz_F = (iTzm[Index, 0] < -30) or (iTzm[Index, 0] > 130)

// Havarijní meze teploty zásobníku
Let @Tz_LL = ((iTzm[Index, 0] < iTtv_meze[Index, 0]) or @Tz_LL) and !((iTzm[Index, 0] > (iTtv_meze[Index, 0] + Tz_hyst)) or @Tz_F)
Let @Tz_HH = ((iTzm[Index, 0] > iTtv_meze[Index, if(@oLegAct,3,2)]) or @Tz_HH) and !((iTzm[Index, 0] < (iTtv_meze[Index, if(@oLegAct,3,2)] - Tz_hyst)) or @Tz_F)

// Regulace teploty zásobníku
Let fx = 0.5 * Tz_hyst
Let @Tz_L = ((iTzm[Index, 0] < (Tzz - fx)) or @Tz_L) and !((iTzm[Index, 0] > (Tzz + fx)) or @Tz_F)
Let @oNeed = @Tz_L and !@xVyp

// Požadavek na spuštění čerpadla z mimoregulačních důvodů
Let @eCerpZap = (@iCerpZap or @Tz_LL) and !@iStop and !@Tz_HH

// Řízení čerpadla
Let @oCerpZap = (@Tz_L and !@xVyp) or (@eCerpZap and !@iStop)

// Zápis do vstupně výstupních parametrů
Let ioStav[Index, 0] = tuvStav
Let oTzz[Index, 0] = Tzz]]></Script>
          </Body>
