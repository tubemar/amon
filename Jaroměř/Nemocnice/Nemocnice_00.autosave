			//6180ppm
			Else :NONE
				Ni1000U2T fX, AR_UI0_okam[5,0], 6180, 15.000, 3920.000
			EndIf 
		EndIf
	//Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
	Else :NONE
		Let AR_UI0_okam[5,0] = AR_UI0_pom[5,0]
	EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AR_UI0_konst[5, 0]*AR_UI0_okam[5, 0]+AR_UI0_konst[5, 1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AR_UI0[5,0], AR_UI0_konst[5,2]

	//******************* ANALOGOVÝ VSTUP UI6 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AR_UI0_typ.6, :NONE
		//Pro převod U->T používám rozsah 0-5V (není třeba měnit default params)
		Let fX = AR_UI0_pom[6,0]/20.0
		//Pt1000
		If AR_TC0_typ.6
			//Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
			Interpol fX, AR_UI0_okam[6,0], Pt1000_Kriv
		//Ni1000
		Else :NONE
			//5000ppm
			If AR_Ni0_typ.6
				Ni1000U2T fX, AR_UI0_okam[6,0], 5000, 15.000, 3920.000
			//6180ppm
			Else :NONE
