                <Bit>2</Bit>
              </Alias>
              <Alias Name="@Tz_F" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Teplota zásobníku, porucha měření</Comment>
                <VarName>tuvStav</VarName>
                <Bit>4</Bit>
              </Alias>
              <Alias Name="@Tz_LL" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Teplota zásobníku, mez LL</Comment>
                <VarName>tuvStav</VarName>
                <Bit>5</Bit>
              </Alias>
              <Alias Name="@Tz_HH" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Teplota zásobníku, mez HH</Comment>
                <VarName>tuvStav</VarName>
                <Bit>6</Bit>
              </Alias>
              <Alias Name="@Tz_L" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Teplota zásobníku nízká</Comment>
                <VarName>tuvStav</VarName>
                <Bit>8</Bit>
              </Alias>
              <Alias Name="@eCerpZap" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Požadavek zapnutí čerpadla mimo regulaci</Comment>
                <VarName>tuvStav</VarName>
