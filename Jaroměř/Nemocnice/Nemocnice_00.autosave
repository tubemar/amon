        <CodeBlock Name="libObecne" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Obecne</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[
// Prázdniny
Holiday Svatky[*,19], Prazdniny

// Léto podle venkovní teploty

// Při výpadku venkovky použijeme jinou, příp. náhradní teplotu
Let TE_reg = if(@TVenkF, TE_const, TVenk)

//Let TE_regJ= if(@TVenkJF or !@R2_CommOK, TE_reg, TVenkJ)

// Tlumená venkovní teplota, vypočtemě z průměru teplot
Let fx = 60 * TE_leto_par[0, 0]
Filtr1R TE_reg, Te_tlum, fx

// Meze regulační a tlumené venkovní teploty
Hyst TE_reg, ix1.0, TE_leto_par[1,0], 1.000, 0x0000
Hyst TE_tlum, ix1.1, TE_leto_par[1,0], 1.000, 0x0000

// Pokud obě vysoké -> léto, pokud obě nízké -> zima
Let @te_xLeto = if(ix1.0 or ix1.1, @te_xLeto, false)
Let @te_xLeto = if(ix1.0 and ix1.1, true, @te_xLeto)
]]></Script>
          </Body>
