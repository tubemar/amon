	Valve tvx_akce[3,0], tvx_prebeh[3,0], tvx_poloha[3,0], @ValUT1_Z, @ValUT1_O
	Valve tvx_akce[4,0], tvx_prebeh[4,0], tvx_poloha[4,0], @ValUT2_Z, @ValUT2_O
	Valve tvx_akce[5,0], tvx_prebeh[5,0], tvx_poloha[5,0], @ValUT3_Z, @ValUT3_O
	Valve tvx_akce[6,0], tvx_prebeh[6,0], tvx_poloha[6,0], @ValUT4_Z, @ValUT4_O
