	    AnIn #Ni10001_2, AI0_Pom[2,0], 5.000, 0.000, 5.000, 0.000, 5.000
	    //Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
	    Interpol AI0_Pom[2,0], AI0_okamzita[2,0], Pt1000_Kriv
	  //Ni1000
	  Else :NONE
           //5000ppm
           If Ni0_typ.2, :NONE
             Ni1000 #Ni10001_2, AI0_okamzita[2,0], 5000
             //6180ppm
           Else :NONE
             Ni1000 #Ni10001_2, AI0_okamzita[2,0], 6180
           EndIf 
	  EndIf
         //Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
       Else :NONE
         AnIn #AI00_2, AI0_okamzita[2,0], AI0_params[2,0], AI0_params[2,1], AI0_params[2,2], AI0_params[2,3], AI0_params[2,4]
       EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AI0_konst[2,0]*AI0_okamzita[2,0]+AI0_konst[2,1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AI0[2,0], AI0_konst[2,2]
       
	//******************* ANALOGOVÝ VSTUP AI0.3 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AI0_typ.3, :NONE
	  //Pt1000
	  If TC0_typ.3, :NONE
	    //Měření napětí na čidle v rozsahu 0 .. 5 V
	    AnIn #Ni10001_3, AI0_Pom[3,0], 5.000, 0.000, 5.000, 0.000, 5.000
	    //Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
	    Interpol AI0_Pom[3,0], AI0_okamzita[3,0], Pt1000_Kriv
	  //Ni1000
	  Else :NONE
           //5000ppm
           If Ni0_typ.3, :NONE
             Ni1000 #Ni10001_3, AI0_okamzita[3,0], 5000
             //6180ppm
           Else :NONE
             Ni1000 #Ni10001_3, AI0_okamzita[3,0], 6180
           EndIf 
	  EndIf
         //Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
       Else :NONE
         AnIn #AI00_3, AI0_okamzita[3,0], AI0_params[3,0], AI0_params[3,1], AI0_params[3,2], AI0_params[3,3], AI0_params[3,4]
       EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AI0_konst[3,0]*AI0_okamzita[3,0]+AI0_konst[3,1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AI0[3,0], AI0_konst[3,2]
       
	//******************* ANALOGOVÝ VSTUP AI0.4 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AI0_typ.4, :NONE
	  //Pt1000
	  If TC0_typ.4, :NONE
	    //Měření napětí na čidle v rozsahu 0 .. 5 V
	    AnIn #Ni10001_4, AI0_Pom[4,0], 5.000, 0.000, 5.000, 0.000, 5.000
	    //Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
	    Interpol AI0_Pom[4,0], AI0_okamzita[4,0], Pt1000_Kriv
	  //Ni1000
	  Else :NONE
           //5000ppm
           If Ni0_typ.4, :NONE
             Ni1000 #Ni10001_4, AI0_okamzita[4,0], 5000
             //6180ppm
           Else :NONE
             Ni1000 #Ni10001_4, AI0_okamzita[4,0], 6180
           EndIf 
	  EndIf
         //Na vstup je připojeno PROUDOVÉ NEBO NAPĚŤOVÉ ČIDLO
       Else :NONE
         AnIn #AI00_4, AI0_okamzita[4,0], AI0_params[4,0], AI0_params[4,1], AI0_params[4,2], AI0_params[4,3], AI0_params[4,4]
       EndIf 
	//Naměřenou hodnotu lze (v případě nutnosti) upravit pomocí korekčních kostant
       Let fX = AI0_konst[4,0]*AI0_okamzita[4,0]+AI0_konst[4,1]
	//Výslednou hodnotu filtrujeme
       Filtr1R fX, AI0[4,0], AI0_konst[4,2]
	
	//******************* ANALOGOVÝ VSTUP AI0.5 *******************
	//Na vstup je připojeno TEPLOTNÍ ČIDLO
       If AI0_typ.5, :NONE
	  //Pt1000
	  If TC0_typ.5, :NONE
	    //Měření napětí na čidle v rozsahu 0 .. 5 V
	    AnIn #Ni10001_5, AI0_Pom[5,0], 5.000, 0.000, 5.000, 0.000, 5.000
	    //Naměřenou hodnotu interpolujeme dle převodní tabulky čidla
	    Interpol AI0_Pom[5,0], AI0_okamzita[5,0], Pt1000_Kriv
	  //Ni1000
	  Else :NONE
           //5000ppm
           If Ni0_typ.5, :NONE
             Ni1000 #Ni10001_5, AI0_okamzita[5,0], 5000
             //6180ppm
           Else :NONE
             Ni1000 #Ni10001_5, AI0_okamzita[5,0], 6180
           EndIf 
	  EndIf
