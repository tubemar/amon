        <CodeBlock Name="libZdroje" Class="Amit.Psp4.Shared.CodeProcedure">
          <Comment>Řízení a řazení zdrojů tepla</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[
	//Společná žádaná pro výstup z kotlů
	Let WTUV_Max = min( max(tvx_Tz[0,0],tvx_Tz[1,0]) + OffsetTUV,95)
	Let WTV_Max = min( max(tvx_Tz[3,0],tvx_Tz[4,0],tvx_Tz[5,0],tvx_Tz[6,0],tvx_Tz[7,0],tvx_Tz[8,0],tvx_Tz[9,0],tvx_Tz[10,0]) + OffsetTV,95)
	Let WTVSpol = max(WTV_Max,WTUV_Max)
	
	//******************************************************************************
	//Kaskáda kotlů
	//******************************************************************************
	SyncMark SyncKonst1[0,0], SyncKonst1[1,0], SyncKonst1[2,0], 0, 0, @Poradi, debug1
	Let Ka_PorKotlu[0,0] = !@DEnKot1 or Alarm_E1.13
	Let Ka_PorKotlu[1,0] = !@DEnKot2 or Alarm_E1.14
	Let Ka_PorKotlu[2,0] = !@DEnKot3 or Alarm_E1.15
	Let Ka_PorKotlu[3,0] = !@DEnKot4 or Alarm_E1.16

	CasCon WTVSpol, TKotVyst, @DisKask, @Poradi, Ka_Period, WKot_Meze[0,0], WKot_Meze[1,0], Ka_TypKotlu, Ka_PorKotlu, Ka_Rizeni, Ka_VykonKot, Ka_ProvozKot, Ka_Hladina, Ka_Zesileni, Ka_Integrace, Ka_SDK, Ka_PripojInt, Ka_OdpojInt, Ka_IngralAct
	
	Let @K1_st1 = Ka_Rizeni[0,0].0
	Let @K1_st2 = Ka_Rizeni[0,0].1
	Let @K2_st1 = Ka_Rizeni[1,0].0
	Let @K2_st2 = Ka_Rizeni[1,0].1
	Let @K3_st1 = Ka_Rizeni[2,0].0
	Let @K3_st2 = Ka_Rizeni[2,0].1
	Let @K4_st1 = Ka_Rizeni[3,0].0
	Let @K4_st2 = Ka_Rizeni[3,0].1

	//Přepočet minut na hodiny pro ViewDet
	Let Ka_ProHodiny[0,0] = long(Ka_ProvozKot[0,0]/60)
	Let Ka_ProHodiny[1,0] = long(Ka_ProvozKot[1,0]/60)
	Let Ka_ProHodiny[2,0] = long(Ka_ProvozKot[2,0]/60)
	Let Ka_ProHodiny[3,0] = long(Ka_ProvozKot[3,0]/60)
	
	//******************************************************************************
	// Čerpadla
	//******************************************************************************
	
	//Ovládání čerpadla K1 s doběhem
	Let ix1.3 = (@K1_st1 or @K1_st2)
	TimerOff ix1.3, tCerpDobeh, ix1.4, NONE
	Let @CKot1 = (ix1.2 or ix1.4)
	//Protáčení čerpadla K1
	Let ix1.0 = not @CKot1
	TimerOn ix1.0, tProtoc[0,0], ix1.1, NONE
	TimerPuls ix1.1, tProtoc[1,0], ix1.2, NONE

	//Ovládání čerpadla K2 s doběhem
	Let ix1.3 = (@K2_st1 or @K2_st2)
	TimerOff ix1.3, tCerpDobeh, ix1.4, NONE
	Let @CKot2 = (ix1.2 or ix1.4)	
	//Protáčení čerpadla K2
	Let ix1.0 = not @CKot2
	TimerOn ix1.0, tProtoc[0,0], ix1.1, NONE
	TimerPuls ix1.1, tProtoc[1,0], ix1.2, NONE
	
	
	//Ovládání čerpadla K3 s doběhem
	Let ix1.3 = (@K3_st1 or @K3_st2)
	TimerOff ix1.3, tCerpDobeh, ix1.4, NONE
	Let @CKot3 = (ix1.2 or ix1.4)
	//Protáčení čerpadla K3
	Let ix1.0 = not @CKot3
	TimerOn ix1.0, tProtoc[0,0], ix1.1, NONE
	TimerPuls ix1.1, tProtoc[1,0], ix1.2, NONE
	
	//Ovládání čerpadla K4 s doběhem
	Let ix1.3 = (@K4_st1 or @K4_st2)
	TimerOff ix1.3, tCerpDobeh, ix1.4, NONE
	Let @CKot4 = (ix1.2 or ix1.4)	
	//Protáčení čerpadla K4
	Let ix1.0 = not @CKot4
	TimerOn ix1.0, tProtoc[0,0], ix1.1, NONE
	TimerPuls ix1.1, tProtoc[1,0], ix1.2, NONE
	
	//******************************************************************************
	// Regulace zpáteček
	//******************************************************************************
	
	Let RoZ_PIDrezim[0,0].0 = !@CKot1
	Let RoZ_PIDrezim[1,0].0 = !@CKot2
	Let RoZ_PIDrezim[2,0].0 = !@CKot3
	Let RoZ_PIDrezim[3,0].0 = !@CKot4
	
	Let ix1 = RoZ_PIDrezim[0,0]
	PID TKotZp1, T_rosa, fx, ix1, RoZ_PIDpar
	Let RoZ_PIDrezim[0,0] = ix1
	Let RoZ_PIDzas[0,0] = fx

	Let ix1 = RoZ_PIDrezim[1,0]
	PID TKotZp2, T_rosa, fx, ix1, RoZ_PIDpar
	Let RoZ_PIDrezim[1,0] = ix1
	Let RoZ_PIDzas[1,0] = fx
	
	Let ix1 = RoZ_PIDrezim[2,0]
	PID TKotZp3, T_rosa, fx, ix1, RoZ_PIDpar
	Let RoZ_PIDrezim[2,0] = ix1
	Let RoZ_PIDzas[2,0] = fx
	
	Let ix1 = RoZ_PIDrezim[3,0]
	PID TKotZp4, T_rosa, fx, ix1, RoZ_PIDpar
	Let RoZ_PIDrezim[3,0] = ix1
	Let RoZ_PIDzas[3,0] = fx

	Valve RoZ_PIDzas[0,0], RoZ_prebeh[0,0], fx, DO0_A.0, DO0_A.1
	Valve RoZ_PIDzas[1,0], RoZ_prebeh[1,0], fx, DO0_A.2, DO0_A.3
	Valve RoZ_PIDzas[2,0], RoZ_prebeh[2,0], fx, DO0_A.4, DO0_A.5
	Valve RoZ_PIDzas[3,0], RoZ_prebeh[3,0], fx, DO0_A.6, DO0_A.7

	
	// Pro zobrazení na display
	Let Burner.0 = @K1_st1 or @Kot1_Run
	Let Burner.1 = @K1_st2 and @Kot1_Run
	Let Burner.2 = @K2_st1 or @Kot2_Run
	Let Burner.3 = @K2_st2 and @Kot2_Run
	Let Burner.4 = @K3_st1 or @Kot3_Run
	Let Burner.5 = @K3_st2 and @Kot3_Run
	Let Burner.6 = @K4_st1 or @Kot4_Run
	Let Burner.7 = @K4_st2 and @Kot4_Run
]]></Script>
          </Body>
        </CodeBlock>
        <CodeBlock Name="ProcAlarm" Class="Amit.Psp4.Shared.CodeProcess">
          <Comment>Poruchy</Comment>
          <Body Class="Amit.Psp4.Shared.CodeBodyPse">
            <Script><![CDATA[	//Příznak restartu
	TimerOn @Restart, tRestart[0,0], ix1.0, NONE
	Let @Restart = If(ix1.0,false,@Restart)

	//Test analogových snímačů VZT1
	Limits AI0_okamzita[0,0], @TVenkF, @TVenkF, -40.0, 130.0, 5.0, 0x0000

	// *****************************************************************************
	//                    Zdroje PORUCH
	// *****************************************************************************
	
	ARI_State 1, AR_State[0,0], 5, NONE					//Komunikace ARION
	ARI_State 2, AR_State[1,0], 5, NONE
	ARI_State 3, AR_State[2,0], 5, NONE
	ARI_State 4, AR_State[3,0], 5, NONE
	ARI_State 5, AR_State[4,0], 5, NONE
	ARI_State 6, AR_State[5,0], 5, NONE
	ARI_State 7, AR_State[6,0], 5, NONE
	ARI_State 9, AR_State[7,0], 5, NONE
	
	If (!@Restart)
		Let Alarm_A1.0 = !@STOP_btnOK					//STOP tlačítko
		Let Alarm_A1.1 =  @STOP_btnOK and !@Gas1st_OK and @Gas2st_OK	//Únik plynu 1.st
		Let Alarm_A1.2 =  @STOP_btnOK and !@Gas2st_OK			//Únik plynu 2.st
		Let Alarm_A1.3 =  @Gas2st_OK and !@OverTeVS_OK			//Přehřátí prostoru
		Let Alarm_A1.4 =  @OverTeVS_OK and !@LeakVS_OK			//Zaplavení prostoru

		Let Alarm_A1.5 = !@SV_Press_OK					//Nízký tlak SV
		
		Let ix1.0 = TKotVyst>105.0
		TimerOn ix1.0, 30000, Alarm_A1.6, NONE				//Přehřátí kotlů
	
		Let ix1.0 = tvx_stav[0,0].6
		TimerOn ix1.0, 30000, ix1.1, NONE
		Let Alarm_A1.7 = !@TempTUV1_OK or ix1.1				//Přehřátí TUV1

		Let ix1.2 = tvx_stav[1,0].6 or tvx_stav[2,0].6
		TimerOn ix1.2, 30000, ix1.3, NONE
		Let Alarm_A1.8 = !@TempTUV2_OK or ix1.3				//Přehřátí TUV2

											//Nízký a vysoký tlak TV
		Limits PTV1, Alarm_A1.10, Alarm_A1.9, PTV_Meze[0,0], PTV_Meze[1,0], 2.000, 0x0000
		Let Alarm_A1.9 = Alarm_A1.9 or (!@TV_Press_OK and @LeakVS_OK)
	
		TimerOn @KomprDopl, tMaxDopl, Alarm_A1.11, NONE			//dlouhé doplňování vzduch
		TimerOn @DoplNadrz, tMaxDopl, Alarm_A1.12, NONE			//dlouhé doplňování voda

		Let Alarm_A1.13 = @Kot1_Err	or @Kot1_Over				//Porucha kotle
		Let Alarm_A1.13 = @K1_st1 and !@Kot1_Run or Alarm_A1.13
		Let Alarm_A1.14 = @Kot2_Err	or @Kot2_Over				//Porucha kotle
		Let Alarm_A1.14 = @K2_st1 and !@Kot2_Run or Alarm_A1.14
		Let Alarm_A1.15 = @Kot3_Err	or @Kot3_Over				//Porucha kotle
		Let Alarm_A1.15 = @K3_st1 and !@Kot3_Run or Alarm_A1.15
		Let Alarm_A1.16 = @Kot4_Err	or @Kot4_Over				//Porucha kotle
		Let Alarm_A1.16 = @K4_st1 and !@Kot4_Run or Alarm_A1.16
		
		Let Alarm_A1.17 = @HUP and !@HUP_Open				//Porucha HUP
		
		Let Alarm_A1.18 = (AR_State[0,0]!=1) or (AR_State[1,0]!=1)
		Let Alarm_A1.19 = (AR_State[2,0]!=1) or (AR_State[3,0]!=1)
		Let Alarm_A1.20 = (AR_State[4,0]!=1) or (AR_State[5,0]!=1)
		Let Alarm_A1.21 = (AR_State[6,0]!=1) or (AR_State[7,0]!=1)
		Let Alarm_A1.22 = false
		Let Alarm_A1.23 = false
		
		Let Alarm_A2.0 = @CKot1 and !@CKot1_Run
		Let Alarm_A2.1 = @CKot2 and !@CKot2_Run
		Let Alarm_A2.2 = @CKot3 and !@CKot3_Run
		Let Alarm_A2.3 = @CKot4 and !@CKot4_Run
		
		Let Alarm_A2.4 = @CNab_TUV1 and !@CTUV1_Run
		Let Alarm_A2.5 = @CNab_TUV2 and !@CTUV2_Run
		Let Alarm_A2.6 = @CNab_TUV3 and !@CTUV3_Run
		
		Let Alarm_A2.7 = @CCir_TUV1 and !@CTUV1c_Run
		Let Alarm_A2.8 = @CCir_TUV2 and !@CTUV2c_Run
		
		Let Alarm_A2.9 = @CTVpod and !@CTVpod_Run
		
		Let Alarm_A2.10 = @CTV1 and !@CTV1_Run
		Let Alarm_A2.11 = @CTV2 and !@CTV2_Run
		Let Alarm_A2.12 = @CTV3 and !@CTV3_Run
		Let Alarm_A2.13 = @CTV4 and !@CTV4_Run
		Let Alarm_A2.14 = @CTV5 and !@CTV5_Run
		Let Alarm_A2.15 = @CTV6 and !@CTV6_Run
		Let Alarm_A2.16 = @CTV7 and !@CTV7_Run
		Let Alarm_A2.17 = @CTV8 and !@CTV8_Run
		Let Alarm_A2.18 = @CTV9 and !@CTV9_Run
		Let Alarm_A2.19 = @CTV10 and !@CTV10_Run
		Let Alarm_A2.20 = @CTV11 and !@CTV11_Run
		Let Alarm_A2.21 = @KomprDopl and !@Kompr_Run
		Let Alarm_A2.22 = @FanOut1 and !@FanOut1_Run
		Let Alarm_A2.23 = @FanOut2 and !@FanOut2_Run
		
	EndIf

	// *****************************************************************************
	//                    PORUCHY Zpracování
	// *****************************************************************************

	//Kvitace alarmů
	Let Alarm_Q1 = If(pQuit.0, ~(Alarm_I1 | Alarm_A1), Alarm_Q1)
	Let Alarm_Q2 = If(pQuit.0, ~(Alarm_I2 | Alarm_A2), Alarm_Q2)
	Let pQuit.0 = false
	
	ErrSig32 Alarm_A1, 0x00000001, Alarm_Q1, 0x00000001, Alarm_E1.0, Alarm_I1.0, 1, 0, 4096, 2000, 0, 0
