              <Alias Name="@xAut" Class="Amit.Psp4.Shared.Psp4Alias">
                <Comment>Stav Aut</Comment>
                <VarName>tuvStav</VarName>
                <Bit>0</Bit>
              </Alias>
              <Alias Name="@xVyp" Class="Amit.Psp4.Shared.Psp4Alias">
